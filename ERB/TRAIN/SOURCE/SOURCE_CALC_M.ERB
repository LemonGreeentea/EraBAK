;MASTER 전용의 소스 계산

;-------------------------------------------------
;쾌Ｃ의 소스
;-------------------------------------------------
@CALC_SOURCE_MASTER0(ARG:0)
LOCAL:0 = SOURCE:(ARG:0):쾌Ｃ
LOCAL:5 = SOURCE:(ARG:0):쾌Ｃ
LOCAL:6 = SOURCE:(ARG:0):쾌Ｃ

SELECTCASE ABL:(ARG:0):Ｃ감
	CASE 0
		TIMES LOCAL:0, 0.04
		TIMES LOCAL:5, 0.70
	CASE 1
		TIMES LOCAL:0, 0.20
		TIMES LOCAL:5, 0.85
	CASE 2
		TIMES LOCAL:0, 1.00
		TIMES LOCAL:5, 1.00
	CASE 3
		TIMES LOCAL:0, 2.30
		TIMES LOCAL:5, 1.15
	CASE 4
		TIMES LOCAL:0, 4.00
		TIMES LOCAL:5, 1.30
	CASEELSE
		LOCAL:0 = LOCAL:0 * ((ABL:(ARG:0):Ｃ감 - 5) * 50 + 550) / 100
		LOCAL:5 = LOCAL:5 * ((ABL:(ARG:0):Ｃ감 - 5) * 10 + 140) / 100
ENDSELECT

IF TALENT:(ARG:0):Ｃ민감
	TIMES LOCAL:0, 2.00
	TIMES LOCAL:5, 1.50
ENDIF

IF TALENT:(ARG:0):Ｃ둔감
	TIMES LOCAL:0, 0.50
	TIMES LOCAL:5, 0.75
ENDIF

IF GETBIT(TALENT:(ARG:0):음란계, 소질_음란_음핵)
	TIMES LOCAL:0, 1.50
	TIMES LOCAL:5, 1.50
ENDIF

;미약사용중
IF TCVAR:(ARG:0):60
	TIMES LOCAL:0, 2.50
	TIMES LOCAL:5, 2.00
ENDIF

;도원향 사용중
IF TFLAG:69
	TIMES LOCAL:0, 1.25
	TIMES LOCAL:5, 1.25
ENDIF

;체력 0에 의한 행동불능
IF TCVAR:(ARG:0):51
	TIMES LOCAL:0, 0.20
	TIMES LOCAL:5, 0.20
ENDIF

;기력 0에 의한 실신중
IF TCVAR:(ARG:0):52
	TIMES LOCAL:0, 0.20
	TIMES LOCAL:5, 0.20
ENDIF

IF HAS_PENIS(ARG:0)
	CUP:(ARG:0):사정 += LOCAL:5 / 5
ENDIF

IF HAS_VAGINA(ARG:0)
	CUP:(ARG:0):윤활 += LOCAL:5 / 3
ENDIF

CUP:(ARG:0):쾌Ｃ += LOCAL:0
CUP:(ARG:0):분수 += SQRT(LOCAL:0 * 400)

