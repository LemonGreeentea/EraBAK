	;번호 LOCAL:0의 캐릭터가 분유 했을 경우
	IF IS_PARTICIPATE_TRAIN(LOCAL:0) && NOWEX:(LOCAL:0):분유 > 0
		;촉수착유중
		IF (SELECTCOM == 207 && IS_MTAR(LOCAL:0)) || IS_EQUIP_TARGET(LOCAL:0, 207)
