;메인 처리
;-------------------------------------------------
@COM130
;실행 판정
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

;전플레이어에 대해 처리
FOR LOCAL:0, 0, MPLY_NUM
	LOCAL:2 = MPLY:(LOCAL:0)

	EXP:(LOCAL:2):배설경험치 += 1

	LOCAL:3 = 800
	LOCAL:4 = 300

	SELECTCASE ABL:(LOCAL:2):배설
		CASE 0
			TIMES LOCAL:3, 0.00
			TIMES LOCAL:4, 1.00
		CASE 1
			TIMES LOCAL:3, 0.10
			TIMES LOCAL:4, 0.60
		CASE 2
			TIMES LOCAL:3, 0.20
			TIMES LOCAL:4, 0.25
		CASE 3
			TIMES LOCAL:3, 0.40
			TIMES LOCAL:4, 0.05
		CASE 4
			TIMES LOCAL:3, 0.70
			TIMES LOCAL:4, 0.00
		CASEELSE
			LOCAL:3 = LOCAL:3 * (100 + (ABL:(LOCAL:2):배설 - 5) * 2) / 100
			LOCAL:4 = 0
	ENDSELECT

	SOURCE:(LOCAL:2):노출 = 1200
	SOURCE:(LOCAL:2):일탈 = 500
	SOURCE:(LOCAL:2):중독충족 = LOCAL:3
	SOURCE:(LOCAL:2):성행동 = 150
	SOURCE:(LOCAL:2):반감 = LOCAL:4

	;주도권에 응한 우월·굴종의 소스 추가
	CALL ADD_SOURCE_INITIATIVE_U(LOCAL:2, 80, 150)

	;뇨잔량을 0으로 한다
	TCVAR:(LOCAL:2):54 = 0
NEXT

;주도권의 소재를 취득
LOCAL:11 = GET_COM_INITIATIVE()

;전 타겟으로 붙어 처리
FOR LOCAL:0, 0, MTAR_NUM
	LOCAL:1 = MTAR:(LOCAL:0)

	SOURCE:(LOCAL:1):성행동 = 90

	;타겟으로 주도권
	IF LOCAL:11 == 1 || LOCAL:11 == 2
		SOURCE:(LOCAL:1):기학 = 150
		SOURCE:(LOCAL:1):일탈 = 250
	;타겟으로 주도권이 없다
	ELSE
		SOURCE:(LOCAL:1):일탈 = 500
		SOURCE:(LOCAL:1):공포 = MAX(150 - 40 * ABL:(LOCAL:1):욕망, 0)
	ENDIF

	;주도권에 응한 우월·굴종의 소스 추가
	CALL ADD_SOURCE_INITIATIVE_U(LOCAL:1, 100, 80)
NEXT

;주도도 변화 기준치
TFLAG:49 = 4

;도착도 변화 기준치
TFLAG:50 = 4

;레즈·ＢＬ경험기준치
TFLAG:51 = 2

RETURN 1

