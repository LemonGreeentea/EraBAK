@COM_ABLE291
;공통 부분
CALL COM_ABLE_COMMON(291)
SIF RESULT == 0
	RETURN 0
;만나러간다·침실호출 ·포로 회화 한정
SIF !GROUPMATCH(FLAG:조교모드, 0, 1, 6)
	RETURN 0
;행동 가능한 캐릭터의 수를 조사한다
LOCAL:10 = 0
FOR LOCAL:0, 0, CHARANUM
	IF IS_AUTOSELECT(LOCAL:0)
		LOCAL:10 ++
	ENDIF
NEXT
;행동 가능한 캐릭터가 없으면 불가
IF LOCAL:10 <= 0
	RETURN 0
ENDIF
RETURN 1

