	CALL COM_ORDER_ELEMENT(ARG:0, "도착도", 16)
ELSEIF ABL:(ARG:0):도착도 >= 100
	CALL COM_ORDER_ELEMENT(ARG:0, "도착도", 8)
ELSE
	CALL COM_ORDER_ELEMENT(ARG:0, "도착도", 0)
ENDIF

