	CALL COM_ORDER_ELEMENT(ARG:0, "쾌감을부정", -2)
ENDIF

IF GETBIT(TALENT:(ARG:0):음란계, 소질_음란_새드) || GETBIT(TALENT:(ARG:0):음란계, 소질_음란_마조) || ABL:(ARG:0):도착도 >= 800
	CALL COM_ORDER_ELEMENT(ARG:0, "도착도", 32)
