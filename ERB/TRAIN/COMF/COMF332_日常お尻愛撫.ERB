;射精箇所と対象をセット
CALL STACK_SPERM(MEQUIP_PLAYER:(ARG:0):0, MEQUIP_PLAYER:(ARG:0):0, 0)

;主導権に応じた優越・屈従のソース追加
CALL ADD_SOURCE_INITIATIVE_U(MEQUIP_PLAYER:(ARG:0):0, 100, 40)

;倒錯度変化基準値
TCVAR:(MEQUIP_PLAYER:(ARG:0):0):50 += 1

;奉仕経験値を得られるコマンドのフラグ
TCVAR:(MEQUIP_PLAYER:(ARG:0):0):4 = 1

