@COM_ABLE98
;공통 부분
CALL COM_ABLE_COMMON(98)
SIF RESULT == 0
	RETURN 0
;플레이어는 1명
SIF MPLY_NUM != 1
	RETURN 0
;타겟도 1명
SIF MTAR_NUM != 1
	RETURN 0
;플레이어가 행동불능이라면 불가
SIF !IS_PLAYABLE(MPLY:0)
	RETURN 0

SIF ITEM:스키마 == 0
	RETURN 0

LOCAL:5 = 0
LOCAL:6 = 0
;모든 플레이어에 대해 판정
FOR LOCAL:0, 0, MPLY_NUM
	;안면승마 되고 있다면 불가
	SIF IS_RIDDEN(MPLY:(LOCAL:0))
		RETURN 0
	;안면승마 하고 있다면 불가
	SIF IS_RIDDEN(MPLY:(LOCAL:0))
		RETURN 0
	;키스중, 패합중이라면 불가
	SIF IS_EQUIP(MPLY:(LOCAL:0), 20, 21)
		RETURN 0
	;대딸, 다리 코키, 스마타, 파이즈리, 오나홀대딸중이라면 불가
	SIF IS_EQUIP_PLAYER(MPLY:(LOCAL:0), 10, 12, 14, 13, 17)
		RETURN 0
	;삽입중이라면 불가
	SIF IS_INSERT_SINGLE(MPLY:(LOCAL:0), "전")
		RETURN 0
	;구속되고 있다면 불가
	SIF IS_BIND(MPLY:(LOCAL:0))
		RETURN 0
NEXT
;모든 타겟으로 붙어 판정
FOR LOCAL:0, 0, MTAR_NUM
	;녀만
	SIF !IS_FEMALE(MTAR:(LOCAL:0))
		RETURN 0
	;안면승마 되고 있다면 불가
	SIF IS_RIDDEN(MTAR:(LOCAL:0))
		RETURN 0
NEXT
RETURN 1

