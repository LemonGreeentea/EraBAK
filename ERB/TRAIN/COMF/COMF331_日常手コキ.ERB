ENDIF
IF TALENT:(ARG:0):수줍음
	CALL COM_ORDER_ELEMENT(ARG:0, "수줍음", -1)
ENDIF
IF TALENT:(ARG:0):헌신적
	CALL COM_ORDER_ELEMENT(ARG:0, "헌신적", 6)
ENDIF
IF TALENT:(ARG:0):남성혐오 && IS_MALE(MASTER)
	CALL COM_ORDER_ELEMENT(ARG:0, "남성혐오", -12)
ENDIF
