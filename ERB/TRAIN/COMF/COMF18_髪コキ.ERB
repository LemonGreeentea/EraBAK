@COM_NAME18
IF MPLY_NUM == 2
	LOCALS:0 = Ｗ머리카락코키
ELSE
	LOCALS:0 = 머리카락코키
ENDIF

RESULTS:0 = %LOCALS:0%한다
RESULTS:1 = %LOCALS:0%당한다
RESULTS:2 = %LOCALS:0%시킨다
RESULTS:3 = %LOCALS:0%된다
RESULTS:4 = %LOCALS:0%시킨다
RESULTS:5 = %LOCALS:0%과시하고

;-------------------------------------------------
;선택 가부 판정
;-------------------------------------------------
@COM_ABLE18
;공통 부분
CALL COM_ABLE_COMMON(18)
SIF RESULT == 0
	RETURN 0
;플레이어는 최대로 2명까지
SIF MPLY_NUM <= 0 || MPLY_NUM > 2
	RETURN 0
;타겟은 최대로 1명까지
SIF MTAR_NUM <= 0 || MTAR_NUM > 1
	RETURN 0

;전플레이어에 대해 판정
FOR LOCAL:0, 0, MPLY_NUM
	;행동불능이라면 불가
	SIF !IS_PLAYABLE(MPLY:(LOCAL:0))
		RETURN 0
	;구속중이라면 불가
	SIF IS_BIND(MPLY:(LOCAL:0))
		RETURN 0
	;삽입중은 플레이어가 한사람의 경우만 허가
	SIF IS_INSERT_SINGLE(MPLY:(LOCAL:0), "전") && MPLY_NUM >= 2
		RETURN 0
	;미들 이상이 필요
	SIF TALENT:(MPLY:(LOCAL:0)):머리길이 < 4
		RETURN 0
	;페라, 파이즈리, 스마타, 풋잡, 엉덩이코키, 꼬리 나호, 꼬리중은 무리
	SIF IS_EQUIP_PLAYER(MPLY:(LOCAL:0), 11, 12, 13, 14, 15, 16, 17)
		RETURN 0
NEXT
;타겟으로 장대가 필요
SIF !HAS_PENIS(MTAR:0)
	RETURN 0
;타겟의 장대가 삽입중이라면 불가
SIF IS_INSERT_SINGLE(MTAR:0, "Ｐ")
	RETURN 0
;타겟의 자지가 메워지고 있으면 안 돼
SIF IS_P_HOLD(MTAR:0)
	RETURN 0
RETURN 1

;-------------------------------------------------
;메인 처리
;-------------------------------------------------
@COM18
;실행 판정
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

;대Ｐ봉사계 커맨드의 계속 플래그를 해제
CALL RELEASE_SERVE_P

;●인원수 보정의 설정
LOCAL:10 = 100

SELECTCASE MPLY_NUM
	CASE 2
		TIMES LOCAL:10, 0.75
ENDSELECT

;●전플레이어에 대해 판정
FOR LOCAL:0, 0, MPLY_NUM
	LOCAL:2 = MPLY:(LOCAL:0)

	EXP:(LOCAL:2):성기술경험치 += 1

	SOURCE:(LOCAL:2):봉사 = SERVE_HOUSHI(LOCAL:2, 250)
	SOURCE:(LOCAL:2):접촉 = 40
	SOURCE:(LOCAL:2):성행동 = 210

	;주도권에 응한 우월·굴종의 소스 추가
	CALL ADD_SOURCE_INITIATIVE_U(LOCAL:2, 180, 80)

	;봉사 경험치를 얻을 수 있는 커맨드의 플래그
	TCVAR:(LOCAL:2):4 = 1

	;타겟으로 주는 쾌감계 소스를 계산
	SOURCE:(MTAR:0):쾌Ｐ += SENSE_HOUSHI_P(LOCAL:2, MTAR:0, 500) * LOCAL:10 / 100

	;사정 개소와 대상을 세트
	CALL STACK_SPERM(MTAR:0, LOCAL:2, RAND:3 ? 사정부위_머리카락 # 사정부위_얼굴)
NEXT

;●타겟으로 붙어 판정
SOURCE:(MTAR:0):접촉 = 20
SOURCE:(MTAR:0):성행동 = 180

;주도권에 응한 우월·굴종의 소스 추가
CALL ADD_SOURCE_INITIATIVE_U(MTAR:0, 120, 60)

;주도도 변화 기준치
TFLAG:49 = 2

;도착도 변화 기준치
TFLAG:50 = 0

;레즈·ＢＬ경험기준치
TFLAG:51 = 4

RETURN 1

;-------------------------------------------------
;계속 커맨드인가 어떤가를 설정
