;-------------------------------------------------
;# _COM_AUTOSELECT_3_커맨드의 우선도 설정
;-------------------------------------------------
@COM_AUTOSELECT_SET_COM_PRIORITY(MAIN_PLAYER, MAIN_TARGET)
#DIM MAIN_PLAYER
#DIM MAIN_TARGET
#DIM L_COUNTER							;FOR문의 카운터
#DIM COM_AUTO_CATEGORY_PRIORITY_SUM		;커맨드 실행 우선도의 계산에 사용하는 일시 변수의 보조
#DIM SEXUAL_PREFERENCE_ADD, 4			;성벽 설정을 반영시키기 위한 보정
#DIM SEXUAL_PREFERENCE_MULTI, 4

;-------------------------------------------------
; 리셋트
;-------------------------------------------------
VARSET COM_AUTO_COM_PRIORITY, 0

;-------------------------------------------------
; 각 커맨드의 우선도 리스트 read
;-------------------------------------------------
FOR L_COUNTER, 1, COM_AUTO_CATEGORY_NUM_MAX + 1
	CALLFORM COM_AUTOSELECT_SET_COM_PRIORITY_{L_COUNTER}(MAIN_PLAYER, MAIN_TARGET)
NEXT

;-------------------------------------------------
; 디버그 커맨드 우선도 금지전
;-------------------------------------------------
;체크가 너무 많으면 읽기 어렵기 때문에 코멘트 아웃 하고 있다
;뭔가 이상했던 때 코멘트 아웃을 제외해 확인하는 용
;[IF_DEBUG]
;	DEBUGPRINTFORML 
;	DEBUGPRINTFORML  DEBUG 커맨드 우선도 금지전
;	DEBUGPRINTFORM      전회 실행 
;	IF TFLAG:55 != 0
;		IF PREVCOM_TYPE == 0
;			DEBUGPRINTFORML {PREVCOM}. %@"%TRAINNAME:PREVCOM%시킨다", 26, LEFT%
;		ELSEIF PREVCOM_TYPE == 1
;			DEBUGPRINTFORML {PREVCOM}. %@"%TRAINNAME:PREVCOM%한다", 26, LEFT%
;		ELSE
;			DEBUGPRINTFORML {PREVCOM}. %@"제삼자가 %TRAINNAME:PREVCOM%", 26, LEFT%
;		ENDIF
;	ELSE
;		DEBUGPRINTFORML 없음
;	ENDIF
;	DEBUGPRINTFORM      윤활은
;	IF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET)
;		DEBUGPRINTFORML 충분히  PALAM {PALAM:MAIN_TARGET:윤활}
;	ELSE
;		DEBUGPRINTFORML 부족  PALAM {PALAM:MAIN_TARGET:윤활}
;	ENDIF
;
;	FOR L_COUNTER, 0, 1000
;		SIF TRAINNAME:L_COUNTER == ""
;			CONTINUE
;		SIF COM_AUTO_COM_PRIORITY:L_COUNTER <= 0
;			CONTINUE
;		DEBUGPRINTFORML {L_COUNTER, 4}. %@"%TRAINNAME:L_COUNTER%한다", 26, LEFT% %NUM_FORMAT(COM_AUTO_COM_PRIORITY:L_COUNTER), 10, RIGHT%
;	NEXT
;	FOR L_COUNTER, ADD_NUM_대상이 실행, ADD_NUM_대상이 실행 + 1000
;		SIF TRAINNAME:(L_COUNTER - ADD_NUM_대상이 실행) == ""
;			CONTINUE
;		SIF COM_AUTO_COM_PRIORITY:L_COUNTER <= 0
;			CONTINUE
;		DEBUGPRINTFORML {L_COUNTER, 4}. %@"%TRAINNAME:(L_COUNTER - ADD_NUM_대상이 실행)%시킨다", 26, LEFT% %NUM_FORMAT(COM_AUTO_COM_PRIORITY:L_COUNTER), 10, RIGHT%
;	NEXT
;[ENDIF]

;-------------------------------------------------
; 제외 카테고리 우선도 0 미만
;-------------------------------------------------
FOR L_COUNTER, 0, ADD_NUM_대상이실행 + 1000
	IF COM_AUTO_CATEGORY_PRIORITY:(COM_AUTO_COM_CATEGORY:L_COUNTER) <= 0
		COM_AUTO_COM_PRIORITY:L_COUNTER = -1
	ENDIF
NEXT

;-------------------------------------------------
; 보정 전회 실행
;	우선도를 1/4로 한다
;-------------------------------------------------
IF TFLAG:55 != 0
	IF PREVCOM >= 0
		IF PREVCOM_TYPE == 0 && COM_AUTO_COM_PRIORITY:(PREVCOM + ADD_NUM_대상이실행) > 0
			COM_AUTO_COM_PRIORITY:(PREVCOM + ADD_NUM_대상이실행) /= 4
		ELSEIF PREVCOM_TYPE == 1 && COM_AUTO_COM_PRIORITY:PREVCOM > 0
			COM_AUTO_COM_PRIORITY:PREVCOM /= 4
		ELSEIF COM_AUTO_COM_PRIORITY:(PREVCOM + ADD_NUM_제삼자에게실행) > 0
			COM_AUTO_COM_PRIORITY:(PREVCOM + ADD_NUM_제삼자에게실행) /= 4
		ENDIF
	ENDIF
ENDIF

;-------------------------------------------------
; 선택 불가능 COM_ABLE
;	판정용 플래그를 온으로 해, 실행 가부만 확인하고
;	불가라면 우선도를 0으로 한다
;-------------------------------------------------
;커맨드 판정 플래그 ON
COM_ENABLE = 1

;주도권을 중심이 되는 캐릭터에게 건네준다
FLAG:주도권소유자 = MAIN_PLAYER

;[IF_DEBUG]
;	DEBUGPRINTFORML 
;	DEBUGPRINTFORML  DEBUG 선택 불가능 체크
;	DEBUGPRINTFORML      COM_ABLE로 튕겨진 커맨드
;[ENDIF]

;MAIN_PLAYER 실행, MAIN_TARGET 대상으로 실시할 수 있을까 체크
CALL CLEAR_MPLY()
CALL CLEAR_MTAR()
CALL ADD_MPLY(MAIN_PLAYER)
CALL ADD_MTAR(MAIN_TARGET)
FOR L_COUNTER, 0, 300
	SIF TRAINNAME:L_COUNTER == ""
		CONTINUE
	RESULT = 0
	TRYCALLFORM COM_ABLE{L_COUNTER}
	;여기에서는 반드시 인원수 부족해 실행 불가가 돌아오므로, 회피
	IF !RESULT && !GROUPMATCH(L_COUNTER, GET_TRAINNUM("차례로삽입"), GET_TRAINNUM("차례로Ａ삽입"), GET_TRAINNUM("번갈아삽입"), GET_TRAINNUM("번갈아Ａ삽입"), GET_TRAINNUM("Ｗ안면승마"))
		[IF_DEBUG]
;			DEBUGPRINTFORML {L_COUNTER, 4}. %TRAINNAME:L_COUNTER%하고는 불가
		[ENDIF]
		COM_AUTO_COM_PRIORITY:L_COUNTER = 0
	ENDIF
NEXT

;MAIN_TARGET 실행, MAIN_PLAYER 실행으로 실시할 수 있을까 체크
CALL CLEAR_MPLY()
CALL CLEAR_MTAR()
CALL ADD_MPLY(MAIN_TARGET)
CALL ADD_MTAR(MAIN_PLAYER)
FOR L_COUNTER, 0, 300
	SIF TRAINNAME:L_COUNTER == ""
		CONTINUE
	RESULT = 0
	TRYCALLFORM COM_ABLE{L_COUNTER}
	;여기에서는 반드시 인원수 부족해 실행 불가가 돌아오므로, 회피
	IF !RESULT && !GROUPMATCH(L_COUNTER, GET_TRAINNUM("차례로삽입"), GET_TRAINNUM("차례로Ａ삽입"), GET_TRAINNUM("번갈아삽입"), GET_TRAINNUM("번갈아Ａ삽입"), GET_TRAINNUM("Ｗ안면승마"))
		[IF_DEBUG]
;			DEBUGPRINTFORML {L_COUNTER, 4}. %TRAINNAME:L_COUNTER%되고는 불가
		[ENDIF]
		COM_AUTO_COM_PRIORITY:(L_COUNTER + ADD_NUM_대상이실행) = 0
	ENDIF
NEXT

;커맨드 판정 플래그 OFF
COM_ENABLE = 0

;[IF_DEBUG]
;	DEBUGPRINTFORML 
;	DEBUGPRINTFORML  DEBUG 커맨드 우선도 COM_ABLE 체크 후
;	DEBUGPRINTFORM      전회 실행 
;	IF TFLAG:55 != 0
;		IF PREVCOM_TYPE == 0
;			DEBUGPRINTFORML {PREVCOM}. %@"%TRAINNAME:PREVCOM%시킨다", 26, LEFT%
;		ELSEIF PREVCOM_TYPE == 1
;			DEBUGPRINTFORML {PREVCOM}. %@"%TRAINNAME:PREVCOM%한다", 26, LEFT%
;		ELSE
;			DEBUGPRINTFORML {PREVCOM}. %@"제삼자가 %TRAINNAME:PREVCOM%", 26, LEFT%
;		ENDIF
;	ENDIF
;	DEBUGPRINTFORM      윤활은
;	IF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET)
;		DEBUGPRINTFORML 충분히  PALAM {PALAM:MAIN_TARGET:윤활}
;	ELSE
;		DEBUGPRINTFORML 부족  PALAM {PALAM:MAIN_TARGET:윤활}
;	ENDIF
;
;	FOR L_COUNTER, 0, 1000
;		IF COM_AUTO_COM_PRIORITY:L_COUNTER > 0
;			DEBUGPRINTFORML {L_COUNTER, 4}. %@"%TRAINNAME:L_COUNTER%한다", 26, LEFT% %NUM_FORMAT(COM_AUTO_COM_PRIORITY:L_COUNTER), 10, RIGHT%
;		ENDIF
;	NEXT
;	FOR L_COUNTER, ADD_NUM_대상이 실행, ADD_NUM_대상이 실행 + 1000
;		IF COM_AUTO_COM_PRIORITY:L_COUNTER > 0
;			DEBUGPRINTFORML {L_COUNTER, 4}. %@"%TRAINNAME:(L_COUNTER - ADD_NUM_대상이 실행)%시킨다", 26, LEFT% %NUM_FORMAT(COM_AUTO_COM_PRIORITY:L_COUNTER), 10, RIGHT%
;		ENDIF
;	NEXT
;[ENDIF]

;-------------------------------------------------
; 정규화
;	커맨드의 우선도를 카테고리마다의 실행 확률에 맞추어 설정
;-------------------------------------------------
VARSET COM_AUTO_COM_PRIORITY_SUM, 0
VARSET COM_AUTO_COM_CATEGORY_COM_NUM, 0
COM_AUTO_CATEGORY_PRIORITY_SUM = 0

FOR L_COUNTER, 0, COM_AUTO_CATEGORY_NUM_MAX + 1
	SIF COM_AUTO_CATEGORY_PRIORITY:L_COUNTER <= 0
		CONTINUE
	COM_AUTO_CATEGORY_PRIORITY_SUM += MAX(COM_AUTO_CATEGORY_PRIORITY:L_COUNTER, 0)
NEXT

IF COM_AUTO_CATEGORY_PRIORITY_SUM >= 1
	FOR L_COUNTER, 0, COM_AUTO_CATEGORY_NUM_MAX + 1
		SIF COM_AUTO_CATEGORY_PRIORITY:L_COUNTER <= 0
			CONTINUE
		COM_AUTO_CATEGORY_PRIORITY:L_COUNTER = COM_AUTO_CATEGORY_PRIORITY:L_COUNTER * 10000 / COM_AUTO_CATEGORY_PRIORITY_SUM
	NEXT
ELSE
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 = 10000
ENDIF

FOR L_COUNTER, 0, 3000
	SIF COM_AUTO_CATEGORY_PRIORITY:(COM_AUTO_COM_CATEGORY:L_COUNTER) <= 0 || COM_AUTO_COM_PRIORITY:L_COUNTER <= 0
		CONTINUE
	COM_AUTO_COM_CATEGORY_COM_NUM:(COM_AUTO_COM_CATEGORY:L_COUNTER) ++
	COM_AUTO_COM_PRIORITY_SUM:(COM_AUTO_COM_CATEGORY:L_COUNTER) += MAX(COM_AUTO_COM_PRIORITY:L_COUNTER, 0)
NEXT

FOR L_COUNTER, 0, COM_AUTO_CATEGORY_NUM_MAX + 1
	SIF COM_AUTO_COM_PRIORITY_SUM:L_COUNTER <= 0 || COM_AUTO_COM_CATEGORY_COM_NUM:L_COUNTER <= 0
		CONTINUE
	COM_AUTO_COM_PRIORITY_SUM:L_COUNTER = COM_AUTO_COM_PRIORITY_SUM:L_COUNTER / COM_AUTO_COM_CATEGORY_COM_NUM:L_COUNTER
NEXT

FOR L_COUNTER, 0, 3000
	SIF COM_AUTO_COM_PRIORITY:L_COUNTER <= 0
		CONTINUE
	IF COM_AUTO_COM_PRIORITY_SUM:(COM_AUTO_COM_CATEGORY:L_COUNTER) > 0 && COM_AUTO_CATEGORY_PRIORITY:(COM_AUTO_COM_CATEGORY:L_COUNTER) > 0
		;카테고리 우선도*100/우선 정도계치
		COM_AUTO_COM_PRIORITY:L_COUNTER = COM_AUTO_COM_PRIORITY:L_COUNTER * COM_AUTO_CATEGORY_PRIORITY:(COM_AUTO_COM_CATEGORY:L_COUNTER) * 100 / COM_AUTO_COM_PRIORITY_SUM:(COM_AUTO_COM_CATEGORY:L_COUNTER)
	ELSE
		COM_AUTO_COM_PRIORITY:L_COUNTER = 0
	ENDIF
NEXT

[IF_DEBUG]
	DEBUGPRINTFORML 
	DEBUGPRINTFORML   DEBUG 카테고리 우선도 정규화 후
	FOR L_COUNTER, 1, COM_AUTO_CATEGORY_NUM_MAX + 1
		IF COM_AUTO_CATEGORY_PRIORITY:L_COUNTER > 0
			DEBUGPRINTFORML {L_COUNTER, 4}. %COM_AUTO_COM_CATEGORY_NAME(L_COUNTER), 26, LEFT% %NUM_FORMAT(COM_AUTO_CATEGORY_PRIORITY:L_COUNTER), 10, RIGHT%
		ENDIF
	NEXT

;	DEBUGPRINTFORML 
;	DEBUGPRINTFORML  DEBUG 커맨드 우선도 정규화 후
;	DEBUGPRINTFORML      윤활 CUP {CUP:MAIN_TARGET:윤활}/PALAM {PALAM:MAIN_TARGET:윤활}
;
;	FOR L_COUNTER, 0, 1000
;		IF COM_AUTO_COM_PRIORITY:L_COUNTER > 0
;			DEBUGPRINTFORML {L_COUNTER, 4}. %@"%TRAINNAME:L_COUNTER%한다", 26, LEFT% %NUM_FORMAT(COM_AUTO_COM_PRIORITY:L_COUNTER), 10, RIGHT%
;		ENDIF
;	NEXT
;	FOR L_COUNTER, ADD_NUM_대상이 실행, ADD_NUM_대상이 실행 + 1000
;		IF COM_AUTO_COM_PRIORITY:L_COUNTER > 0
;			DEBUGPRINTFORML {L_COUNTER, 4}. %@"%TRAINNAME:(L_COUNTER - ADD_NUM_대상이 실행)%시킨다", 26, LEFT% %NUM_FORMAT(COM_AUTO_COM_PRIORITY:L_COUNTER), 10, RIGHT%
;		ENDIF
;	NEXT
[ENDIF]

;-------------------------------------------------
; 보정 성벽 설정
;	기호 Lv를 설정한 커맨드는, 카테고리의 선택율을 무시해 우선도를 올린다
;-------------------------------------------------
SEXUAL_PREFERENCE_ADD:0 = 0
SEXUAL_PREFERENCE_ADD:1 = 300000
SEXUAL_PREFERENCE_ADD:2 = 800000
SEXUAL_PREFERENCE_ADD:3 = 2000000

SEXUAL_PREFERENCE_MULTI:0 = 1
SEXUAL_PREFERENCE_MULTI:1 = 2
SEXUAL_PREFERENCE_MULTI:2 = 4
SEXUAL_PREFERENCE_MULTI:3 = 12

;-------------------------------------------------
; 디버그 성벽 설정 기호 레벨 표시
;-------------------------------------------------
[IF_DEBUG]
	DEBUGPRINTFORML 
	DEBUGPRINTFORML   DEBUG 성벽 설정
[ENDIF]
FOR L_COUNTER, 0, 300
	;카테고리가 금지되고 있으면 추가하지 않는다
	SIF COM_AUTO_CATEGORY_PRIORITY:(COM_AUTO_COM_CATEGORY:L_COUNTER) <= 0
		CONTINUE
	SIF COM_TENDENCY:MAIN_PLAYER:L_COUNTER <= 0 && COM_TENDENCY:MAIN_PLAYER:(L_COUNTER + ADD_NUM_대상이실행) <= 0 && COM_TENDENCY:MAIN_PLAYER:(L_COUNTER + ADD_NUM_제삼자에게실행) <= 0
		CONTINUE

	IF COM_TENDENCY:MAIN_PLAYER:L_COUNTER > 0 && COM_EXP:MAIN_PLAYER:L_COUNTER > 0
		[IF_DEBUG]
			DEBUGPRINTFORML {L_COUNTER, 4}. %@"%TRAINNAME:L_COUNTER%한다", 26, LEFT% 기호 Lv.{COM_TENDENCY:MAIN_PLAYER:L_COUNTER}
		[ENDIF]
		COM_AUTO_COM_PRIORITY:L_COUNTER = MAX(COM_AUTO_COM_PRIORITY:L_COUNTER, 0) * SEXUAL_PREFERENCE_MULTI:(COM_TENDENCY:MAIN_PLAYER:L_COUNTER)
		COM_AUTO_COM_PRIORITY:L_COUNTER += SEXUAL_PREFERENCE_ADD:(COM_TENDENCY:MAIN_PLAYER:L_COUNTER)
	ENDIF

	IF COM_TENDENCY:MAIN_PLAYER:(L_COUNTER + ADD_NUM_대상이실행) > 0 && COM_EXP:MAIN_PLAYER:L_COUNTER > 0
		[IF_DEBUG]
			DEBUGPRINTFORML {L_COUNTER, 4}. %@"%TRAINNAME:L_COUNTER%된다", 26, LEFT% 기호 Lv.{COM_TENDENCY:MAIN_PLAYER:(L_COUNTER + ADD_NUM_대상이실행)}
		[ENDIF]
		COM_AUTO_COM_PRIORITY:(L_COUNTER + ADD_NUM_대상이실행) = MAX(COM_AUTO_COM_PRIORITY:(L_COUNTER + ADD_NUM_대상이실행), 0) * SEXUAL_PREFERENCE_MULTI:(COM_TENDENCY:MAIN_PLAYER:(L_COUNTER + ADD_NUM_대상이실행))
		COM_AUTO_COM_PRIORITY:(L_COUNTER + ADD_NUM_대상이실행) += SEXUAL_PREFERENCE_ADD:(COM_TENDENCY:MAIN_PLAYER:(L_COUNTER + ADD_NUM_대상이실행))
	ENDIF

	IF COM_TENDENCY:MAIN_PLAYER:(L_COUNTER + ADD_NUM_제삼자에게실행) > 0 && COM_EXP:MAIN_PLAYER:L_COUNTER > 0
		[IF_DEBUG]
			DEBUGPRINTFORML {L_COUNTER, 4}. %@"제삼자에게 %TRAINNAME:L_COUNTER%", 26, LEFT% 기호 Lv.{COM_TENDENCY:MAIN_PLAYER:(L_COUNTER + ADD_NUM_제삼자에게실행)}
		[ENDIF]
		COM_AUTO_COM_PRIORITY:(L_COUNTER + ADD_NUM_제삼자에게실행) = MAX(COM_AUTO_COM_PRIORITY:(L_COUNTER + ADD_NUM_제삼자에게실행), 0) * SEXUAL_PREFERENCE_MULTI:(COM_TENDENCY:MAIN_PLAYER:(L_COUNTER + ADD_NUM_제삼자에게실행))
		COM_AUTO_COM_PRIORITY:(L_COUNTER + ADD_NUM_제삼자에게실행) += SEXUAL_PREFERENCE_ADD:(COM_TENDENCY:MAIN_PLAYER:(L_COUNTER + ADD_NUM_제삼자에게실행))
	ENDIF
NEXT

;-------------------------------------------------
; 보정 윤활 부족
;	일부의 커맨드는 윤활 부족하면 선택 저하
;-------------------------------------------------
IF !COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET)
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("꼬리삽입") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("꼬리삽입") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("꼬리Ａ삽입") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("꼬리Ａ삽입") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("조개맞대기") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("조개맞대기") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("쌍두바이브") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("쌍두바이브") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("차례로삽입") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("차례로삽입") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("차례로Ａ삽입") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("차례로Ａ삽입") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("번갈아삽입") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("번갈아삽입") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("번갈아Ａ삽입") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("번갈아Ａ삽입") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("바이브") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("바이브") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("애널바이브") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("애널바이브") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("Ｖ확장벌룬") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("Ｖ확장벌룬") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("Ａ확장벌룬") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("Ａ확장벌룬") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("피스트퍽") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("피스트퍽") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("애널피스트") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("애널피스트") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("촉수삽입") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("촉수삽입") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("촉수Ａ삽입") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("촉수Ａ삽입") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("촉수산란") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("촉수산란") /= 2
	SIF COM_AUTO_COM_PRIORITY:GET_TRAINNUM("촉수Ａ산란") > 0
		COM_AUTO_COM_PRIORITY:GET_TRAINNUM("촉수Ａ산란") /= 2
	SIF COM_AUTO_COM_PRIORITY:(ADD_NUM_대상이실행 + GET_TRAINNUM("스마타")) > 0
