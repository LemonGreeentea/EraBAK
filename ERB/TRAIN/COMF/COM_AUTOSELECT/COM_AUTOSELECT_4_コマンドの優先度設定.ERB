	COM_AUTO_COM_PRIORITY:(ADD_NUM_대상이실행 + GET_TRAINNUM("Ａ배면입위")) = -1
	COM_AUTO_COM_PRIORITY:(ADD_NUM_대상이실행 + GET_TRAINNUM("Ａ서서뒷치기")) = -1
ENDIF

;-------------------------------------------------
; 제외 커맨드 필터의 반영
;-------------------------------------------------
FOR L_COUNTER, 0, 1000
	IF COM_FILTER:L_COUNTER
		COM_AUTO_COM_PRIORITY:L_COUNTER = -1
		COM_AUTO_COM_PRIORITY:(L_COUNTER + ADD_NUM_대상이실행) = -1
	ENDIF
NEXT

;-------------------------------------------------
; 선택 불가능 COM_ABLE
;	재차 우선도를 0으로 한다
;-------------------------------------------------
;커맨드 판정 플래그 ON
COM_ENABLE = 1

;주도권을 중심이 되는 캐릭터에게 건네준다
FLAG:주도권소유자 = MAIN_PLAYER

CALL CLEAR_MPLY()
CALL CLEAR_MTAR()
CALL ADD_MPLY(MAIN_PLAYER)
CALL ADD_MTAR(MAIN_TARGET)
FOR L_COUNTER, 0, ADD_NUM_대상이실행
	SIF COM_AUTO_COM_PRIORITY:L_COUNTER <= 0
		CONTINUE
	RESULT = 0
	TRYCALLFORM COM_ABLE{L_COUNTER}
	;여기에서는 반드시 실행 불가가 돌아오므로, 회피
	IF !RESULT && !GROUPMATCH(L_COUNTER, GET_TRAINNUM("2개삽입"), GET_TRAINNUM("Ａ2개삽입"), GET_TRAINNUM("차례로삽입"), GET_TRAINNUM("차례로Ａ삽입"), GET_TRAINNUM("번갈아삽입"), GET_TRAINNUM("번갈아Ａ삽입"), GET_TRAINNUM("Ｗ안면승마"))
		COM_AUTO_COM_PRIORITY:L_COUNTER = -1
	ENDIF
NEXT
CALL CLEAR_MPLY()
CALL CLEAR_MTAR()
CALL ADD_MPLY(MAIN_TARGET)
CALL ADD_MTAR(MAIN_PLAYER)
FOR L_COUNTER, ADD_NUM_대상이실행, ADD_NUM_대상이실행 + 1000
	SIF COM_AUTO_COM_PRIORITY:L_COUNTER <= 0
		CONTINUE
	RESULT = 0
	TRYCALLFORM COM_ABLE{L_COUNTER - ADD_NUM_대상이실행}
	;여기에서는 반드시 실행 불가가 돌아오므로, 회피
	IF !RESULT && !GROUPMATCH(L_COUNTER - ADD_NUM_대상이실행, GET_TRAINNUM("2개삽입"), GET_TRAINNUM("Ａ2개삽입"), GET_TRAINNUM("차례로삽입"), GET_TRAINNUM("차례로Ａ삽입"), GET_TRAINNUM("번갈아삽입"), GET_TRAINNUM("번갈아Ａ삽입"), GET_TRAINNUM("Ｗ안면승마"), GET_TRAINNUM("Ｗ안면승마"))
		COM_AUTO_COM_PRIORITY:L_COUNTER = -1
	ENDIF
NEXT

;커맨드 판정 플래그 OFF
COM_ENABLE = 0

[IF_DEBUG]
	DEBUGPRINTFORML 
	DEBUGPRINTFORML   DEBUG 커맨드 우선도 최종
	DEBUGPRINTFORML      윤활 CUP {CUP:MAIN_TARGET:윤활} / PALAM {PALAM:MAIN_TARGET:윤활}
	FOR L_COUNTER, 0, ADD_NUM_대상이실행
		IF COM_AUTO_COM_PRIORITY:L_COUNTER > 0
			DEBUGPRINTFORML {L_COUNTER, 4}. %@"%TRAINNAME:L_COUNTER%한다", 26, LEFT% %NUM_FORMAT(COM_AUTO_COM_PRIORITY:L_COUNTER), 10, RIGHT%
		ENDIF
	NEXT
	FOR L_COUNTER, ADD_NUM_대상이실행, ADD_NUM_대상이실행 + 1000
		IF COM_AUTO_COM_PRIORITY:(L_COUNTER) > 0
			DEBUGPRINTFORML {L_COUNTER, 4}. %@"%TRAINNAME:(L_COUNTER - ADD_NUM_대상이실행)%시킨다", 26, LEFT% %NUM_FORMAT(COM_AUTO_COM_PRIORITY:L_COUNTER), 10, RIGHT%
		ENDIF
	NEXT
[ENDIF]

RETURN 0

