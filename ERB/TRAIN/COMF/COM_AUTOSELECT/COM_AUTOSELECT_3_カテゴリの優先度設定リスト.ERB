;-------------------------------------------------
;# 01 COM_CATEGORY_NUM 애무한다
;-------------------------------------------------
@COM_AUTOSELECT_SET_CATEGORY_PRIORITY_1(MAIN_PLAYER, MAIN_TARGET)
#DIM MAIN_PLAYER
#DIM MAIN_TARGET

;-------------------------------------------------
; 기초
;-------------------------------------------------
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 += GET_INITIATIVE_U_RANK(MAIN_PLAYER) * 5
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 += AUTO_N_TYPE_활발(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 += AUTO_N_TYPE_얌전함(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 += AUTO_N_TYPE_쿨(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 += AUTO_N_TYPE_늠름했다(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 += AUTO_N_TYPE_건달(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 += AUTO_N_TYPE_매료(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 += AUTO_N_TYPE_매료(MASTER, 0)
;-------------------------------------------------
; 성적 기호 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 && (GETBIT(SEXUAL_PREFERENCE:MAIN_PLAYER:0, 성적기호_애무하고싶다) || GETBIT(SEXUAL_PREFERENCE:MAIN_PLAYER:0, 성적기호_봉사하고싶다))
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 *= 3
ENDIF
;-------------------------------------------------
; 함락 경향 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 && TALENT:MAIN_PLAYER:연모
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 *= 2
ENDIF
;-------------------------------------------------
; 특수 세력 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 && TALENT:MAIN_PLAYER:특수세력소질 == 특수세력_자경단
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_애무한다 *= 2
ENDIF

RETURN 0

;-------------------------------------------------
;# 02 COM_AUTO_CATEGORY_NUM_성교한다
;-------------------------------------------------
@COM_AUTOSELECT_SET_CATEGORY_PRIORITY_2(MAIN_PLAYER, MAIN_TARGET)
#DIM MAIN_PLAYER
#DIM MAIN_TARGET
#DIM COM_AUTO_CHECK_FLAG_IS_CATEGORY	;가부 플래그

;-------------------------------------------------
; 기초
;-------------------------------------------------
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 += GET_INITIATIVE_U_RANK(MAIN_PLAYER) * 5
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 += AUTO_N_TYPE_활발(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 += AUTO_N_TYPE_얌전함(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 += AUTO_N_TYPE_쿨(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 += AUTO_N_TYPE_늠름했다(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 += AUTO_N_TYPE_건달(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 += AUTO_N_TYPE_매료(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 += AUTO_N_TYPE_매료(MASTER, 0)
;-------------------------------------------------
; 가부 체크
;-------------------------------------------------
;가부 체크 리셋트
VARSET COM_AUTO_CHECK_FLAG_IS_CATEGORY
;윤활 추가 체크 리셋트
VARSET COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH
;-------------------------------------------------
; # 대상이 위안모브나 특수 세력
;	특수 세력의 경우, 서큐버스나 자경단은 제외함
;-------------------------------------------------
IF CHECK_SP_SADIST_CHARA(MAIN_PLAYER) || GROUPMATCH(TALENT:MAIN_PLAYER:위안모브, 위안_캐릭터_개, 위안_캐릭터_돼지,  위안_캐릭터_말, 위안_캐릭터_원숭이)
	;---------------------------------------------
	;대상 남성·실행 여성
	;---------------------------------------------
	IF IS_MALE(MAIN_TARGET) && IS_MALE(MAIN_PLAYER)
		;대상의 윤활·실행의 도착도 800이상·실행의 주도도Ｕ800이상·실행의 새드가 필요
		IF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) && ABL:MAIN_PLAYER:도착도 >= 800 && ABL:MAIN_PLAYER:주도도Ｕ >= 800 && GETBIT(TALENT:MAIN_PLAYER:음란계, 소질_음란_새드)
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활을 채우면 조건을 달성한다
		ELSEIF ABL:MAIN_PLAYER:도착도 >= 800 && ABL:MAIN_PLAYER:주도도Ｕ >= 800 && GETBIT(TALENT:MAIN_PLAYER:음란계, 소질_음란_새드)
			COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH = 1
		ENDIF
	;---------------------------------------------
	;대상 남성·실행 남성
	;---------------------------------------------
	ELSEIF IS_MALE(MAIN_TARGET) && IS_MALE(MAIN_PLAYER)
		;대상의 윤활·실행의 도착도 500이상·실행의 주도도Ｕ500이상
		IF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) && ABL:MAIN_PLAYER:도착도 >= 500 && ABL:MAIN_PLAYER:주도도Ｕ >= 500
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활을 채우면 조건을 달성한다
		ELSEIF ABL:MAIN_PLAYER:도착도 >= 500 && ABL:MAIN_PLAYER:주도도Ｕ >= 500
			COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH = 1
		ENDIF
	;---------------------------------------------
	;대상 여성·실행 여성　
	;---------------------------------------------
	ELSEIF IS_FEMALE(MAIN_TARGET) && IS_FEMALE(MAIN_PLAYER)
		;대상의 윤활·실행의 도착도 100이상·실행의 주도도Ｕ100이상
		IF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) && ABL:MAIN_PLAYER:도착도 >= 100 && ABL:MAIN_PLAYER:주도도Ｕ >= 300
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활을 채우면 조건을 달성한다
		ELSEIF ABL:MAIN_PLAYER:도착도 >= 100 && ABL:MAIN_PLAYER:주도도Ｕ >= 100
			COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH = 1
		ENDIF
	;---------------------------------------------
	;대상 여성·실행 남성
	;---------------------------------------------
	ELSEIF IS_FEMALE(MAIN_TARGET) && IS_MALE(MAIN_PLAYER)
		;대상의 윤활
		IF  COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) 
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활을 채우면 조건을 달성한다
		ELSE
			COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH = 1
		ENDIF
	ENDIF
;-------------------------------------------------
; # 대상이 위안모브나 특수 세력이 아니다
;	또는 특수 세력이어도 서큐버스나 자경단
;-------------------------------------------------
ELSE
	;---------------------------------------------
	;대상 남성＆실행 여성 역전하므로 가장 도착도와 주도도Ｕ를 필요로 한다
	;---------------------------------------------
	IF IS_MALE(MAIN_TARGET) && IS_FEMALE(MAIN_PLAYER)
		;대상의 윤활이 불충분하면 새드기분이 필요
		IF !COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) && ABL:MAIN_PLAYER:도착도 >= 500 && ABL:MAIN_PLAYER:주도도Ｕ >= 500
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활이 충분한들, 실행의 도착도 300이상·실행의 주도도Ｕ300이상
		ELSEIF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) && ABL:MAIN_PLAYER:도착도 >= 300 && ABL:MAIN_PLAYER:주도도Ｕ >= 300
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활을 채우면 조건을 달성된다
		ELSEIF ABL:MAIN_PLAYER:도착도 >= 300 && ABL:MAIN_PLAYER:주도도Ｕ >= 300
			COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH = 1
		ENDIF
	;---------------------------------------------
	;대상 남성＆실행 남성 남자역의 여배우를 좋아한다
	;---------------------------------------------
	ELSEIF IS_MALE(MAIN_TARGET) && IS_MALE(MAIN_PLAYER)
		;대상의 윤활이 불충분하면 새드기분이 필요
		IF !COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) && ABL:MAIN_PLAYER:도착도 >= 100 && ABL:MAIN_PLAYER:주도도Ｕ >= 300
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활이 충분한들, 실행의 주도도Ｕ-100이상
		ELSEIF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) && ABL:MAIN_PLAYER:주도도Ｕ >= -100
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활을 채우면 조건을 달성된다
		ELSEIF ABL:MAIN_PLAYER:주도도Ｕ >= -100
			COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH = 1
		ENDIF
	;---------------------------------------------
	;대상 여성＆실행 여성 여자 역을 좋아한다
	;---------------------------------------------
	ELSEIF IS_FEMALE(MAIN_TARGET) && IS_FEMALE(MAIN_PLAYER)
		;대상의 윤활이 불충분하면 새드기분이 필요
		IF !COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) && ABL:MAIN_PLAYER:도착도 >= 300 && ABL:MAIN_PLAYER:주도도Ｕ >= 500
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활이 충분한들, 실행의 도착도 100이상·실행의 주도도Ｕ100이상
		ELSEIF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) && ABL:MAIN_PLAYER:도착도 >= 100 && ABL:MAIN_PLAYER:주도도Ｕ >= 100
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활을 채우면 조건을 달성된다
		ELSEIF ABL:MAIN_PLAYER:도착도 >= 100 && ABL:MAIN_PLAYER:주도도Ｕ >= 100
			COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH = 1
		ENDIF
	;---------------------------------------------
	;대상 여성＆실행 남성
	;---------------------------------------------
	ELSEIF IS_FEMALE(MAIN_TARGET) && IS_MALE(MAIN_PLAYER)
		;대상의 윤활이 불충분하면, 실행의 도착도 100이상·실행의 주도도Ｕ100이상
		IF !COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET) && ABL:MAIN_PLAYER:도착도 >= 100 && ABL:MAIN_PLAYER:주도도Ｕ >= 100
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활이 충분한들 무조건
		ELSEIF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET)
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;대상의 윤활을 채우면 조건을 달성된다
		ELSE
			COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH = 1
		ENDIF
	ENDIF
ENDIF
;-------------------------------------------------
; 성적 기호 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 > 0 && GETBIT(SEXUAL_PREFERENCE:MAIN_PLAYER:0, 성적기호_성교하고싶다)
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 *= 3
ENDIF
;-------------------------------------------------
; 함락 경향 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 > 0 && TALENT:MAIN_PLAYER:연모
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 *= 2
ENDIF
;-------------------------------------------------
; 특수 세력 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 > 0 && TALENT:MAIN_PLAYER:특수세력소질 == 특수세력_홉고블린
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 *= 2
ENDIF
;-------------------------------------------------
; 특수 세력 함락 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 > 0 && TALENT:MAIN_PLAYER:특수세력함락계 == 특수세력_서큐버스
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 *= 2
ENDIF
;-------------------------------------------------
; 그 외 보정
;-------------------------------------------------
;	삽입 상태 보정	@IS_INSERT_MUTUAL	3= ARG:1이 ARG:0의Ｖ에 삽입	4= ARG:1이 ARG:0의Ａ에 삽입
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 > 0 && GROUPMATCH(IS_INSERT_MUTUAL(MAIN_PLAYER, MAIN_TARGET), 3, 4)
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 /= 2
ENDIF
;	턴수보정 처음의 5 턴 정도는 다른 행동을 취하기 쉽다
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 > 0 && TFLAG:55 < 5
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 /= 10
ENDIF
;	콘피그 설정 보정 남자 주인공에게로의 역조교의 경향이 인내 시킨다
;	장대를 만지작거리지 않는 설정이라고 코멘트 되고 있어 성교 제한되고 있었으므로, 암컷화일까하고 생각해 뒤는 적극에 파도록(듯이)해 본다
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 > 0 && CONFIG:8 == 1 && MAIN_TARGET == MASTER && HAS_PENIS(MAIN_TARGET) && GROUPMATCH(FLAG:조교모드, 조교_역조교특수, 조교_역조교통상, 조교_위안)
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 *= 2
ENDIF
;	피로
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 > 0 && BASE:MAIN_PLAYER:체력 <= MAXBASE:MAIN_PLAYER:체력 / 2
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 /= 10
ENDIF
;-------------------------------------------------
; 불가
;-------------------------------------------------
IF !COM_AUTO_CHECK_FLAG_IS_CATEGORY
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_성교한다 = -1
ENDIF

RETURN 0

;-------------------------------------------------
;# 03 COM_AUTO_CATEGORY_NUM_윤활 추가한다
;	성교 할 수 없을 때, 피임 할 수 없을 때의 받침접시에도
;-------------------------------------------------
@COM_AUTOSELECT_SET_CATEGORY_PRIORITY_3(MAIN_PLAYER, MAIN_TARGET)
#DIM MAIN_PLAYER
#DIM MAIN_TARGET

;-------------------------------------------------
; 기초
;-------------------------------------------------
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 += GET_INITIATIVE_U_RANK(MAIN_PLAYER) * 5
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 += AUTO_N_TYPE_활발(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 += AUTO_N_TYPE_얌전함(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 += AUTO_N_TYPE_쿨(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 += AUTO_N_TYPE_늠름했다(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 += AUTO_N_TYPE_건달(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 += AUTO_N_TYPE_매료(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 += AUTO_N_TYPE_매료(MASTER, 0)
;-------------------------------------------------
; 성적 기호 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 > 0  && GETBIT(SEXUAL_PREFERENCE:MAIN_PLAYER:0, 성적기호_성교하고싶다)
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 *= 3
ENDIF
;-------------------------------------------------
; 특수 세력 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 > 0 && TALENT:MAIN_PLAYER:특수세력소질 == 특수세력_홉고블린
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 *= 2
ENDIF
;-------------------------------------------------
; 함락 경향 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 > 0 && TALENT:MAIN_PLAYER:연모
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 *= 2
ENDIF
;-------------------------------------------------
; 특수 소질 보정
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 > 0 && TALENT:MAIN_PLAYER:기사
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 *= 2
ENDIF
;-------------------------------------------------
; 그 외 보정
;-------------------------------------------------
;	젖고가 부족함
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 > 0 && !COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_TARGET)
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 *= 3
ENDIF
;-------------------------------------------------
; 불가
;-------------------------------------------------
IF !COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_윤활추가한다 = -1
ENDIF

RETURN 0

;-------------------------------------------------
;# 04 COM_AUTO_CATEGORY_NUM_피임한다
;-------------------------------------------------
@COM_AUTOSELECT_SET_CATEGORY_PRIORITY_4(MAIN_PLAYER, MAIN_TARGET)
#DIM MAIN_PLAYER
#DIM MAIN_TARGET

;-------------------------------------------------
; 기초
;-------------------------------------------------
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_피임한다 += GET_INITIATIVE_U_RANK(MAIN_PLAYER) * 5
