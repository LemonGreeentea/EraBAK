IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_소배설시킨다 > 0 && TALENT:MAIN_PLAYER:특수세력소질 == 특수세력_광신도
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_소배설시킨다 *= 2
ENDIF
;-------------------------------------------------
; 불가
;-------------------------------------------------
IF !CHECK_SP_SADIST_CHARA(MAIN_PLAYER)
	IF ABL:MAIN_PLAYER:배설 < 1 || ABL:MAIN_PLAYER:도착도 < 100 || (ABL:MAIN_PLAYER:주도도Ｕ >= -100 && ABL:MAIN_PLAYER:주도도Ｕ < 100)
		COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_소배설시킨다 = -1
	ENDIF
ENDIF

RETURN 0

;-------------------------------------------------
;# 21 COM_AUTO_CATEGORY_NUM_대배설시킨다
;	배설 2·도착도 300이상·주도도Ｕ가-300 미만이나 주도도가 300이상
;-------------------------------------------------
@COM_AUTOSELECT_SET_CATEGORY_PRIORITY_21(MAIN_PLAYER, MAIN_TARGET)
#DIM MAIN_PLAYER
#DIM MAIN_TARGET

;-------------------------------------------------
; 기초
;-------------------------------------------------
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 += GET_INITIATIVE_U_RANK(MAIN_PLAYER) * -1 * 5
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 += GET_PERVERSION_RANK(MAIN_PLAYER) * 5
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 += AUTO_N_TYPE_활발(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 += AUTO_N_TYPE_얌전함(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 += AUTO_N_TYPE_쿨(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 += AUTO_N_TYPE_늠름했다(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 += AUTO_N_TYPE_건달(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 += AUTO_N_TYPE_매료(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 += AUTO_N_TYPE_매료(MASTER, 1)
;-------------------------------------------------
; 성적 기호
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 > 0  && GETBIT(SEXUAL_PREFERENCE:MAIN_PLAYER:0, 성적기호_욕보이고싶다)
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 *= 3
ENDIF
;-------------------------------------------------
; 특수 세력
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 > 0 && TALENT:MAIN_PLAYER:특수세력소질 == 특수세력_광신도
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 *= 2
ENDIF
;-------------------------------------------------
; 불가
;-------------------------------------------------
IF !CHECK_SP_SADIST_CHARA(MAIN_PLAYER)
	IF ABL:MAIN_PLAYER:배설 < 2 || ABL:MAIN_PLAYER:도착도 < 300 || (ABL:MAIN_PLAYER:주도도Ｕ >= -300 && ABL:MAIN_PLAYER:주도도Ｕ < 300)
		COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_대배설시킨다 = -1
	ENDIF
ENDIF

RETURN 0

;-------------------------------------------------
;# 22 COM_AUTO_CATEGORY_NUM_촉수를 취급하게 한다
;	신비지식·도착도 500이상·주도도Ｕ-500 미만
;-------------------------------------------------
@COM_AUTOSELECT_SET_CATEGORY_PRIORITY_22(MAIN_PLAYER, MAIN_TARGET)
#DIM MAIN_PLAYER
#DIM MAIN_TARGET

;-------------------------------------------------
; 기초
;-------------------------------------------------
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 += GET_INITIATIVE_U_RANK(MAIN_PLAYER) * -1 * 5
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 += GET_PERVERSION_RANK(MAIN_PLAYER) * 5
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 += AUTO_N_TYPE_활발(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 += AUTO_N_TYPE_얌전함(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 += AUTO_N_TYPE_쿨(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 += AUTO_N_TYPE_늠름했다(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 += AUTO_N_TYPE_건달(MAIN_PLAYER, 1)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 += AUTO_N_TYPE_매료(MAIN_PLAYER, 0)
COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 += AUTO_N_TYPE_매료(MASTER, 1)
;-------------------------------------------------
; 성적 기호
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 > 0  && GETBIT(SEXUAL_PREFERENCE:MAIN_PLAYER:0, 성적기호_촉수로범해지고싶다)
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 *= 3
ENDIF
;-------------------------------------------------
; 함락 경향
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 > 0 && TALENT:MAIN_PLAYER:복종
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 *= 2
ENDIF
;-------------------------------------------------
; 특수 세력 함락
;-------------------------------------------------
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 > 0 && TALENT:MAIN_PLAYER:특수세력함락계 == 특수세력_촉수
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 *= 2
ENDIF
;-------------------------------------------------
; 불가
;-------------------------------------------------
IF !TALENT:MAIN_TARGET:신비지식
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 = -1
ENDIF
IF ABL:MAIN_PLAYER:도착도 < 500 || ABL:MAIN_PLAYER:주도도Ｕ >= -500
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_촉수를취급하게한다 = -1
ENDIF

RETURN 0

;-------------------------------------------------
;# 23 COM_AUTO_CATEGORY_NUM_설득
;-------------------------------------------------
@COM_AUTOSELECT_SET_CATEGORY_PRIORITY_23(MAIN_PLAYER, MAIN_TARGET)
#DIM MAIN_PLAYER
#DIM MAIN_TARGET
#DIM L_COUNTER
#DIM L_COUNTER_CHECK

;-------------------------------------------------
; 기초
;-------------------------------------------------
;TFLAG:55 현재 시간
IF FLAG:조교모드 == 조교_역조교통상 && CFLAG:MAIN_PLAYER:소속 != CFLAG:MAIN_TARGET:소속 && TFLAG:55 >= 5 && FLAG:권유경과기간 > 3
	L_COUNTER_CHECK = 4
	FOR L_COUNTER, 0, COM_AUTO_CATEGORY_NUM_MAX + 1
		L_COUNTER_CHECK += MAX(COM_AUTO_CATEGORY_PRIORITY:L_COUNTER, 0)
	NEXT
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_설득 = LIMIT(FLAG:권유경과기간 * 20, 0, 90)
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_설득 = L_COUNTER_CHECK * COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_설득 / (100 - COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_설득)
ELSE
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_설득 = -1
ENDIF
IF COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_설득 <= 0
	COM_AUTO_CATEGORY_PRIORITY:COM_AUTO_CATEGORY_NUM_설득 = -1
ENDIF

DEBUGPRINTFORML      FLAG:권유 경과 기간 {FLAG:권유경과기간}기

RETURN 0

