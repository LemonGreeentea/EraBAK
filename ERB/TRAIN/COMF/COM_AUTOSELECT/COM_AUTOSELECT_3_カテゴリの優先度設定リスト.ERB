		IF !COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_PLAYER) && ABL:MAIN_PLAYER:도착도 >= 100 && ABL:MAIN_PLAYER:주도도Ｕ < -100
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
