	;---------------------------------------------
	;대상 남성＆실행 남성 남자역의 여배우를 좋아한다
	;---------------------------------------------
	ELSEIF IS_MALE(MAIN_TARGET) && IS_MALE(MAIN_PLAYER)
		;자신의 윤활이 불충분하면 마조기분이 필요
		IF !COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_PLAYER) && ABL:MAIN_PLAYER:도착도 >= 300 && ABL:MAIN_PLAYER:주도도Ｕ < -500
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;자신의 윤활이 충분한들, 실행의 도착도 100이상·실행의 주도도Ｕ-100 미만
		ELSEIF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_PLAYER) && ABL:MAIN_PLAYER:도착도 >= 100 && ABL:MAIN_PLAYER:주도도Ｕ < -300
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;실행의 윤활을 채우면 조건을 달성한다
		ELSEIF ABL:MAIN_PLAYER:도착도 >= 100 && ABL:MAIN_PLAYER:주도도Ｕ < -300
			COM_AUTO_CHECK_FLAG_IS_WET_ENOUGH = 1
		ENDIF
	;---------------------------------------------
	;대상 여성＆실행 여성 여자 역을 좋아한다
	;---------------------------------------------
	ELSEIF IS_FEMALE(MAIN_TARGET) && IS_FEMALE(MAIN_PLAYER)
		;자신의 윤활이 불충분하면 마조기분이 필요
		IF !COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_PLAYER) && ABL:MAIN_PLAYER:도착도 >= 100 && ABL:MAIN_PLAYER:주도도Ｕ < -300
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;자신의 윤활이 충분한들, 실행의 주도도Ｕ100 미만
		ELSEIF COM_AUTOSELECT_IS_WET_ENOUGH(MAIN_PLAYER) && ABL:MAIN_PLAYER:주도도Ｕ < 100
			COM_AUTO_CHECK_FLAG_IS_CATEGORY = 1
		;실행의 윤활을 채우면 조건을 달성한다
