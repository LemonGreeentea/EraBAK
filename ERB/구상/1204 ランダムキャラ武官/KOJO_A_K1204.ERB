@KOJO_TRAIN_START_A1_K1204
;[공허]상태라면 구상을 표시하지 않는다
IF TALENT:공허 || CFLAG:400 == 1
	RETURN
ENDIF
;━━━━━━━━━━━━━━━━━━━━

;━━━━━━━━━━━━━━━━━━━━
IF CFLAG:400 == 0
	PRINTFORML 구상을 선택해 주세요
		PRINTL [0] 구상을 사용하지 않는다
		PRINTL [1] 무관 구상

		$INPUT_LOOP
		INPUT
		
		IF RESULT == 0
			PRINTFORMW 구상을 사용하지 않습니다
			CFLAG:400 = 1
			RETURN
		ELSEIF RESULT == 1
			PRINTFORMW 무관 구상을 사용합니다
			CFLAG:400 = 2
				PRINTFORML 구상에 맞추어 소질을 변경합니까?
				PRINTFORML （주의  꽤 소질이 변질합니다）
					PRINTL [0] 변경하지 않는다
					PRINTL [1] 변경한다
				$INPUT_LOOP2
				INPUT
				IF RESULT == 0
					PRINTFORMW 소질은 그대로 개시합니다.
					RETURN
				ELSEIF RESULT == 1
					PRINTFORMW 소질을 변경합니다.
						TALENT:아픔에약함 = 0
						TALENT:프라이드낮음 = 0
						TALENT:겁쟁이 = 0
						TALENT:억압 = 0
						
						TALENT:해방 = 1
						TALENT:아픔에강함 = 1
						TALENT:프라이드높음 = 1
					ELSE
						GOTO INPUT_LOOP2
					ENDIF
			ELSE
				GOTO INPUT_LOOP
			ENDIF
ENDIF

