@TAG_INIT(대상, 호출)
#DIM 대상
#DIM 호출
FOR LOCAL, 0, MAX_TAG
	TAG:대상:LOCAL = -1
NEXT
TRYCALLFORM INIT_TAG_{호출}(대상)


@ADD_TAG(대상, 태그)
#DIM 대상
#DIM 태그
FOR LOCAL, 0, MAX_TAG
	SIF TAG:대상:LOCAL == 태그
		RETURN 0
NEXT
FOR LOCAL, 0, MAX_TAG
	IF TAG:대상:LOCAL == -1
		TAG:대상:LOCAL = 태그
		BREAK
	ENDIF
NEXT

@REMOVE_TAG(대상, 태그)
#DIM 대상
#DIM 태그
FOR LOCAL, 0, MAX_TAG
	IF TAG:대상:LOCAL == 태그
		TAG:대상:LOCAL = -1
		BREAK
	ENDIF
NEXT

@INVERT_TAG(대상, 태그)
#DIM 대상
#DIM 태그
IF HAS_TAG(대상, 태그)
	CALL REMOVE_TAG(대상, 태그)
ELSE
	CALL ADD_TAG(대상, 태그)
ENDIF

@HAS_TAG(대상, 태그)
#FUNCTION
#DIM 대상
#DIM 태그
FOR LOCAL, 0, MAX_TAG
	SIF TAG:대상:LOCAL == 태그
		RETURNF 1
NEXT

RETURNF 0

@IS_TAG_EMPTY(대상)
#FUNCTION
#DIM 대상
FOR LOCAL, 0, MAX_TAG
	SIF TAG:대상:LOCAL != -1
		RETURNF 0
NEXT

RETURNF 1

