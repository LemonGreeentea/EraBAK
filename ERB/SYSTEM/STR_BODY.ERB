#FUNCTION
#DIM LCOUNT
#DIM 분할수

VARSET LOCALS

SPLIT ARGS, ":", LOCALS
분할수 = RESULT

FOR LCOUNT, 0, 분할수
	LOCALS:11 = %LOCALS:LCOUNT%
	SIF GETBIT(ARG:0, (NUM_STRBODY(LOCALS:11)))
