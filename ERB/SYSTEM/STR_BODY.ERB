		ELSEIF TALENT:ARG:매혹
			LOCALS:0 = %LOCALS:0% %SPLIT_R("매혹적인:아름다운")%
		ENDIF
	SIF !CHECK_STRBODY("수식", 속성)
		LOCALS:0 = %LOCALS:0%신체
	ENDIF
ENDSELECT
