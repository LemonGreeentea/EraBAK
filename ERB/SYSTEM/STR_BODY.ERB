		LOCALS:0 = %LOCALS:0% %SPLIT_R("민감한:예민함")%
	ENDIF
	SIF !CHECK_STRBODY("수식", 속성)
		LOCALS:0 = %LOCALS:0% %SPLIT_R("음핵:클리토리스")%
CASE "신체"	
	IF IS_MALE(ARG)
		SIF !CHECK_STRBODY("수식", 속성)
			LOCALS:0 = %부위%
	ELSE
		IF TALENT:(ARG:0):체격 == 체격_아이
			LOCALS:0 = %LOCALS:0% %SPLIT_R("작은:미숙한:어린:몸집이 작은")%
