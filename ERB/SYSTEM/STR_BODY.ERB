		LOCALS:0 = %LOCALS:0% %SPLIT_R("발정한:달아오른")%
	ELSEIF CHECK_STRBODY("욕정", 속성)
		LOCALS:0 = %LOCALS:0% %SPLIT_R("핑 선:충혈한:발기한:딱딱해진:부풀어 오른")%
