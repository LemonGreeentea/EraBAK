#DIM LCOUNT
#DIMS 부위후보, 15
#DIMS 부위
#DIM 부위수
#DIM 분할수

VARSET 부위후보

부위 = ""
부위수 = 0
VARSET LOCAL
VARSET LOCALS

SPLIT ARGS, ":", LOCALS
분할수 = RESULT

FOR LCOUNT, 0, 분할수
	LOCALS:11 = %LOCALS:LCOUNT%
	SELECTCASE LOCALS:11
		CASE "가슴", "유두" , "엉덩이", "질", "애널", "허리", "음순", "클리", "엉덩이살", "신체"
			부위후보:부위수 = %LOCALS:11%
			부위수 ++
	ENDSELECT
NEXT
SIF 부위수 > 0
	부위 = %부위후보:(RAND:부위수)%

RETURNF 부위



;--------------------------------------------------------
;@CHECK_STRBODY
;특정의 조건이 지정되고 있는지 체크한다
;--------------------------------------------------------
@CHECK_STRBODY(ARGS, ARG:0)
#FUNCTION
#DIM LCOUNT
#DIM 분할수

VARSET 분할수
VARSET LOCAL
VARSET LOCALS

SPLIT ARGS, ":", LOCALS
분할수 = RESULT
LOCAL:0 = 1
FOR LCOUNT, 0, 분할수
	LOCALS:11 = %LOCALS:LCOUNT%
	SIF !GETBIT(ARG:0, (NUM_STRBODY(LOCALS:11)))
		RETURNF 0
NEXT

RETURNF 1

