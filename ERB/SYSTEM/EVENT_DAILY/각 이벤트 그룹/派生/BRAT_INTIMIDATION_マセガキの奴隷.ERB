@EVENT_DAILY_DERIVATION_BRAT_AFTER_SETTARGET()
FOR LOCAL, 0, CHARANUM
	SIF !GETBIT(TALENT:LOCAL:데일리계, 소질_데일리_에로꼬맹이의노예)
		CONTINUE
	SIF !IS_FEMALE(LOCAL)
		CONTINUE
	SIF CFLAG:LOCAL:특수상태 != 0
		CONTINUE
	SIF CFLAG:LOCAL:포로처
		CONTINUE
	SIF RAND:3
		CONTINUE
	DAILY_TARGET:DAILY_TARGET_NUM = LOCAL
	DAILY_TARGET_NUM ++
NEXT

SIF DAILY_TARGET_NUM < 1
	RETURN 0
	
RETURN 1

