;---------------------
;발생 확률(1000분 율 100으로 10%)
;---------------------
@EVENT_DAILY_SUCCUBUS_DREAM_RATE()
RETURN 50

;---------------------
;확률 이외의 발생 판정
;---------------------
@EVENT_DAILY_SUCCUBUS_DREAM_DECISION()
#DIM 서큐버스
서큐버스 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:(특수세력_서큐버스))
SIF 서큐버스 == -1
	RETURN 0

RETURN 1

;---------------------
;장르
;---------------------
@EVENT_DAILY_SUCCUBUS_DREAM_GENRE()
RETURN 데일리_장르_특수세력


;---------------------
;특정의 조건을 채우는 캐릭터를 랜덤에 선택하는 경우에 이용
;다른 함수는 필수이지만, 이것만은 없어도 좋다  라고 할까 퍼포먼스에 영향을 주므로 불필요하면 만들어서는 안 된다
;대상이 존재하지 않고 데일리를 개시 할 수 없는 경우는 0을 돌려주는 것으로 데일리의 발생을 캔슬한다
;---------------------
@EVENT_DAILY_SUCCUBUS_DREAM_SETTARGET()
#DIM 서큐버스
서큐버스 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:특수세력_서큐버스)

FOR LOCAL, 0, CHARANUM
	;소속이 서큐버스가 아니고, 포로가 아니고, 여자.
	;MASTER가 아니고, 군주가 아니고, 동물이 아니고, 특수 캐릭터가 아니고, 특수 세력 캐릭터가 아니고, 죽지 않고, 미등장도 아니다
{
	IF CFLAG:(LOCAL):소속 != 서큐버스 && !CFLAG:(LOCAL):포로처 && IS_FEMALE(LOCAL) 
	&& LOCAL != MASTER && LOCAL != GET_COUNTRY_BOSS(CFLAG:LOCAL:소속) && !IS_ANIMAL(LOCAL) && !IS_SP_CHARA(LOCAL) && !TALENT:LOCAL:특수세력소질
	&& CFLAG:LOCAL:특수상태 != 특수상태_사망 && !ALLSAMES(CFLAG:LOCAL:특수상태, CFLAG:LOCAL:소속, 0)
}
		SIF CFLAG:LOCAL:소속 == CFLAG:MASTER:소속 && RAND:3
			CONTINUE
		DAILY_TARGET:DAILY_TARGET_NUM = LOCAL
		DAILY_TARGET_NUM ++
	ENDIF
NEXT
SIF DAILY_TARGET_NUM < 1
	RETURN 0

RETURN 1

;---------------------
;본체
;---------------------
@EVENT_DAILY_SUCCUBUS_DREAM
#DIM 대상
#DIM 서큐버스

대상 = DAILY_TARGET:(RAND:DAILY_TARGET_NUM)
서큐버스 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:특수세력_서큐버스)

PRINTFORML 서큐버스가 %조사처리(ANAME(대상),"를")% 동료로 할 수 있도록 눈을 붙인 것 같다
PRINTFORMW 그녀의 안의 성욕을 불러일으킬 수 있도록, 추잡한 꿈을 보이고 있다…
PRINTFORML 
SELECTCASE RAND:5
	CASE 0
		PRINTFORML %조사처리(ANAME(대상),"는")% 이상적인 체구의 남성에 안겨 미친듯이 자지러지는 신음소리를 내고 있다
		PRINTFORML 그에게 거느려지면서 씩씩한 페니스로 격렬하게 밀어올릴 수 있어 단정치 못하게 허덕인다
		PRINTFORML 격렬하게도 상냥한 섹스에 %조사처리(ANAME(대상),"는")% 헤롱헤롱 되어 있어암컷의 얼굴로 그에게 매달린다
		PRINTFORMW 조르는 것처럼 혀를 내밀면, 그도 교묘하게 얽히게 되어져 와 기분 좋은 나머지 머리가 멍-하니 한다
		PRINTFORML 사랑이 있는 키스 끼워에 자궁이 뜨끔뜨끔 쑤셔, 열중해 그에게 매달려 허리를 구불거리게 할 수 있다
		PRINTFORML 그의 씩씩한 페니스가 움직이는 것을을 질전체로 느끼면서 %조사처리(ANAME(대상),"는")% 여러 번 가볍게 절정에 이른다
		PRINTFORML %ANAME(대상)%의 관능의 고조됨에 맞추는 것처럼 그가 허리의 움직임을 조금씩 가속시켜 간다
		PRINTFORMW 그리고 그의 작열의 정액이 자궁에 발해지는 것과 동시에, %조사처리(ANAME(대상),"는")% 정신을 잃을 정도의 절정에 이르렀다
		PRINTFORMW %조사처리(ANAME(대상),"는")% 그와 그 뒤도 끝없이 키스 섹스를 계속하고 사랑과 육욕에 빠져 갔다…
	CASE 1
		PRINTFORML %조사처리(ANAME(대상),"는")% 거구의 오니에 밀어 쓰러트려지면서 격렬하게 범해지고 신음하고 있다
		PRINTFORML 인의 팔(정도)만큼도 있는 거대한 페니스를 쑤셔 넣어지고 질은 터질듯이 되어 있다
		PRINTFORML 그가허리를 흔드는 동안에 배가 보콕과 부풀어 올라, %조사처리(ANAME(대상),"는")% 조수를 뿜면서 흰색 눈을 벗겨 경련한다
		PRINTFORMW 하지만 꿈 속을 위해서(때문에), %조사처리(ANAME(대상),"는")% 고통은 느끼지 않고 피도 흐르지 않고 다만 폭력적인 쾌감이 전신을 뛰어 돌아다닌다
		PRINTFORML 성노예는 커녕 고기 오나홀과 같은 취급이지만, 개처럼 야무지지 못하게 혀를 늘어뜨린 아헤얼굴을 쬔다
		PRINTFORML 신체도 완전히 예의범절을 가르칠 수 있어 자궁의 안까지 넓히고 잘라 그의 페니스를 근원까지 물고 있다
		PRINTFORML 일발 찌르기마다 절정 해 동시에 이성이 파괴되어 섹스의 일밖에 생각할 수 없게 되어 간다
		PRINTFORMW 결과의 끝에, %조사처리(ANAME(대상),"는")% 스스로 허리를 구불거리게 할 수 있는 조르는 것처럼 그의 자식을 요구하는 것처럼 되어 있었다
		PRINTFORMW 그에게 마음에 들었던 %조사처리(ANAME(대상),"는")% 그 뒤도 오나홀로서 끝없이 마구 범해지는 일이 되었다…
	CASE 2
		PRINTFORML %조사처리(ANAME(대상),"는")% 육변기로서 목걸이를 붙여져 중년 아버지에게 길러지고 있다
		PRINTFORML 보기에도 욕망의 덩어리가 기름기가 돈 메타보 중년에게 범해지면서, %조사처리(ANAME(대상),"는")% 개처럼 기뻐하고 있다
		PRINTFORML %조사처리(ANAME(대상),"는")% 그의 씩씩한 절륜페니스와 끈적한 섹스에 조교되고 있는 것 같다
		PRINTFORMW 연일 연야 주인님에게 귀여워해져 사랑스러운 자식을 충분히 따라지는 것이 지상의 포상이다
		PRINTFORML 암컷구멍은 이미 주인님 딱 맞는 전용 페니스 케이스가 되어 있어, 상시밀을 늘어뜨리고 있다
		PRINTFORML %조사처리(ANAME(대상),"는")% 길러지는 일 사용되는 사범 되는 것이 여자의 기쁨이다라는 듯이 그에게 아양을 떤다
		PRINTFORML 어떤 부끄러운 플레이도 이미 포상이며, %조사처리(ANAME(대상),"는")% 그 삶의 방법에 아무 미혹도 없다
		PRINTFORMW 그리고 오늘도 또%조사처리(ANAME(대상),"는")% 주인님에 명령받는 대로 침대에 누워 가랑이를 열었다
		PRINTFORMW 주인님 전용의 자지암컷 펫으로서 깨어날 때까지 끝없이 귀여워해졌다…
	CASE 3
		PRINTFORML %조사처리(ANAME(대상),"는")% 연인의 남성과 전신을 얽히게 되어지면서의 섹스에 몰두하고 있다
		PRINTFORML 그의 손가락이 혀가 피부를 길 때에 행복감으로 가득 차 달콤한 한숨을 흘려 몽롱하게 한 표정이 된다
		PRINTFORML 용감한 페니스로 차분히 질내가 자극되면 시비레가 달려 오슬오슬 떨려 버린다
		PRINTFORMW 그에게 상냥하게 안기면서 나의 아이를 임신해 주라고 말해지고 %조사처리(ANAME(대상),"는")% 참지 못하고 절정 해 버렸다
		PRINTFORML 퓻퓻 하고 발해지는 그의 자식의 열을 태내에서 느끼면서 몇 번이나 절정을 반복해 간다
		PRINTFORML 질 내 되면서 혀를 격렬하게 들이마셔져 %조사처리(ANAME(대상),"는")% 정신나간 같은 기분이 되어 움찔움찔 경련했다
		PRINTFORML 사정이 끝난 뒤도 한동안 서로 둘이서 껴안아, 기분 좋은 여운에 계속 잠겼다
		PRINTFORMW 이윽고 다시 그가 허리를 움직이면 %ANAME(대상)%도 그에게 키스를 해 대답해, 다시 섹스에 몰두했다
		PRINTFORMW 사랑이 있는 섹스에 열중해, 그 이외을 생각할 수 없게 되어 갔다…
	CASE 4
		PRINTFORML %조사처리(ANAME(대상),"는")% 들요괴의 보금자리에서 둥지의 주인에게 격렬한 씨뿌리기 프레스를 받고 있다
		PRINTFORML 퍼억퍼억하고 자궁이 무너질 것 같을 정도의 피스톤을 받아 %조사처리(ANAME(대상),"는")% 손발을 흠칫거리게 해 신음한다
		PRINTFORML 그렇게 난폭한 취급에도 구애받지 않고 고통은 느끼지 않고, 일발 찌르기마다 강렬한 쾌락으로 시야가 새하얗게 된다
		PRINTFORMW 드뷰르룰! (와)과 힘차게 사정되면 쾌락은 정점으로 시달 참지 못하고 절정 당해 버렸다
		PRINTFORML 그는 %조사처리(ANAME(대상),"을")% 마음에 든 것 같아 그 뒤도 절륜요괴 자지로 귀여워해지고 계속하는 일이 된다
		PRINTFORML 한 번 절정한 %조사처리(ANAME(대상),"는")% 이미 쾌락에 저항하지 못하고, 스스로 허리를 구불거리게 할 수 있는 그를 받아들이고 몇 번이나 절정 한다
		PRINTFORML 그의 진심의 임신하게 한 씨뿌리기 프레스를 받은 %조사처리(ANAME(대상),"는")%, 필사적으로 매달려 환희의 소리를 높인다
		PRINTFORMW 몇 번째인가의 사정을 태내에 발해지면서 %조사처리(ANAME(대상),"는")% 맛본 적이 없는 암컷의 기쁨에 달했다
		PRINTFORMW 이미 %조사처리(ANAME(대상),"는")% 몸도 마음도 그의 아내 혹은 성노예와 완성되어 내려감 쾌락에 빠져 갔다…
ENDSELECT
CALL FUCK(대상, "욕망, 욕망, 봉사, 성기술, 성교, 정애, Ｃ, Ｖ, Ｂ, Ｍ, Ｖ성교, 키스, 펠라티오", "", 0, "", "", "")
PRINTFORML 
IF RAND:5 + ABL:대상:욕망 >= 5
	PRINTFORMW %조사처리(ANAME(대상),"는")% 서큐버스로부터 주어지는 쾌락의 포로가 되었다
	CALL COLOR_PRINTW("그녀는 보여진 꿈을 잊을 수 있지 못하고 서큐버스 세력에 내려 버렸다…", 칼라_핑크)
	;CFLAG:대상:소속 =서큐버스
	CALL CHANGE_COUNTRY(대상, 서큐버스)
ELSE
	PRINTFORMW %조사처리(ANAME(대상),"는")% 어떻게든 서큐버스의 유혹으로부터 피한 것 같다
ENDIF

RETURN 1
