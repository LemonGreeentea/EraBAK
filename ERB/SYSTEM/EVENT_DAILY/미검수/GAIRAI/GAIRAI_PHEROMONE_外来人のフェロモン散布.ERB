;---------------------
;기본적인 발생 확률(1000분 율 100으로 10%)
;이것에 콘피그 항목의 발생 빈도를 걸칠 수 있으므로, 반드시 이 대로는 되지 않는다
;---------------------
@EVENT_DAILY_GAIRAI_PHEROMONE_RATE()
RETURN (DVAR:페로몬중독_발생회수 > 0 ? 200 # 50)


;---------------------
;확률 이외의 발생 판정
;0기 이후가 되면 발생한다든가, 데일리측에서 이용하고 있는 변수가-1이라면 일으키지 않는다  같은 튕기는 방법을 할 경우에 사용한다
;---------------------
@EVENT_DAILY_GAIRAI_PHEROMONE_DECISION()
#DIM 외래인
외래인 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:특수세력_외래인)
SIF 외래인 == -1
	RETURN 0

RETURN 1

;---------------------
;장르
;콘피그 설정으로 사용
;---------------------
@EVENT_DAILY_GAIRAI_PHEROMONE_GENRE()
RETURN 데일리_장르_특수세력

;---------------------
;특정의 조건을 채우는 캐릭터를 랜덤에 선택하는 경우에 이용
;다른 함수는 필수이지만, 이것만은 없어도 좋다  라고 할까 퍼포먼스에 영향을 주므로 불필요하면 만들어서는 안 된다
;대상이 존재하지 않고 데일리를 개시 할 수 없는 경우는 0을 돌려주는 것으로 데일리의 발생을 캔슬한다
;---------------------
@EVENT_DAILY_GAIRAI_PHEROMONE_SETTARGET()
#DIM 대상
#DIM 외래인
외래인 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:특수세력_외래인)
대상 = ID_TO_CHARA(DVAR:페로몬중독_대상ID)

;전회의 발생으로부터의 상태변화를 본다
IF 대상 != -1
	;군주가 되어 있다면 해제
	IF 대상 == GET_COUNTRY_BOSS(CFLAG:대상:소속)
		DVAR:페로몬중독_대상ID = 0
		RETURN 0
	ENDIF

	;외래인 소속이 되고 있다면 해제
	IF CFLAG:대상:소속 == 외래인
		DVAR:페로몬중독_대상ID = 0
		RETURN 0
	ENDIF
	
	;죽어 있으면 되돌린다
	IF CFLAG:대상:특수상태 == 특수상태_사망
		DVAR:페로몬중독_대상ID = 0
		RETURN 0
	ENDIF
	
	;미등장
	IF ALLSAMES(CFLAG:대상:특수상태, CFLAG:대상:소속, 0)
		DVAR:페로몬중독_대상ID = 0
		RETURN 0
	ENDIF
	
	;여자를 그만두었다면 해제
	IF !IS_FEMALE(대상)
		DVAR:페로몬중독_대상ID = 0
		RETURN 0
	ENDIF

	;포로가 되어 있다면 되돌린다
	SIF CFLAG:대상:포로처
		RETURN 0
;대상이 없으면 새롭게 설정한다
ELSE
	FOR LOCAL, 0, CHARANUM
		;소속이 외래인이 아니고, 포로가 아니고, 여자.
		;MASTER가 아니고, 군주가 아니고, 동물이 아니고, 특수 캐릭터가 아니고, 특수 세력 캐릭터가 아니고, 죽지 않고, 미등장도 아니다
{
		IF CFLAG:(LOCAL):소속 != 외래인 && !CFLAG:(LOCAL):포로처 && IS_FEMALE(LOCAL) 
		&& LOCAL != MASTER && LOCAL != GET_COUNTRY_BOSS(CFLAG:LOCAL:소속) && !IS_ANIMAL(LOCAL) && !IS_SP_CHARA(LOCAL) && !TALENT:LOCAL:특수세력소질
		&& CFLAG:LOCAL:특수상태 != 특수상태_사망 && !ALLSAMES(CFLAG:LOCAL:특수상태, CFLAG:LOCAL:소속, 0)
}
			SIF CFLAG:LOCAL:소속 == CFLAG:MASTER:소속 && RAND:3
				CONTINUE
			DAILY_TARGET:DAILY_TARGET_NUM = LOCAL
			DAILY_TARGET_NUM ++
		ENDIF
	NEXT
	SIF DAILY_TARGET_NUM < 1
		RETURN 0
	DVAR:페로몬중독_대상ID = GET_ID(DAILY_TARGET:(RAND:DAILY_TARGET_NUM))
ENDIF

RETURN 1

;---------------------
;본체
;이벤트가 발생했을 경우는 1, 발생하지 않았던 경우는 0을 돌려준다
;발생하지 않았다는 것은 예를 들면, 특정 조건을 채우는 캐릭터로부터 랜덤에 한사람 선택하는 데일리로, 원래 그 조건을 채우는 캐릭터가 한사람도 없는 것 같은 때
;구사양으로 만든 적 있는 사람용으로 말하면 「구사양의 데일리 본체 첫머리에서-1을 돌려주는 것 같은 상황」
;---------------------
@EVENT_DAILY_GAIRAI_PHEROMONE()
#DIM 대상
#DIM 외래인
#DIM 후보캐릭터, 3000
#DIM 후보수
#DIM 세력번호

외래인 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:(특수세력_외래인))
대상 = ID_TO_CHARA(DVAR:페로몬중독_대상ID)

IF DVAR:페로몬중독_발생회수 == 0
	PRINTFORML 외래인이 성노 조교용의 기계를 개발 했다
	PRINTFORML 여자를 끌어 들이는 페로몬을 발생시킨다
	PRINTFORML 조속히 실험을 실시한 결과, %조사처리(ANAME(대상),"를")% 끌어 들일 수 있었다
	PRINTFORML 그 표정은 분명하게 제정신은 아니고, 발정한 것 같은 용모를 보이고 있다
	PRINTFORML %조사처리(ANAME(대상),"는")% 야무지지 못하게 혀를 늘어뜨리면서 비밀스런 장소를 만지작거려 돌리면서 휘청휘청 외래인의 바탕으로 다가간다
	PRINTFORMW 실험 성공을 확신한 그는 만족한 것 같게 웃으면, %조사처리(ANAME(대상),"를")% 그늘로 데리고 들어갔다
ELSE
	PRINTFORML 다시 페로몬 발생 장치를 사용했다
	PRINTFORML 이번은 페로몬의 중독성을 확인하기 위한 실험이다
	PRINTFORML 한동안 기다리고 있으면, 휘청휘청%조사처리(ANAME(대상),"가")% 왔다
	PRINTFORML 그 표정은 역시 발정해 버리고 있어 기대로 가득 차 있다
	PRINTFORMW 외래인은 %ANAME(대상)%의 모습에 실험의 성공을 확신해 만족한 것 같게 웃으면, 그늘로 데리고 들어갔다
ENDIF
PRINTFORML 
SELECTCASE RAND:20
	CASE 0
		PRINTFORML %조사처리(ANAME(대상),"는")% 네발로 엎드림의 모습으로 외래인에 범해지고 있다
		PRINTFORML 외래인이 난폭하게 허리를 쳐박을 때에, 고기가 서로 치는 소리와 함께%ANAME(대상)%의 교성이 리드미컬하게 영향을 준다
		PRINTFORML 격렬한 피스톤에 자궁의 욱신거림이 인내 할 수 없게 된 %조사처리(ANAME(대상),"는")%, 응석부리는 것처럼 씨뿌리기를 조르기 하기 시작한다
		PRINTFORMW 그가 거기에 응하는 것처럼 그녀 안쪽 깊고로 사정하면, %조사처리(ANAME(대상),"는")% 기쁨의 소리를 높여 절정 했다
	CASE 1
		PRINTFORML %조사처리(ANAME(대상),"는")% 개와 같은 모습으로 외래인에 범해지고 있다
		PRINTFORML 그에게 덮어씌워지면서 격렬하게 페니스를 쑤셔 넣어져 %조사처리(ANAME(대상),"는")% 기쁨의 소리를 높여 몸을 진동시킨다
		PRINTFORML 일발 찌르기마다 정수리까지 저리는 것 같은 쾌락이 %조사처리(ANAME(대상),"를")% 덮쳐, 넘어질 것 같게 되는 것을 떨리는 팔로 어떻게든 견딘다
		PRINTFORMW 짐승과 같이 격렬한 교미는 해가 질 때까지 계속되어, %조사처리(ANAME(대상),"는")% 몇 번이나 씨뿌리기 절정을 맛보는 일이 되었다
	CASE 2
		PRINTFORML %조사처리(ANAME(대상),"는")% 외래인에 밀어 쓰러트려져 난무에 페니스를 쑤셔 넣어지고 있다
		PRINTFORML 자궁에 영향을 주는 것 같은 격렬한 스트로크에, %조사처리(ANAME(대상),"는")% 야무지지 못한 표정으로 히익히익 거리며 허덕이고 있다
		PRINTFORML 용감한 삽입질에 몸은 완전히 굴복 하고 있어, 자식을 조르는 것처럼 질육을 실룩거리게 해 페니스를 자극한다
		PRINTFORMW 이윽고 그의 정액이 힘차게 발해지면, %조사처리(ANAME(대상),"는")% 단정치 못한 소리와 함께 몸을 뒤로 젖히게 했다
	CASE 3
		PRINTFORML %조사처리(ANAME(대상),"는")% 굴곡위의 자세로 격렬하게 페니스를 쳐박아지고 있다
		PRINTFORML 대담한 페니스로 질육을 빠득빠득 후벼파지면서, %조사처리(ANAME(대상),"는")% 눈물을 흘려 기쁨의 소리를 높인다
		PRINTFORML 갑자기 즌! (와)과 한층 더 깊이 쳐박아지면, %조사처리(ANAME(대상),"는")% 머리가 새하얗게 되어 절정 해 버렸다
		PRINTFORMW 완전히 육체가 지배된 %조사처리(ANAME(대상),"는")%, 그 뒤도 그에게 범해지는 대로 미친듯이 자지러지는 신음소리를 냈다
	CASE 4
		PRINTFORML %조사처리(ANAME(대상),"는")% 외래인에 밀어 쓰러트려ㅋ瓚㎎? 범해지고 있다
		PRINTFORML 그 표정은 기쁨으로 가득 차 있어 떨리는 손발로 그에게 매달려 요염하게 허덕이고 있다
		PRINTFORML 추잡한 미츠네와 함께 페니스가 출입할 때에, %ANAME(대상)%의 질은 자식을 요구해 꿈틀거림 페니스에 휘감긴다
		PRINTFORMW 그 억압에 그가 참지 못하고 질내사정 하면, %조사처리(ANAME(대상),"는")% 아헤얼굴을 쬐어 절정 했다
	CASE 5
		PRINTFORML %조사처리(ANAME(대상),"는")%전라에 벗겨져 난폭하게 범해지고 있다
		PRINTFORML 그의 페니스를 깊숙히 받아들이면서, %조사처리(ANAME(대상),"는")% 유우를 부르는 것 같은 음미로운 표정으로 혀를 얽히게 되어지고 있다
		PRINTFORML 마치 창녀와 같은 %ANAME(대상)%의 추잡한 행동에, 외래인의 흥분도 가속해 더욱 격렬하게 허리를 쳐박는다
		PRINTFORMW %조사처리(ANAME(대상),"는")% 격렬한 피스톤에 더욱 더 기쁨몸을 구불거리게 되어지면서, 절정에 올라 갔다
	CASE 6
		PRINTFORML 외래인의 매우 굵은의 페니스로 격렬하게 보르치오를 찔러져 %조사처리(ANAME(대상),"는")% 절정 했다
		PRINTFORML 그의 육욕대로 끝없이 범해지고 계속된 %조사처리(ANAME(대상),"는")%, 완전히 암컷의 얼굴을 하면서 움찔움찔 경련한다
		PRINTFORML 절정의 연속으로 머리를 정신나가게 하면서도, 그 미육은 힘들게 페니스를 단단히 조이고 한층 더 쾌락을 추구하고 있다
		PRINTFORMW 그 뒤도 두 명은 시간을 잊어 계속 교접해, %조사처리(ANAME(대상),"는")% 몇 번이나 그의 정액을 받아 들였다
	CASE 7
		PRINTFORML %조사처리(ANAME(대상),"는")% 외래인 위에서 야무지지 못한 표정으로 허리를 흔들고 있다
		PRINTFORML 페니스가 출입할 때에 질육이 후벼파져, %조사처리(ANAME(대상),"는")% 견딜 수 없다고 한 소리를 높여 몸을 구불거리게 할 수 있다
		PRINTFORML 갑자기 허리를 밀어올릴 수 있어 자궁을 킥 되는 것 같은 충격이 달려, %조사처리(ANAME(대상),"는")% 크게 키를 뒤로 젖히게 했다
		PRINTFORMW %조사처리(ANAME(대상),"는")% 쾌락으로 머리를 새하얗게 해 경련하면서, 눈을♥로 해 기쁨의 말을 짜냈다
	CASE 8
		PRINTFORML %조사처리(ANAME(대상),"는")% 지면에 밀어 쓰러트려져 격렬하게 페니스를 쑤셔 넣어지고 있다
		PRINTFORML 그의 정열적인 섹스로 몇 번이나 절정 당한 %조사처리(ANAME(대상),"는")%, 몸에 힘이 들어가지 않고 되는 대로 되어 있다
		PRINTFORML 절정 해 민감하게 된 미육을 고리고와 후벼파지면, %조사처리(ANAME(대상),"는")% 망가진 인형과 같이 미친듯이 자지러지는 신음소리를 낸다
		PRINTFORMW 하지만 그의 공격은 전혀 쇠약해지지 않고, %조사처리(ANAME(대상),"는")% 그 뒤도 심하게 괴롭혀져 버렸다
	CASE 9
		PRINTFORML %조사처리(ANAME(대상),"는")% 외래인에 걸치면서 추잡하게 신체를 구불거리게 되어지고 있다
		PRINTFORML 꿀단지를 삐걱삐걱 넓혀 매우 굵은의 페니스를 물어, 그 씩씩함에 %조사처리(ANAME(대상),"는")% 기쁨의 표정을 보여 허덕인다
		PRINTFORML 외래인은 %ANAME(대상)%의 봉사에 만족하면서, 그 음미로움을 칭찬하면, 포상의 씨뿌리기를 예고한다
		PRINTFORMW 그 말에 %조사처리(ANAME(대상),"는")% 기쁨으로 자궁을 쑤시게 해 포상을 받으려고 한층 더 격렬하게 봉사를 계속했다
	CASE 10
		PRINTFORML %조사처리(ANAME(대상),"는")% 비밀스런 장소로부터 정액을 흘러넘치게 하면서 바닥에 가로놓여 있다
		PRINTFORML 심하게 범해졌음에도 관련되지 않고 그 표정은 기쁨으로 가득 차 있어 숨을 거칠게 하면서 때때로 움찔움찔 떨고 있다
		PRINTFORML 충분히 질 내 된 하복부를 외래인에 상냥하게 어루만질 수 있으면, %조사처리(ANAME(대상),"는")% 몸부림을 해 또 달했다
		PRINTFORMW 자궁의 욱신거림이 멈추지 않는 %조사처리(ANAME(대상),"는")% 보지를 손가락으로 다치고 열면 다음을 조르기 하기 시작했다
	CASE 11
		PRINTFORML %조사처리(ANAME(대상),"는")% 한쪽 발을 움켜 쥐어지면서 격렬하게 밀어올려지고 있다
		PRINTFORML 일발 찌르기마다 자궁에 영향을 주는 것 같은 몹시 거친 피스톤에, %조사처리(ANAME(대상),"는")% 단정치 못한 소리를 높이고 기뻐한다
		PRINTFORML 쾌락인 나머지 신체가 삐걱삐걱 떨려 붕괴될 것 같게 되는 것을, 벽에 의지하고 어떻게든 견디고 있다
		PRINTFORMW 그런 그녀에게 상관하는 일 없이 공격을 격렬함을 늘려, %ANAME(대상)%도 히익히익 거리며 허덕이면서 그것을 받아 들였다
