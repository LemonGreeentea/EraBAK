;---------------------
;기본적인 발생 확률(1000분 율 100으로 10%)
;이것에 콘피그 항목의 발생 빈도를 걸칠 수 있으므로, 반드시 이 대로는 되지 않는다
;---------------------
@EVENT_DAILY_GAIRAI_EXPERIMENT_RATE()
RETURN 40


;---------------------
;확률 이외의 발생 판정
;0기 이후가 되면 발생한다든가, 데일리측에서 이용하고 있는 변수가-1이라면 일으키지 않는다  같은 튕기는 방법을 할 경우에 사용한다
;---------------------
@EVENT_DAILY_GAIRAI_EXPERIMENT_DECISION()
#DIM 외래인

외래인 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:(특수세력_외래인))
SIF 외래인 == -1 || 외래인 == CFLAG:MASTER:소속
	RETURN 0

RETURN 1

;---------------------
;장르
;콘피그 설정으로 사용
;---------------------
@EVENT_DAILY_GAIRAI_EXPERIMENT_GENRE()
RETURN 데일리_장르_특수세력

;---------------------
;본체
;이벤트가 발생했을 경우는 1, 발생하지 않았던 경우는 0을 돌려준다
;발생하지 않았다는 것은 예를 들면, 특정 조건을 채우는 캐릭터로부터 랜덤에 한사람 선택하는 데일리로, 원래 그 조건을 채우는 캐릭터가 한사람도 없는 것 같은 때
;구사양으로 만든 적 있는 사람용으로 말하면 「구사양의 데일리 본체 첫머리에서-1을 돌려주는 것 같은 상황」
;---------------------
@EVENT_DAILY_GAIRAI_EXPERIMENT()
#DIM 대상
#DIM 외래인
#DIM 세력번호

외래인 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:(특수세력_외래인))

PRINTFORML 외래인의 실험 시설의 하나를 찾아냈다
PRINTFORML 가까이의 마을사람들을 휩쓸어서는 비도인 실험을 실시하고 있는 것 같다
PRINTFORMW 어떻게 하지?
CALL ASK_YN("잠입한다", "그만둔다")
IF RESULT == 1
	PRINTFORMW 그만두었다
ELSE
	PRINTFORML 발해 둘 수 없다
	PRINTFORML 잠입해 시설을 파괴하기로 했다
	PRINTFORMW 누구를 잠입시킬까?
	CALL SINGLE_DRAWLINE
	CALL SELECT_CHARA_LIST_ONLY_LOGIC_SEX("GAIRAI_EXPERIMENT", "GAIRAI_EXPERIMENT")
	대상 = RESULT
	IF 대상 == -1
		PRINTFORML ……아니, 역시 위험함
		PRINTFORMW 그만두기로 했다
		RETURN -1
	ELSE
		IF 대상 == MASTER
			PRINTFORMW %ANAME(대상)%스스로 잠입하기로 했다
		ELSE
			PRINTFORMW %조사처리(ANAME(대상),"를")% 잠입시키기로 했다
		ENDIF
		PRINTFORML ·
		PRINTFORML ·
		PRINTFORMW ·
		IF (ABL:대상:무투 + ABL:대상:지략) / 10 > RAND:10 + 11
			PRINTFORML 했다!
			PRINTFORMW 순조롭게 잠입에 성공한 %조사처리(ANAME(대상),"는")% 그 시설을 파괴하는 것을 성공했다
			FOR LOCAL, 1, MAX_COUNTRY
				SIF LOCAL != 외래인 && IS_COUNTRY(LOCAL)
					CALL CHANGE_RELATION_C_TO_C(LOCAL, CFLAG:MASTER:1, 100, -100)
			NEXT
			CALL COLOR_PRINT(@"이 성과에 타세력의 평가가 올랐다", 칼라_주의)
			PRINTFORMW 
			CALL CHANGE_RELATION_C_TO_C(외래인, CFLAG:MASTER:1, -500, 500)
			CALL COLOR_PRINT(@"하지만 외래 사람들의 눈엣가시로 되어 버렸다", 칼라_경고)
			PRINTFORMW 
		ELSE
			PRINTFORML 끝냈다!
			PRINTFORML 작전에 실패한 %조사처리(ANAME(대상),"는")% 잡혀 버렸다
			PRINTFORMW 외래 사람들은 이 침입자를 새로운 발명품의 실험대로 하기로 했다
			CALL GAIRAI_SPY_RAPE(대상)
			PRINTFORML 
			PRINTFORMW 심하게 범해진 후, %조사처리(ANAME(대상),"는")% 어떻게든 도망치기 시작할 수 있었다
			FOR LOCAL, 0, CHARANUM
				IF CFLAG:(LOCAL):소속 == 외래인 && !IS_ANIMAL(LOCAL) && TALENT:LOCAL:특수세력소질 == 특수세력_외래인
					ABL:LOCAL:무투 += (RAND:3 + 1)
					ABL:LOCAL:지략 += (RAND:3 + 1)
					ABL:LOCAL:정치 += (RAND:3 + 1)
				ENDIF
			NEXT
			CALL COLOR_PRINT(@"하지만 실험의 데이터에 의해 그들의 세력이 강화되어 버렸다", 칼라_핑크)
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF

RETURN 1

