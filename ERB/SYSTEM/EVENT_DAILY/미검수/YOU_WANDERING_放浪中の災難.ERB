;---------------------
;기본적인 발생 확률(1000분 율 100으로 10%)
;이것에 콘피그 항목의 발생 빈도를 걸칠 수 있으므로, 반드시 이 대로는 되지 않는다
;---------------------
@EVENT_DAILY_YOU_WANDERING_RATE()
RETURN 100

;---------------------
;확률 이외의 발생 판정
;0기 이후가 되면 발생한다든가, 데일리측에서 이용하고 있는 변수가-1이라면 일으키지 않는다  같은 튕기는 방법을 할 경우에 사용한다
;---------------------
@EVENT_DAILY_YOU_WANDERING_DECISION()
;미검수 OFF면 스킵
SIF FLAG:데일리이벤트_미검수 == 0
	RETURN 0
	
SIF CFLAG:MASTER:소속 != 0 || CFLAG:MASTER:포로처 != 0
	RETURN 0

SIF !IS_FEMALE(MASTER)
	RETURN 0

SIF MAX(ABL:MASTER:무투, ABL:MASTER:지략, ABL:MASTER:정치, ABL:MASTER:방위) >= 70 || ABL:MASTER:무투 + ABL:MASTER:지략 + ABL:MASTER:정치 + ABL:MASTER:방위 >= 240
	RETURN 0

RETURN 1

;---------------------
;장르
;콘피그 설정으로 사용
;---------------------
@EVENT_DAILY_YOU_WANDERING_GENRE()
RETURN 데일리_장르_에로

;---------------------
;본체
;이벤트가 발생했을 경우는 1, 발생하지 않았던 경우는 0을 돌려준다
;발생하지 않았다는 것은 예를 들면, 특정 조건을 채우는 캐릭터로부터 랜덤에 한사람 선택하는 데일리로, 원래 그 조건을 채우는 캐릭터가 한사람도 없는 것 같은 때
;구사양으로 만든 적 있는 사람용으로 말하면 「구사양의 데일리 본체 첫머리에서-1을 돌려주는 것 같은 상황」
;---------------------
@EVENT_DAILY_YOU_WANDERING()
#DIM 도둑

도둑 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:(특수세력_도둑))

SELECTCASE RAND:30
	;짐승에게 음식을 조르는 & MONEY < 100
	CASE 0
		PRINTFORML 여행의 도중, 배고픔으로 어쩔 수 없게 되어 뒷골목의 한쪽 구석에 쓰러져 버렸다
		PRINTFORML 하면 떠돌이 개가 어딘가로부터 훔쳐 온 빵을 탐내고 있는데 조우했다
		PRINTFORMW 지금의 %ANAME(MASTER)%에 있어서는 그것조차도 맛있는 음식으로 보여, 무심코 배가 울어 버린다
		PRINTFORML 하면 그 소리를 들은 떠돌이 개가 이쪽을 되돌아 보았다
		PRINTFORML 근골 융성하게 한 보기에도 흉포할 것 같은 수캐다
		PRINTFORMW 어떻게 하지?
		CALL ASK_YN("아첨해 음식을 조른다", "힘으로 음식을 빼앗는다")
		IF RESULT == 1
			PRINTFORMW %조사처리(ANAME(MASTER),"는")% 힘으로 빼앗으려고 했다
			PRINTFORML ·
			PRINTFORML ·
			PRINTFORMW ·
			PRINTFORML 하지만 이길 수 없었다
			PRINTFORML 너덜너덜로 되어 구르는 %ANAME(MASTER)%에 대해, 수캐는 콧김을 거칠게 해 냄새를 맡아 왔다
			PRINTFORMW 공포로 떨려 본능적으로 복종의 포즈를 취하고 있으면, 그는 와온! (와)과 울어 덮어씌워 왔다
		ELSE
			PRINTFORML 지나친 배고픔에 %조사처리(ANAME(MASTER),"는")% 프라이드를 버려 엎드린 자세로 되었다
			PRINTFORML 그는 경계하는 것 같은 눈초리로 이쪽을 노려봐 온다
			PRINTFORMW %조사처리(ANAME(MASTER),"는")% 아첨하도록(듯이) 목을 울리면, 벌렁 굴러 복종의 포즈를 취했다
			PRINTFORML 구응구응과 울고 있으면 그는 콧김을 거칠게 하면서 천천히 가까워져, 냄새를 맡아 왔다
			PRINTFORMW 공포로 떨리면서 복종의 포즈를 계속하고 있으면, 그는 와온! (와)과 울어 덮어씌워 왔다
		ENDIF
		PRINTFORML %조사처리(ANAME(MASTER),"는")% 떠돌이 개 덮어씌워져 격렬하게 범해지면서, 아헤아헤와 허덕이고 있다
		PRINTFORML 그는 %ANAME(MASTER)%의 신체를 마음에 든 것 같아, 일심 불란에 허리를 흔들어 페니스를 쳐박아 온다
		PRINTFORML 짐승에 범해지는 굴욕을 느끼면서도, 그 용감한 허리 사용은 가부간의 대답 없고 %조사처리(ANAME(MASTER),"를")% 암컷으로서 눈을 뜨게 한다
		PRINTFORML 갑자기 쿵! (와)과 귀두로 마음껏 자궁을 찔러져 무심코 신체를 뛰게 해 교성을 올려 버렸다
		PRINTFORMW 소와 꼭 죄이는 질육에 떠돌이 개가 크게 짖으면, 대량의 정액이 %ANAME(MASTER)%의 태내에 풀어졌다
		CALL FUCK(MASTER, "욕망, 성기술, 성교, Ｃ, Ｖ, 수간, Ｖ성교", "처녀상실, 질내사정", GET_SPERM_ID("개"), @"떠돌이 개의 페니스", @"떠돌이 개", "", "강간")
		PRINTFORML 
		PRINTFORML 그 후, 만족한 들개가 없어지면
		PRINTFORMW %조사처리(ANAME(MASTER),"는")% 휘청휘청 일어나, 울면서 개의 먹던 것의 빵을 가득 넣었다
	;아귀에게 습격당한다
	CASE 1
		PRINTFORMW 방랑중, 지친 %조사처리(ANAME(MASTER),"가")% 인기척이 없는 빈집으로 쉬고 있으면 아귀에게 조우했다
		PRINTFORML 그는 %조사처리(ANAME(MASTER),"를")% 보면 날카로운 소리를 높여 덤벼 들어 왔다!
		CALL ASK_MULTI("응전한다" ,"도주한다" ,"설득한다")
		IF RESULT == 2
			PRINTFORMW 설득을 시도했다!
		ELSEIF RESULT == 1
			PRINTFORMW 도주를 시도했다!
		ELSE
			PRINTFORMW 응전했다!
		ENDIF
		PRINTFORML ·
		PRINTFORML ·
		PRINTFORMW ·
		PRINTFORML 하지만 실패해 버렸다…
		PRINTFORMW 아귀는 잡은 %조사처리(ANAME(MASTER),"를")% 난폭하게 쓰러트리고 옷을 벗겨 왔다
		PRINTFORML 
		PRINTFORML 빈 집안에 고기가 서로 치는 소리와 %조사처리(ANAME(MASTER),"가")% 흐려진 신음소리가 울린다
		PRINTFORML 그는 %ANAME(MASTER)%의 신체에 열중해 달라붙으면서 크게 허리를 그라인드 시켜 공격해대어 온다
		PRINTFORML 어떻게든 떼어내려고 해도, 그는 %ANAME(MASTER)%의 신체에 힘들게 매달려 떼어 놓으려고 하지 않는다
		PRINTFORML 그 끈질긴 애무와 격렬한 피스톤에, 점차 인내 할 수 없게 되어 신체를 구불거리게 할 수 있는 선꾸중이고라고 끝낸다
		PRINTFORMW 이윽고 그가 한층 더 깊게 페니스를 쑤셔 넣어 정액을 발하면, %조사처리(ANAME(MASTER),"는")% 단정치 못하게 절정 당해 버렸다
		CALL FUCK(MASTER, "욕망, 성기술, 성교, 사랑, Ｃ, Ｖ, Ｂ, Ｍ, Ａ, Ｖ성교, Ａ성교", "처녀상실, 질내사정, Ａ처녀상실, 장내사정, 키스상실", GET_SPERM_ID("들요괴"), @"아귀의 \@RAND:2 ? 페니스 # 입술\@", @"아귀", "", "강간")
		PRINTFORML 
		PRINTFORML 아귀는 심하게 %조사처리(ANAME(MASTER),"를")% 범해진 후, 만족해 떠나 갔다
		PRINTFORMW 뒤에는 전신을 더럽혀지고 어안이 벙벙히 한 채로 늘어진 %조사처리(ANAME(MASTER),"가")% 남겨졌다…
	;불합리한 조사
	CASE 2
		PRINTFORML 방랑중의 어떤마을에서, 병사에 불러 세울 수 있었다
		PRINTFORML 테러를 경계해 의심스러운 인물이 아닌가 조사한다고 한다
		PRINTFORML 그들은 힐쭉거리면서 %조사처리(ANAME(MASTER),"를")% 핥고 돌리도록(듯이) 바라봐 온다
		PRINTFORML 기분의 나쁨을 느껴 위축 하고 있으면 군사의 한사람이 갑자기%ANAME(MASTER)%의 팔을 잡아 왔다
		PRINTFORMW %조사처리(ANAME(MASTER),"가")% 놀라고 있으면 그들은, 이상하기 때문에 좀 더 자주(잘) 조사한다고 고해, 억지로 대기소로 연행되었다
		PRINTFORML 
		PRINTFORML 대기소의 안쪽에 데리고 들어가진 %조사처리(ANAME(MASTER),"가")%, 병사들에게 윤간되고 있다
		PRINTFORML 도망치려고 해서는 밀어 쓰러트려지고 맞아 벌로서 범해지고 울부짖는 %ANAME(MASTER)% 안에 정액이 따라진다
		PRINTFORMW 그 반복에 완전히 얌전해진 %조사처리(ANAME(MASTER),"는")%, 지금 울면서도 온순하게 그들에게 봉사하고 있다
		PRINTFORML 지금도 또한 한사람의 병사에 걸쳐 허리를 흔들면서, 입이나 손을 사용해 다른 페니스를 계속 열심히 훑어 내고 있다
		PRINTFORML 질내사정 되어도 물론 도망치는 것은 용서되지 않고, 경련이 일어난 웃는 얼굴로 한 방울 남김없이 받아 들이게 되어졌다
		PRINTFORMW %ANAME(MASTER)%의 육체는 호평으로, 그들의 조사는 밤새도록 계속되어 정액으로 질척질척으로 되어 버렸다
		CALL FUCK(MASTER, "욕망, 성기술, 성교, 봉사, 사랑, Ｃ, Ｖ, Ｂ, Ｍ, Ａ, 윤간, 펠라티오, Ｖ성교, Ａ성교", "처녀상실, 질내사정, Ａ처녀상실, 장내사정", GET_SPERM_ID("병사"), @"병사의 \@RAND:2 ? 페니스 # 입술\@", @"병사", "", "윤간")
		PRINTFORML 
		PRINTFORMW %조사처리(ANAME(MASTER),"는")% 밤새도록 조사할 수 있던 후, 간신히 무일푼으로 내던져졌다
		MONEY = 0
	;도둑에게 습격당한다
	CASE 3
		PRINTFORMW 산길을 걷고 있으면, 주위로부터 누군가의 기색을 느꼈다
		PRINTFORML 어떻게 하지?
		CALL ASK_MULTI("모습을 묻는다" ,"주위에 말을 건다" ,"빠른 걸음을 떠난다")
		IF RESULT == 2
			PRINTFORML 싫은 예감이 든 %조사처리(ANAME(MASTER),"는")% 쏜살같이 그 자리를 달리기 시작했다
			PRINTFORML 하면 주위의 수풀로부터 사람의 그림자가 나타나, %ANAME(MASTER)%의 전방을 막아 왔다
			PRINTFORMW 당황해 뒤꿈치를 돌려주려고 하는 것도, 한층 더 복수의 사람의 그림자에 둘러싸여 버렸다
		ELSEIF RESULT == 1
			PRINTFORML 누군가가 있는지도 모른다, 그렇게 생각한 %조사처리(ANAME(MASTER),"는")% 주위에 말을 걸었다
			PRINTFORML 하면 주위의 수풀로부터 복수의 사람의 그림자가 나타나는…도둑의 집단이다!
			PRINTFORMW 당황해 도망치려고 했지만 때는 이미 늦고, %조사처리(ANAME(MASTER),"는")% 둘러싸여 버렸다
		ELSE
			PRINTFORML 짐승인가 뭔가가 있을까
			PRINTFORML 함부로 움직이지 않고 경계하고 있으면, 돌연 배후에서 입을 채워졌다!
			PRINTFORML 발버둥 쳐 도망치려고 하는 것도, 그것도 상관하지 않고 그 자리에 억눌러져 버렸다
			PRINTFORMW 하면 주위의 수풀로부터 남자 동료다운 집단이 모습을 나타냈다…
		ENDIF
		PRINTFORMW 도둑들은 히죽히죽 웃으면서, 저항하는 %조사처리(ANAME(MASTER),"를")% 솜씨 좋게 단단히 묶으면 어딘가에 데리고 갔다…
		PRINTFORML 
		PRINTFORML 어딘가의 오두막에서 질척질척으로 된 %조사처리(ANAME(MASTER),"가")% 끔찍하게 가로놓여 있다
		PRINTFORML 납치된 %조사처리(ANAME(MASTER),"는")% 그들의 성노예로서 심하게 윤간다 되어, 너덜너덜이 되어 있다
		PRINTFORML 지나친 처사에 %조사처리(ANAME(MASTER),"는")% 망연히 되어 있어 비밀스런 장소로부터 흘러넘치는 정액을 공허하게 바라보고 있었다
		PRINTFORML 도둑들은 %조사처리(ANAME(MASTER),"를")% 지금부터 어떻게 사용할까, 누구의 아이를 임신할까와 천하게 보인 회화를 하고 있다
		PRINTFORMW 이윽고 휴식을 끝낸 그들이 다시 %ANAME(MASTER)%에 모여 오면, 오두막으로부터 또 비통한 신음소리가 영향 냈다
		CALL FUCK(MASTER, "욕망, 성기술, 성교, 봉사, 사랑, Ｃ, Ｖ, Ｂ, Ｍ, Ａ, 윤간, 펠라티오, Ｖ성교, Ａ성교", "처녀상실, 질내사정, Ａ처녀상실, 장내사정", GET_SPERM_ID("도둑"), @"도둑의 \@RAND:2 ? 페니스 # 입술\@", @"도둑", "", "윤간")
		CALL FUCK(MASTER, "욕망, 성기술, 성교, 봉사, 사랑, Ｃ, Ｖ, Ｂ, Ｍ, Ａ, 윤간, 펠라티오, Ｖ성교, Ａ성교", "처녀상실, 질내사정, Ａ처녀상실, 장내사정", GET_SPERM_ID("도둑"), @"도둑의 \@RAND:2 ? 페니스 # 입술\@", @"도둑", "", "윤간")
		PRINTFORML 
		PRINTFORMW 그들의 성노예로서 심하게 범해지고 있었지만, 틈을 찔러 어떻게든 도망치기 시작할 수가 있었다
		CALL SET_TATTOO_RANDOM(MASTER, STR_FOR_TATTOO(-1), 0)
		IF RESULT != -1
			CALL COLOR_PRINT(@"하지만 능욕이 증거로 삼아 %GET_TATTOO_NAME(RESULT)%에, 「%TATTOO:MASTER:RESULT%」라고 문신을 넣어졌다", 칼라_주의)
			PRINTFORMW
		ENDIF
	;갓파의 세력권에 비집고 들어간다
	CASE 4
		PRINTFORML 여행의 도중, 강로 쉬고 있으면 수면에 기포가 떠올라 왔다
		PRINTFORML 겠지 들여다 보면, 갑자기 강중에서 팔이 뻗어 와 발목을 잡아 왔다!
		PRINTFORMW 수중에 끌어들여질 것 같게 되어, 저항하고 있으면 이윽고 강중에서 갓파가 나타났다
		PRINTFORML 갑작스러운 일에 항의를 하면, 여기는 자신의 뜰이니까 멋대로 느긋하게 쉬지마 라고 불평을 말해졌다
		PRINTFORML 그리고 떼어 놓기를 원하면 오이를 보내라고 말해졌다
		PRINTFORML 어떻게 하지?
		CALL ASK_YN("쓰러트린다", "오이를 준다")
		IF RESULT == 0
			PRINTFORML 요괴 상대에게 자비는 없다
			PRINTFORMW %조사처리(ANAME(MASTER),"는")% 갓파를 쓰러트리기로 했다!
		ELSE
			PRINTFORML 하지만 소지에 오이는 없었다
			PRINTFORMW 어쩔 수 없기에 갓파를 쓰러트리기로 했다!
		ENDIF
		PRINTFORML ·
		PRINTFORML ·
		PRINTFORMW ·
		PRINTFORML 하지만 당해 버렸다…
		PRINTFORMW 갓파는 저항 할 수 없게 된 %조사처리(ANAME(MASTER),"를")% 수중으로 끌어들여 갔다
