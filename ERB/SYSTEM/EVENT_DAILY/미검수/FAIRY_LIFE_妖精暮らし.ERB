;---------------------
;발생 확률(1000분 율 100으로 10%)
;---------------------
@EVENT_DAILY_FAIRY_LIFE_RATE()
RETURN (DVAR:요정생활_동거플래그 > 0 ? 150 # 30)

;---------------------
;확률 이외의 발생 판정
;---------------------
@EVENT_DAILY_FAIRY_LIFE_DECISION()
SIF FLAG:데일리이벤트_미검수 == 0
	RETURN 0
SIF DAY < 12
	RETURN 0
SIF !HAS_PENIS(MASTER)
	RETURN 0
SIF DVAR:요정생활_동거플래그 == -1
	RETURN 0
RETURN 1

;---------------------
;장르
;---------------------
@EVENT_DAILY_FAIRY_LIFE_GENRE()
RETURN 데일리_장르_에로

;---------------------
;본체
;---------------------
@EVENT_DAILY_FAIRY_LIFE

;요정과의 만남 이벤트, 동거(同棲) 하고 있는 요정이 있는 경우는 발생하지 않는다
IF DVAR:요정생활_동거플래그 == 0
	SELECTCASE RAND:4
		;밭을 망치는 요정
		CASE 0
			PRINTFORMW 영지민으로부터 밭을 망치는 요정이 있어 곤란해 하고 있으면 상담받았다
			PRINTFORML 어떻게 하지?
			CALL ASK_MULTI("병에게 순찰을 돌게한다" ,"함정을 건다" ,"아무것도하지않는다")
			IF RESULT == 2
				PRINTFORML 요정의 못된 장난 정도로 하나 하나 상관하고 있을 수 없다
				PRINTFORMW (듣)묻지 않았던 것으로 했다
			ELSEIF RESULT == 0
				PRINTFORML 방치도할 수 없기에 병사에 순찰을 돌게하기로 했다
				PRINTFORML ·
				PRINTFORML ·
				PRINTFORMW ·
				PRINTFORML 며칠 후, 병사가 요정을 잡았다
				PRINTFORML 그들은 벌이라고 칭해 그녀를 동반하고 귀가 감옥에 이었다
				PRINTFORMW 그 후은 상상할 수 있었지만, 그들의 평소의 불만을 줄일 수 있다면과 묵인했다
				PRINTFORML …그리고 잠시동안, 감옥의 일각부터는 밤낮 묻지 않고
				PRINTFORMW 병사들의 노리겟감으로 되는 요정의 신음소리가 들려 왔다
			ELSE
				PRINTFORML 함정을 걸기로 했다
				PRINTFORML 사냥꾼에게 도와 받아, 밭의 주위에 요정용의 함정을 걸고 한동안 상태를 보았다
				PRINTFORML ·
				PRINTFORML ·
				PRINTFORMW ·
				PRINTFORML 며칠 후, 요정을 잡는 것을 성공했다
				PRINTFORML 그녀는 우리 중(안)에서 공포에 떨고 있다
				PRINTFORMW 이야기를 하면 배가 비어있어 무심코 흑심이었던 것 같다
				PRINTFORML 어떻게 하지?
				CALL ASK_MULTI("인도한다" ,"숨겨둔다" ,"범한다")
				IF RESULT == 2
					PRINTFORML 요정의 신체를 바라보면, 어리면서 훌륭하게 여자를 풍길 수 있는 라인을 하고 있다
					PRINTFORML 므흣이라고한 %조사처리(ANAME(MASTER),"는")% 그녀를 우리로부터 내 주면 가까이의 빈 오두막에 데리고 들어갔다
					PRINTFORMW 그리고 그녀를 쓰러트리고 옷을 벗겨내면, 투명한 것 같은 피부에 달라붙었다
					PRINTFORML 매끈매끈한 피부에 손가락을 기게 하면 스폰지같이 가라앉아, 그녀는 비통한 소리를 높인다
					PRINTFORML 하지만 한동안 전신을 애무해 주면 그녀는 점차 달콤한 신음소리를 루등 하기 시작했다
					PRINTFORML 이미 인내 할 수 없는 %조사처리(ANAME(MASTER),"는")% 예쁜 핑크색의 고기단지에 마음껏 하나의 물건을 쑤셔 넣었다
					PRINTFORMW 비명을 올려 몸부림치는 그녀에 맞추어 경련하는 질육은 힘들고 %조사처리(ANAME(MASTER),"를")% 단단히 조여 온다
					PRINTFORML 충분히 페니스를 감싸지는 감각에 %조사처리(ANAME(MASTER),"는")% 열중해 허리를 쳐박는다
					PRINTFORML 경련하면서도 눈물을 흘려 용서를 비는 그녀의 표정도 또%ANAME(MASTER)%의 흥분을 부추겼다
					PRINTFORML 정자가 복받쳐 오는 감각에 %조사처리(ANAME(MASTER),"가")% 허리의 움직임을 빨리 하면 그녀의 교성도 날카롭고 영향 낸다
					PRINTFORMW 그리고 욕망대로 안쪽 깊이 사정하면, 그녀는 활처럼 신체를 뒤로 젖히면서 정신을 잃었다
					PRINTFORMW 극상의 고기 오나홀을 한껏 맛본 후, 너덜너덜의 그녀를 방치해 오두막을 떠났다
					CALL FUCK(MASTER, "성기술, 성교, Ｃ, 사정, Ｖ삽입", "동정상실", 0, "", "", "요정의 질", "강간")
					PRINTFORMW 
					DVAR:요정생활_동거플래그 = -1
				ELSEIF RESULT == 0
					PRINTFORML 하지만 피해를 낸 것은 사실이다
					PRINTFORMW %조사처리(ANAME(MASTER),"는")% 피해에 있던 영지민에게 요정을 인도했다
					PRINTFORML 요정은 그 후, 영지민들의 울분을 풀기 위해서(때문에) 성노예로 된 것 같다
					PRINTFORMW 조금의 죄악감을 안으면서도, 능욕 되는 그녀를 상상해 사타구니가 뜨거워졌다
					DVAR:요정생활_동거플래그 = -1
				ELSE
					PRINTFORML 나쁜 아이는 아닌 것 같다
					PRINTFORML 그렇게 생각한 %조사처리(ANAME(MASTER),"는")% 자신이 책임을 가지면 영지민을 설득해 숨겨두는 일로 했다
					PRINTFORMW 그녀는 초경계하고 있었지만, 상냥하게 설득해 주면 %ANAME(MASTER)%의 집에 따라 왔다
					CALL COLOR_PRINT(@"명도 없는 요정이 %ANAME(MASTER)%의 집에 정착했습니다", 칼라_주의)
					PRINTFORMW 
					DVAR:요정생활_동거플래그 = 1
				ENDIF
			ENDIF
		;비를 피하는 요정
		CASE 1
			PRINTFORML 가로 쉬고 있으면 돌연의 억수가 왔다
			PRINTFORML 지나친 비의 기세에 창으로부터 밖을 보고 있으면, 문득 처마 밑에 작은 사람의 그림자를 보기 시작했다
			PRINTFORMW 창으로부터 몸을 나서면 요정의 소녀와 시선이 마주친, 아무래도 비를 피하고 있는 것 같다
			PRINTFORML 어떻게 하지?
			CALL ASK_MULTI("가에 부른다" ,"쫓아버린다" ,"범한다")
			IF RESULT == 2
				PRINTFORML 요정의 신체를 바라보면, 어리면서 훌륭하게 여자를 풍길 수 있는 라인을 하고 있다
				PRINTFORML %조사처리(ANAME(MASTER),"는")% 마음 속에서 입맛을 다심 하면서 상냥한 말로 그녀를 집안에 불렀다
				PRINTFORMW 의심하는 일 없이 따라 오는 그녀를 침실에 데리고 들어가면, 억지로 침대에 쓰러트렸다
				PRINTFORML 당연히 그녀는 격렬하게 날뛰었지만 여러 번 귀싸대기 해 주면, 얌전해졌다
				PRINTFORML 그녀의 옷을 벗겨내면 투명한 것 같은 흰 피부와 상상 이상으로 어른스러워진 지체가 이슬이 된다
				PRINTFORML 이미 인내 할 수 없게 된 %ANAME(MASTER)%도 알몸이 되면 그녀에게 덮어씌우는 것처럼 달라붙는다
				PRINTFORMW 울면서 몸을 비트는 그녀의 표정도 또%ANAME(MASTER)%의 흥분을 부추겨, 열중해 전신을 애무한다
				PRINTFORML 점차 그녀도 반응 하기 시작해, 성감대를 훑으면 흠칫 몸을 흔들게 한 달콤한 한숨을 흘렸다
				PRINTFORML %조사처리(ANAME(MASTER),"는")% 그녀를 옆으로 놓으면 허리를 잡아, 격분한 하나의 물건을 그 핑크의 비밀스런 장소에 쑤셔 넣었다
				PRINTFORML 그녀는 고통에 비명을 올렸지만, %ANAME(MASTER)%하카마원않고 허리를 강탈 힘든 질육을 밀어 헤쳐 간다
				PRINTFORMW 바둥바둥의 꿀단지는 페니스를 전방향으로부터 단단히 조여, 긴장을 늦추면 사정해 버릴 것 같게 된다
				PRINTFORML 어리면서 훌륭한 암컷구멍에 %조사처리(ANAME(MASTER),"는")% 열중해 허리를 쳐박고 그녀의 안을 맛봐 간다
				PRINTFORML 그녀는 눈물을 흘리면서 %ANAME(MASTER)%의 팔에 매달려, 움찔움찔 몸을 활에 경련한다
				PRINTFORML 정자가 복받쳐 오는 감각에 %조사처리(ANAME(MASTER),"가")% 허리의 움직임을 빨리 하면 그녀의 교성도 날카롭고 영향 낸다
				PRINTFORMW 그리고 욕망대로 안쪽 깊이 사정하면, 그녀는 활처럼 신체를 뒤로 젖히면서 정신을 잃었다
				PRINTFORMW 비가 갤 때까지 극상의 고기 오나홀을 한껏 맛본 후, 너덜너덜의 그녀를 내던졌다
				CALL FUCK(MASTER, "성기술, 성교, Ｃ, 사정, Ｖ삽입", "동정상실", 0, "", "", "요정의 질", "강간")
				PRINTFORMW 
				PRINTFORML 
				DVAR:요정생활_동거플래그 = -1
			ELSEIF RESULT == 1
				PRINTFORML 어떤 못된 장난을 되는지 모른다
				PRINTFORMW 쉿 쉿쫓아버리면 그녀는 곤란한 같은 얼굴을 보이면서 억수의 빗속에 사라져 갔다
				DVAR:요정생활_동거플래그 = -1
			ELSE
				PRINTFORML 그곳에서는 감기에 걸려 버릴 것이다, 안에 들어가세요
				PRINTFORML %조사처리(ANAME(MASTER),"가")% 그렇게 부르면 그녀는 놀람 한 것 같아, 뛰어 올라 이쪽을 응시해 왔다
				PRINTFORMW 놀래켜 버린 것을 사과 한 번 더 안에 부른다
				PRINTFORML 그녀는 한동안%ANAME(MASTER)%의 얼굴과 비를 비교해 보고 주저하고 있었지만, 흠칫흠칫 집안에 들어 왔다
				PRINTFORML 타올을 건네주어 따뜻한 차를 넣어 주면 그녀는 요정으로 해서는 예의 바르게 답례를 말했다
				PRINTFORMW 잠깐 빗소리안, 그녀와 정신이 없는 회화를 한다
				PRINTFORML 처음은 긴장하고 있던 그녀도 익숙해져 왔는지, 웃는 얼굴을 보여 재미있는 이야기를 들려주었다
				PRINTFORML 이윽고 비가 그쳤지만, 이미 밖은 깜깜하게 되어 있었다
				PRINTFORMW 이제 늦었으니까 묵어 가도록(듯이) 고하면 그녀는 에헤헤 웃어 수긍했다
				PRINTFORMW …그리고 그녀는 왠지 모르게 %ANAME(MASTER)%의 집에 자리잡는 일이 되었다
				CALL COLOR_PRINT(@"명도 없는 요정이 %ANAME(MASTER)%의 집에 정착했습니다", 칼라_주의)
				PRINTFORMW 
				DVAR:요정생활_동거플래그 = 1
			ENDIF
		;타 먹어 하는 요정
		CASE 2
			PRINTFORML 도를 걷고 있으면, 지장님의 공양물을 군것질하는 요정을 만났다
			PRINTFORMW 그녀는 %조사처리(ANAME(MASTER),"로")% 보여져, 만두를 가득 넣으면서 흠칫 몸을 움추리고 경직되었다
			PRINTFORML 어떻게 하지?
			CALL ASK_MULTI("음식을 올린다" ,"잡는다" ,"떠난다")
			IF RESULT == 2
				PRINTFORML 별로 그것 정도 문제 없을 것이다
				PRINTFORMW %조사처리(ANAME(MASTER),"는")% 아무것도 보지 않았던 것으로 해 그 자리를 떠났다
				DVAR:요정생활_동거플래그 = -1
			ELSEIF RESULT == 1
				PRINTFORML 공양물을 먹는다고는 벌 받을 놈
				PRINTFORML %조사처리(ANAME(MASTER),"는")% 휙 손을 뻗어 그녀를 잡았다
				PRINTFORMW 그녀는 몸을 비틀게 한 용서해 주세요간원 하고 있다
				PRINTFORML 어떻게 하지?
				CALL ASK_YN("판다", "놓친다")
				IF RESULT == 1
					PRINTFORML 자주(잘) 생각하면 이것 정도로 눈꼬리를 세우는 일도 없다
					PRINTFORMW 그녀도 반성하고 있는 것 같은 것으로 놓쳐 주었다
					DVAR:요정생활_동거플래그 = -1
				ELSE
					PRINTFORML 모처럼 잡은 것이니까 놓치다니 터무니 없다
					PRINTFORML 어떻게 하지 생각하고 있으면 그러고 보니 요정 취미의 남자가 있던 일을 생각해 낸다
					PRINTFORMW %조사처리(ANAME(MASTER),"는")% 그녀를 단단히 묶으면 남자의 바탕으로 팔러 실시했다
					PRINTFORML 그는 요정을 꽤나 마음에 들어, 맞돈으로 사 주었다
					PRINTFORML 아마 그녀는 그의 애완동물로서 일생 귀여워해질 것이다
					PRINTFORMW 그녀의 비통한 소리를 등으로 (들)물으면서, %조사처리(ANAME(MASTER),"는")% 금화를 세면서 귀로에 들었다
					MONEY += 5000
					CALL COLOR_PRINTL("금", GETDEFCOLOR(), "5000", 칼라_주의, "를 손에 넣었다", GETDEFCOLOR())
					DVAR:요정생활_동거플래그 = -1
					PRINTFORMW 
				ENDIF
			ELSE
				PRINTFORML 상당히 배가 비어있을 것이다
				PRINTFORML %조사처리(ANAME(MASTER),"는")% 품에 있던 남은 것의 과자를 그녀에게 보냈다
				PRINTFORML 그녀는 일순간 주저 하면서도, 흠칫흠칫 그것을 받았다
				PRINTFORMW %조사처리(ANAME(MASTER),"는")% 좋은 일을 했다고 만족해, 그녀에게 이별을 고해 귀로에 들었다
				PRINTFORML …하지만 그녀가 따라 온다
				PRINTFORMW 그 안어딘가에 갈 것이라고 생각하고 있었지만, 무려 집까지 따라 와 버렸다
				PRINTFORML 어떻게 하지?
				CALL ASK_MULTI("가에 부른다" ,"쫓아버린다" ,"범한다")
				IF RESULT == 1
					PRINTFORML 나쁘지만 요정의 상대는 미안이다
					PRINTFORMW %조사처리(ANAME(MASTER),"가")% 쫓아버리면 그녀는 유감스러운 표정을 보여 떠나 갔다
					DVAR:요정생활_동거플래그 = -1
				ELSEIF RESULT == 2
					PRINTFORML 되돌아 봐 응시하면, 그녀는 수줍은 것처럼 뺨을 물들이면서 숙였다
					PRINTFORML 그 행동에 불끈불끈 온 %조사처리(ANAME(MASTER),"는")% 그녀의 팔을 잡으면 집안에 끌어들여 넣었다
					PRINTFORMW 놀라 당황하는 그녀를 침실에 데리고 들어가면, 침대에 쓰러트리고 덮어씌운다
					PRINTFORML 그녀는 무엇을 되는지 모르고 있는 같아 두근두근 한 모습으로 응시해 온다
					PRINTFORML 일말의 죄악감을 느끼면서 %조사처리(ANAME(MASTER),"는")% 그녀에게 상냥하게 키스를 하면 옷을 벗게 한다
					PRINTFORML 이윽고 눈과 같이 새하얀 피부가 드러나, 그녀는 부끄러운 듯이 머뭇머뭇 했다
					PRINTFORMW %ANAME(MASTER)%도 옷을 벗어 버려 그녀를 껴안으면서 상냥하게 전신을 애무해 준다
					PRINTFORML 그녀는 낯간지러운 듯이 몸을 비틀게 한 한숨을 흘려, 때때로 피쿡과 몸을 진동시켰다
