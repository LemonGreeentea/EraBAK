;---------------------
;기본적인 발생 확률(1000분 율 100으로 10%)
;이것에 콘피그 항목의 발생 빈도를 걸칠 수 있으므로, 반드시 이 대로는 되지 않는다
;---------------------
@EVENT_DAILY_SEEDBED_RATE()
RETURN 1000


;---------------------
;확률 이외의 발생 판정
;0기 이후가 되면 발생한다든가, 데일리측에서 이용하고 있는 변수가-1이라면 일으키지 않는다  같은 튕기는 방법을 할 경우에 사용한다
;---------------------
@EVENT_DAILY_SEEDBED_DECISION()
RETURN 1

;---------------------
;장르
;콘피그 설정으로 사용
;---------------------
@EVENT_DAILY_SEEDBED_GENRE()
RETURN 데일리_장르_에로

;---------------------
;특정의 조건을 채우는 캐릭터를 랜덤에 선택하는 경우에 이용
;다른 함수는 필수이지만, 이것만은 없어도 좋다  라고 할까 퍼포먼스에 영향을 주므로 불필요하면 만들어서는 안 된다
;대상이 존재하지 않고 데일리를 개시 할 수 없는 경우는 0을 돌려주는 것으로 데일리의 발생을 캔슬한다
;---------------------
@EVENT_DAILY_SEEDBED_SETTARGET()
FOR LOCAL, 0, CHARANUM
	;포로가 아니고, 죽지 않고, 모판 소지자의 여자로,1/3의 확률
	IF !CFLAG:LOCAL:포로처 && CFLAG:LOCAL:특수상태 != 특수상태_사망 && GETBIT(TALENT:LOCAL:음란계, 소질_음란_모판) && IS_FEMALE(LOCAL) && !RAND:3 && IS_APPEAR(LOCAL)
		DAILY_TARGET:DAILY_TARGET_NUM = LOCAL
		DAILY_TARGET_NUM ++
	ENDIF
NEXT
SIF DAILY_TARGET_NUM < 1
	RETURN 0
RETURN 1


;---------------------
;본체
;이벤트가 발생했을 경우는 1, 발생하지 않았던 경우는 0을 돌려준다
;발생하지 않았다는 것은 예를 들면, 특정 조건을 채우는 캐릭터로부터 랜덤에 한사람 선택하는 데일리로, 원래 그 조건을 채우는 캐릭터가 한사람도 없는 것 같은 때
;구사양으로 만든 적 있는 사람용으로 말하면 「구사양의 데일리 본체 첫머리에서-1을 돌려주는 것 같은 상황」
;---------------------
@EVENT_DAILY_SEEDBED()
#DIM 대상

FOR LOCAL, 0, DAILY_TARGET_NUM
	대상 = DAILY_TARGET:LOCAL
	PRINTFORMW 모판에 새로 만들 수 있었던 %ANAME(대상)%의 신체가, 밤마다 쑤시고 있는 것 같다……
	SELECTCASE RAND:12
		CASE 0
			PRINTFORML %조사처리(ANAME(대상),"는")% 평상시와 변함없는 생활을 보내 있는 것처럼 보인다
			PRINTFORML ……그 뺨이 매우 세키등응으로 있는 것을 제외하면
			PRINTFORML 그 뱃속에서는, 무수한 촉수가 우글거리고 있었다
			PRINTFORML 그녀에게 기생한 촉수들은, 그 암컷구멍이나 엉덩이 구멍을 때때로 괴롭히고 있다
			PRINTFORML 다른 사람의 눈앞에서, 육체를 탐내지고 있다. 긴장을 늦추면 소리가 새어, 발각되어 버릴지도 모른다
			PRINTFORML 그 자체에 흥분을 느끼면서, %조사처리(ANAME(대상),"는")% 몇 번이나 절정 했다……
		CASE 1
			PRINTFORML 최근 이 근처에서 빈발하는 강간 사건의 범인을 찾기 (위해)때문에, %조사처리(ANAME(대상),"는")% 밤의 순찰에 나왔다
			PRINTFORML ……범인이 발견되지 않는 것을, %조사처리(ANAME(대상),"는")% 알고 있었다. 왜냐하면 그녀가 범인인 것이니까
			PRINTFORML 오늘도 또, 이런 달밤에 혼자서 돌아 다니는 조심성이 없는 아가씨를 보기 시작했다
			PRINTFORML %조사처리(ANAME(대상),"는")% 그 체내에 기생하는 촉수를 전개해, 소녀에게 덤벼 든다
			PRINTFORML %조사처리(ANAME(대상),"는")% 그 후, 사냥의 보수로서 충분히 촉수의 종을 받은 것 같다……
		CASE 2
			PRINTFORML 최근 피부 윤기가 좋은, 어딘가 요염하게 되었다고, %조사처리(ANAME(대상),"는")% 한결 같은 소문이다
			PRINTFORML 그것은 아마, 이같이, 매일밤 자기 방에서 촉수의 상대를 하고 있기 때문일 것이다
			PRINTFORML 구로부터 흘러 들어가지는 정액은 여자의 신체를 아름답고, 추잡하게 길러내는 성분이 포함되어 있다
			PRINTFORML 양구멍이나 전신을 애무하는 브러쉬 상태의 촉수는, 각질을 제거해, 피부에 침투하는 미용 성분을 분비하고 있다
			PRINTFORML 그들에게 몸을 맡기면, 기분 좋을 뿐만 아니라 여자로서 아름답게 거칠어 진다
			PRINTFORML %조사처리(ANAME(대상),"와")% 그들은, 확실히 공생의 관계에 있었다……
		CASE 3
			PRINTFORML 오늘의 %조사처리(ANAME(대상),"는")%, 착 밀착된 검은 이너를 껴입고 있다……
			PRINTFORML ……로 가장해, 그것은 이너에 의태 한 촉수였다
			PRINTFORML %조사처리(ANAME(대상),"는")% 모르는체 하는 얼굴로 보내면서도, 그 육체를 촉수에 탐내지고 있다
			PRINTFORML %조사처리(ANAME(대상),"는")% 때때로 뺨을 붉히고, 숨을 거칠게 하면서도, 어떻게든 평정을 유지하려 하고 있다……
			PRINTFORML 자기 방으로 돌아가, 촉수로부터 해방되면, 마개를 되고 있던 양구멍으로부터 애액과 백탁이 걸죽 흘러 떨어졌다……
		CASE 4
			PRINTFORML 오늘의 %조사처리(ANAME(대상),"는")%, 전체적으로 낙낙한 의상을 감기고 있다……
			PRINTFORML 그 안쪽을 촉수가 기어다니고 있으니까, 그러한 옷을 선택하지 않을 수 없었던 것이다
			PRINTFORML 일에 열심히 해, 사람과 이야기를 해……그것들 모든 사이 사이에, 자궁에 서식하는 촉수가 질척질척 고기구멍을 휘젓는다
			PRINTFORML 결국 인내 할 수 없게 된 %조사처리(ANAME(대상),"는")%, 빠른 걸음으로 자기 방으로 돌아가, 옷을 벗어 던진다
			PRINTFORML 그리고 그 신체를, 촉수에 한껏 위로해 받았다……
		CASE 5
			PRINTFORML %조사처리(ANAME(대상),"는")% 고기로 할 수 있던 캡슐에 휩싸여, 자고 있다
			PRINTFORML 촉수에 의해 만들어진, 특제의 침대다
			PRINTFORML 촉수의 점액의 효과로, %조사처리(ANAME(대상),"는")% 깊고 편한 잠에 붙어 있다
			PRINTFORML 그 사이에도, 촉수는 그녀의 암컷구멍이나 엉덩이 구멍, 유방을 조율해, 개조해 나간다
			PRINTFORML 본인의 상관없는 곳으로, %조사처리(ANAME(대상),"는")% 추잡한 모판으로 다시 태어나 간다……
			PRINTFORML 아침이 되어 눈을 뜬 %조사처리(ANAME(대상),"는")%, 점액을 닦아내 옷을 입으면, 여느 때처럼 일하러 나왔다
		CASE 6
			PRINTFORML %조사처리(ANAME(대상),"는")% 목욕탕에도 들어가지 않고, 배설도 하고 있지 않다
			PRINTFORML 라고 해도 불결할 것은 아니다. 어느쪽이나 촉수가 알선을 해 주기 때문이다
			PRINTFORML 매일 밤의 행위 중(안)에서, 방광이나 장에 기생하는 촉수가, 노폐물을 먹어 간다
			PRINTFORML 브러쉬 상태의 촉수가 피부를 상냥하게 쓸어올려, 때를 예쁘게 철거해 간다
			PRINTFORML 기분이 좋은 데다가 이것만을 해 준다. 그들을 거절하는 이유를, %조사처리(ANAME(대상),"는")% 발견해 낼 수 없었다……
		CASE 7
			PRINTFORML %조사처리(ANAME(대상),"는")% 밤, 휘청휘청 돌아 다니고 있다. 그렇다고 해도, 본인에게 의식은 없다
			PRINTFORML 그녀에게 기생하는 촉수가 귀로부터 뇌에 전류를 흘려, 멋대로 조종하고 있다
			PRINTFORML 밤길을 조심성이 없게도 혼자서 돌아 다니는 아가씨를 찾아내, %조사처리(ANAME(대상),"를")% 조종하는 촉수는 소녀에게 달려든다
			PRINTFORML 그리고 그녀를 붙잡아 뒷골목에 데리고 들어가면, %ANAME(대상)%의 자궁 내부에서 당신의 신체를 전개했다
			PRINTFORML 촉수는 %ANAME(대상)%의 육체와 함께, 소녀에게도 한껏 당신의 종을 옮겨 심었다……
		CASE 8
			PRINTFORML %조사처리(ANAME(대상),"는")% 최근, 무술 훈련으로 지고 알지 못하고다
			PRINTFORML 그녀에게 기생하는 촉수가 귀로부터 뇌에 비집고 들어가, 행동의 어시스트를 하고 있다
			PRINTFORML 게다가 부드러운 촉수가 근육과 같이 수축하는 것으로, 모든 행동을 강력하게 한다
			PRINTFORML 그 후, 훈련이 잘된 예로서 %조사처리(ANAME(대상),"는")% 그들에게 충분히 신체를 제공한 것 같다……
		CASE 9
			PRINTFORML %조사처리(ANAME(대상),"는")% 촉수에 육체를 탐내지고 있다
			PRINTFORML 모든 구멍에 촉수가 쑤셔 넣어져 한층 더 전신을 쓸어올려지고 있다
			PRINTFORML 기절할 정도의 쾌락에, %조사처리(ANAME(대상),"는")% 흐려진 교성을 계속 올리고 있다
			PRINTFORML 그들이 정액을 발할 때, 그 정액에 포함되는 최음효과로, %조사처리(ANAME(대상),"는")% 고조되어 간다
			PRINTFORML 행위는 자꾸자꾸격렬해지면서, 밤이 희어질 때까지 계속되었다……
		CASE 10
			PRINTFORML 무수한 촉수가 %조사처리(ANAME(대상),"를")% 범하고 있다
			PRINTFORML 그런 가운데, 극세의 촉수가, %ANAME(대상)%의 요도로 기어들었다
			PRINTFORML 보통이라면 고형물이 들어갈리가 없는 곳을 범해지는 아픔과 쾌락에, %조사처리(ANAME(대상),"는")% 몹시 놀란다
			PRINTFORML 이윽고 촉수는 그녀의 방광에 완전하게 침입해, 거기서 떼어내져 그녀의 체내에 기생했다……
		CASE 11
			PRINTFORML 쓸데없이 긴 촉수가, %ANAME(대상)%의 엉덩이 구멍으로 비집고 들어간다
			PRINTFORML 그것은 그녀의 장내를 충분히 묻으면, 거기서 당신의 신체를 떼어냈다
			PRINTFORML 격렬한 용변 마려움을 기억하는 %조사처리(ANAME(대상),"를")% 뒷전으로, 촉수는 거기로부터 나가려고 하지 않는다
			PRINTFORML 아무래도 그는, %ANAME(대상)%의 장내에 서식해 버린 것 같다……
		CASE 12
			PRINTFORML 가는 촉수가, 촉수에 범해진 %ANAME(대상)%의 질 구멍으로 비집고 들어간다
			PRINTFORML 그 가늘음에 어딘지 부족함을 기억하는 %조사처리(ANAME(대상),"를")% 뒷전으로, 그것은 그녀의 자궁에까지 침입했다
			PRINTFORML 본래 이물이 들어갈 리가 없는 장소를 범해지는 고통과 쾌락에, %조사처리(ANAME(대상),"는")% 몸부림치고 있다
			PRINTFORML 촉수는 거기서 당신의 첨단을 떼어내, 그녀의 자궁에 서식했다……
		CASE 13
			PRINTFORML %조사처리(ANAME(대상),"는")% 촉수와 무연인, 보통 하루를 보내고 있다……
			PRINTFORML ……라는 것은, 그녀의 귓구멍으로부터 뇌에 침입한 촉수가 보이고 있는 환각이다
			PRINTFORML 촉수 특제의 고기의 캡슐 거두어들여진 그녀는, 전신이 개조되어 능욕되고 있다
			PRINTFORML 그런데도 그녀는 그것을 눈치채지 못하고, 아침에 일어 나고 나서 잘 때까지의 평범한 하루의 환각을 계속 보았다……
	ENDSELECT
	CALL FUCK_TENTACLE(대상, GET_SPERM_ID("촉수"), @"촉수", @"촉수")

	PRINTFORMW
NEXT
RETURN 1

@EVENT_DAILY_SEEDBED_ALLOW_WHEN_WANDERING
RETURN 1

