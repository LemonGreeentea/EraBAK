;---------------------
;기본적인 발생 확률(1000분 율 100으로 10%)
;이것에 콘피그 항목의 발생 빈도를 걸칠 수 있으므로, 반드시 이 대로는 되지 않는다
;---------------------
@EVENT_DAILY_TENTACLE_BIRTH_RATE()
RETURN 50


;---------------------
;확률 이외의 발생 판정
;0기 이후가 되면 발생한다든가, 데일리측에서 이용하고 있는 변수가-1이라면 일으키지 않는다  같은 튕기는 방법을 할 경우에 사용한다
;---------------------
@EVENT_DAILY_TENTACLE_BIRTH_DECISION()
#DIM 촉수
;미검수 OFF면 스킵
SIF FLAG:데일리이벤트_미검수 == 0
	RETURN 0
촉수 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:(특수세력_촉수))
;촉수 세력이 없는 경우는 캔슬
SIF 촉수 == -1
	RETURN 0

RETURN 1

;---------------------
;장르
;콘피그 설정으로 사용
;---------------------
@EVENT_DAILY_TENTACLE_BIRTH_GENRE()
RETURN 데일리_장르_에로

;---------------------
;특정의 조건을 채우는 캐릭터를 랜덤에 선택하는 경우에 이용
;다른 함수는 필수이지만, 이것만은 없어도 좋다  라고 할까 퍼포먼스에 영향을 주므로 불필요하면 만들어서는 안 된다
;대상이 존재하지 않고 데일리를 개시 할 수 없는 경우는 0을 돌려주는 것으로 데일리의 발생을 캔슬한다
;---------------------
@EVENT_DAILY_TENTACLE_BIRTH_SETTARGET()
;방랑중 캐릭터로부터 타겟을 랜덤 선출
FOR LOCAL, 0, CHARANUM
	;녀로, 포로가 아니고 사망도 하고 있지 않고, 어디에도 소속하지 않았다(당신은 방랑 판정이 아니고 미등장 판정이 되어 있기에 추가로 이렇게 한다)
	IF IS_FEMALE(LOCAL) && !CFLAG:LOCAL:포로처 && !CFLAG:LOCAL:소속 && (CFLAG:LOCAL:특수상태 == 특수상태_방랑 || LOCAL == MASTER)
		DAILY_TARGET:DAILY_TARGET_NUM = LOCAL
		DAILY_TARGET_NUM ++
	ENDIF
NEXT

;후보가 없으면 되돌린다
SIF DAILY_TARGET_NUM < 1
	RETURN 0
RETURN 1

;---------------------
;본체
;이벤트가 발생했을 경우는 1, 발생하지 않았던 경우는 0을 돌려준다
;발생하지 않았다는 것은 예를 들면, 특정 조건을 채우는 캐릭터로부터 랜덤에 한사람 선택하는 데일리로, 원래 그 조건을 채우는 캐릭터가 한사람도 없는 것 같은 때
;구사양으로 만든 적 있는 사람용으로 말하면 「구사양의 데일리 본체 첫머리에서-1을 돌려주는 것 같은 상황」
;---------------------
@EVENT_DAILY_TENTACLE_BIRTH()
#DIM 대상
#DIM 촉수
#DIM 병수

촉수 = GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:(특수세력_촉수))


;후보 난수 가운데, 제일 비싼 값을 낸 캐릭터를 대상으로 한다
대상 = DAILY_TARGET:(RAND:DAILY_TARGET_NUM)

PRINTFORML 번식을 위한 새로운 모판을 요구해 방황하고 있던 촉수가, 방랑중의 %조사처리(ANAME(대상),"를")% 찾아냈다
PRINTFORML 촉수는 %ANAME(대상)%에 눈치채지지 않게 지켜보다, 그녀가 인기의 없는 장소에 도달했을 때에 덤벼 들었다!
PRINTFORMW 갑작스러운 습격에 %조사처리(ANAME(대상),"는")% 반응하지 못하고 잡혀 버려, 전신이 묶여 그늘로 데리고 들어가졌다……
PRINTFORML 
PRINTFORMW %조사처리(ANAME(대상),"가")% 촉수에 범해지고 있다
SELECTCASE RAND:25
	CASE 0
		PRINTFORML 인의 팔 정도도 있는 대담한 촉수를 격렬하게 쑤셔 넣어져, %조사처리(ANAME(대상),"는")% 눈물을 흘리면서 전신을 경련시킨다
		PRINTFORML 촉수가 출입할 때에 %ANAME(대상)%의 하복부가 보콕과 부풀어 올라, %ANAME(대상)%의 신체가 리드미컬하게 튄다
		PRINTFORML 자궁이 망쳐질 정도의 충격에 머릿속이 새하얗게 되면서도, %조사처리(ANAME(대상),"는")% 이를 악물어 들 없게 참고 있었지만
		PRINTFORMW 깊숙히 쑤셔 넣어진 촉수로부터 대량의 정액을 토해내지면, 지나친 열에 %조사처리(ANAME(대상),"는")% 절규와 함께 절정 당해 버렸다
	CASE 1
		PRINTFORML 그 밀혈에는 앞을 다투어 대량의 촉수가 기어들고 있어 그 격렬함에 %조사처리(ANAME(대상),"는")% 머리를 뒤로 젖히게 한다
		PRINTFORML 그 하복부는 안으로 꿈틀거리는 촉수에 맞추어 불퉁불퉁 부풀어 올라, 그때마다 %조사처리(ANAME(대상),"는")% 움찔움찔 신체를 진동시킨다
		PRINTFORML 대량의 촉수에 질척질척 질육을 끊임없이 자극계속 되어, 점차 %ANAME(대상)%의 목으로부터는 긁힌 신음소리가 새어나온다
		PRINTFORMW 그리고 그것들의 촉수로부터 작열의 정액을 발해지면, %조사처리(ANAME(대상),"는")% 야무지지 못한 아헤얼굴을 쬐면서 절정 해 버렸다
	CASE 2
		PRINTFORML 알몸에 홀랑 벗겨져 대자의 모습으로 구속된 %ANAME(대상)%의 밀혈에 미끌한 촉수가 깊숙히 찔러넣어지고 있다
		PRINTFORML 체내를 촉수가 기어 돌아다니는 감촉에 %조사처리(ANAME(대상),"는")% 공포심이 달려 필사적이어도 금액이, 촉수는 개의치 않고 자궁입구로 간신히 도착한다
		PRINTFORML 촉수가 동글동글 태내로 기어들려고 하면, 그 감각에 %조사처리(ANAME(대상),"는")% 울면서 오슬오슬 등줄기를 진동시켰다
		PRINTFORMW %조사처리(ANAME(대상),"는")% 태내에 도푸도푸 촉수의 정액을 따라지는 것을 느끼면서, 지나친 열에 바라지 않는 절정으로 달성되었다
	CASE 3
		PRINTFORML 양구멍에 촉수를 깊숙히 쑤셔 넣어진 %조사처리(ANAME(대상),"는")%, 망가진 인형과 같이 때때로 흐려진 신음소리를 내고 있다
		PRINTFORML 그 입으로부터는 엉덩이 구멍으로부터 삽입된 촉수의 첨단이 얼굴을 내밀고 있어 그 움직임에 맞추어 %조사처리(ANAME(대상),"는")% 경련하고 있다
		PRINTFORML 촉수에 꼬치로 되어 체내를 빠짐없이 유린된 %조사처리(ANAME(대상),"는")%, 공허한 표정으로 주어지는 자극에 맞추어 떨릴 뿐이다
		PRINTFORMW 이윽고 %ANAME(대상)% 안에 그들의 자식이 따라지면, %조사처리(ANAME(대상),"는")% 신체를 뒤로 젖히게 하면서 미지의 절정으로 이끌렸다
	CASE 4
		PRINTFORML %조사처리(ANAME(대상),"는")% 범해지고 있는 것에도 불구하고, 스스로 적극적으로 허리를 흔들어 촉수를 받아들이고 있다
		PRINTFORML 촉수에 주입된 미약의 효과에 의해, 지금은 %ANAME(대상)%의 머릿속은 쾌락으로 다 메워지고 씨뿌리기를 애타게 기다리고 있다
		PRINTFORML 그 암컷구멍에는 다수의 촉수가 출입해 터질듯이 되어 있지만, %조사처리(ANAME(대상),"는")% 아헤얼굴을 쬐어 기쁨의 소리를 높인다
		PRINTFORMW 차례차례로 다른 씨뿌리기 계속 되어, 자궁이 그들의 종으로 채워질 때마다 %조사처리(ANAME(대상),"는")% 보다 행복하게 가득 찬 절정을 맛보았다
	CASE 5
		PRINTFORML 구속되고 엉덩이를 내미는 모습의 %ANAME(대상)%의 밀혈에, 질퍽질퍽 추잡한 소리를 내면서 한 개의 촉수가 출입하고 있다
		PRINTFORML 그 촉수의 표면에는 작은 오돌오돌이 무수에 나 있어 출입할 때에 질벽을 빠득빠득 후벼파지고 %조사처리(ANAME(대상),"는")% 좋아해 미친다
		PRINTFORML 적확하게 암컷의 약점을 몰아세우는 촉수에, 이미 몇 번이나 절정 당한 %조사처리(ANAME(대상),"는")%, 이미 저항심도 사라져 쾌락에 몸을 맡기고 있다
		PRINTFORMW 교육이 살았다고 판단한 촉수가 이번은 씨뿌리기를 위해서(때문에) 안쪽 깊게 침입해 오면, %조사처리(ANAME(대상),"는")% 기쁨의 소리와 함께 그것을 받아들였다
	CASE 6
		PRINTFORML 역공중에 매달림으로 된 상태의 %조사처리(ANAME(대상),"는")% 전신의 구멍이라는 구멍에 촉수를 쑤셔 넣어져 흐려진 신음소리를 올리고 있다
		PRINTFORML 질육, 엉덩이살, 입안, 유방내, 모두에 촉수가 푹 꽂아넣어져, 그것들의 움직임에 맞추어 %조사처리(ANAME(대상),"는")% 전신을 진동시킨다
		PRINTFORML %ANAME(대상)%의 이성은 너무 폭력적인 쾌락에 벌써 바람에 날아가고 있어 전신으로부터 주어지는 쾌락에 몸을 바쳐 좋아해 미친다
		PRINTFORMW %조사처리(ANAME(대상),"는")% 촉수와 일체화하는 것 같은 착각을 받으면서 전신에 씨뿌리기 계속 되어, 그때마다 몇 번이나 몇 번이나 절정에 이르렀다
	CASE 7
		PRINTFORML %조사처리(ANAME(대상),"는")% 촉수에 대자로 구속되면서 아히아히와 괴로운 듯한 소리를 높여 발버둥치고 있는
		PRINTFORML 그 배는 임월의 임산부같이 부풀어 오르고 있어 표면이 불퉁불퉁 유동해 안으로 뭔가가 꿈틀거리고 있는 것을 알 수 있다
		PRINTFORML 태내에 발해진 촉수의 종이 %ANAME(대상)%의 마력을 빨아 올리고 급속히 성장하면서, 안으로부터 %조사처리(ANAME(대상),"를")% 범하고 있다
		PRINTFORMW 이윽고 출산때를 맞이하면 그들은 일제히 출구를 목표로 해, 우리 아이에게 질육을 후벼파지는 감각과 함께%조사처리(ANAME(대상),"는")% 강렬한 절정에 이르렀다
	CASE 8
		PRINTFORML 암컷구멍 깊이 쑤셔 넣어진 촉수로부터 대량의 정액이 발해지면서, %조사처리(ANAME(대상),"는")% 크게 몸을 뒤로 젖히게 해 절정 한다
		PRINTFORML 잡히고 나서 끝없이 씨뿌리기 계속 된 %조사처리(ANAME(대상),"는")% 이미 몸도 마음도 굴복 당하고 있어 그 표정은 완전히 쾌락에 정신나가고 있다
		PRINTFORML 씨뿌리기를 끝낸 촉수가 밀혈으로부터 뽑아 내지면, 라는 천한 소리와 함께 대량의 걸쭉한 백탁액이 넘쳐 나온다
		PRINTFORMW %조사처리(ANAME(대상),"는")% 아헤얼굴인 채 지면에 힘 없게 가로놓이면서, 태내에서 촉수의 종이 마력을 들이마셔 커져 가는 것을 느꼈다
	CASE 9
		PRINTFORML %조사처리(ANAME(대상),"는")% 대량의 촉수에 다 메워지면서 전신의 구멍이라는 구멍을 범해지고 있다
		PRINTFORML 이 암컷을 번식 상대로서 마음에 든 촉수는 확실히 임신하게 할 수 있도록 가차 없이 %ANAME(대상)%의 안쪽 깊게 기어들어 몇 번이나 종을 발한다
		PRINTFORML 그때마다 %조사처리(ANAME(대상),"는")% 바라지 않는 절정에 달성되고 눈물을 흘려 용서를 빌지만, 촉수에 마음이 있을 리도 없고 또 다른 촉수가 쑤셔 넣어진다
		PRINTFORMW 그 중 %ANAME(대상)%도 주어지는 쾌락에 솔직하게 몸을 맡기고이고, 공허한 표정으로 스스로 신체를 구불거리게 할 수 있어 그들을 받아들여 갔다
	CASE 10
		PRINTFORML %ANAME(대상)%의 양유두에 첨단이 무수에 분기한 촉수가 쑤셔 넣어져 질척질척 만지작거려지고 있다
		PRINTFORML 그 있을 수 없는 광경에 %조사처리(ANAME(대상),"는")% 몹시 놀라면서 유선안을 촉수가 기어 돌아다니는 감각에 목으로부터 떨린 소리를 흘려 허덕인다
		PRINTFORML 유방을 자대로서 개조를 끝낸 촉수는 다음에 산란을 시작해, 알과 함께 미칠 것 같은 쾌락이 주어져 %조사처리(ANAME(대상),"는")% 교성을 올린다
		PRINTFORMW 수배에 부풀어 올라 두근두근 맥동 하는 유방을 응시하면서, %조사처리(ANAME(대상),"는")% 황홀한 표정을 띄워 산란때를 기다려 바랬다
	CASE 11
		PRINTFORML 촉수의 벽에 파묻힌 %조사처리(ANAME(대상),"는")% 전신을 살짝 깨물기 되도록(듯이) 열심히 애무되고 있다
		PRINTFORML 처음은 무섭게 느끼고 있었지만 점차 그 감촉에 전신이 녹는 것 같은 감각에 빠져, 오슬오슬 몸을 진동시키고 신음소리를 루등 하기 시작한다
		PRINTFORML 대기에 노출된 보지도 안타까운 것같이 애액을 늘어뜨리면서 빠끔빠끔 실룩거려, %조사처리(ANAME(대상),"는")% 무의식 중에 허리를 구불거리게 되어지고 있었다
		PRINTFORMW 이윽고 대망의 촉수가 비밀스런 장소에 쑤셔 넣어지면, %조사처리(ANAME(대상),"는")% 기쁨의 소리와 함께 절정 하면서, 눈을♥로 해 씨뿌리기를 졸라댔다
	CASE 12
		PRINTFORML %조사처리(ANAME(대상),"는")% 제정신을 잃은 여성에 덮어누를 수 있어 그 사타구니로부터 성장하는 촉수를 쑤셔 넣어지고 있다
		PRINTFORML 모판의 자궁에 기생한 촉수가 그녀를 조종해, 다음의 모판으로서 확정한 %ANAME(대상)%안으로 알을 받아들이려고 하고 있다
		PRINTFORML 어떻게든 도망치려고 하는 것도, 씩씩한 촉수에 질육을 교묘하게 비비어질 때에 저리는 것 같은 쾌락이 달려, 삐걱삐걱 허리가 떨려 버린다
