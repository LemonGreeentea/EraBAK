;---------------------
;기본적인 발생 확률(1000분 율 100으로 10%)
;이것에 콘피그 항목의 발생 빈도를 걸칠 수 있으므로, 반드시 이 대로는 되지 않는다
;---------------------
@EVENT_DAILY_RESCUE_RATE()
RETURN 30


;---------------------
;확률 이외의 발생 판정
;0기 이후가 되면 발생한다든가, 데일리측에서 이용하고 있는 변수가-1이라면 일으키지 않는다  같은 튕기는 방법을 할 경우에 사용한다
;---------------------
@EVENT_DAILY_RESCUE_DECISION()
SIF FLAG:데일리이벤트_NTR == 0
	RETURN 0
LOCAL:1 = 0

FOR LOCAL, 0, CHARANUM
	SIF CFLAG:LOCAL:소속 == CFLAG:MASTER:소속 && CFLAG:LOCAL:포로처 != 0 && CFLAG:LOCAL:포로처 != CFLAG:MASTER:소속
		RETURN 1
NEXT

RETURN 0

;---------------------
;장르
;콘피그 설정으로 사용
;---------------------
@EVENT_DAILY_RESCUE_GENRE()
RETURN 데일리_장르_에로

;---------------------
;본체
;이벤트가 발생했을 경우는 1, 발생하지 않았던 경우는 0을 돌려준다
;발생하지 않았다는 것은 예를 들면, 특정 조건을 채우는 캐릭터로부터 랜덤에 한사람 선택하는 데일리로, 원래 그 조건을 채우는 캐릭터가 한사람도 없는 것 같은 때
;구사양으로 만든 적 있는 사람용으로 말하면 「구사양의 데일리 본체 첫머리에서-1을 돌려주는 것 같은 상황」
;---------------------
@EVENT_DAILY_RESCUE()
#DIM 대상
#DIM 실행자
#DIM 포로번호, 3000

PRINTFORML 적국에 잡힌 동료를 구출하는 작전이 제안 되었다
PRINTFORML 어떻게 하지?
CALL ASK_YN("실행한다", "그만둔다")
IF RESULT == 1
	PRINTFORMW 위험하다, 지금은 기를 엿보자
	RETURN 1
ELSE
	PRINTFORMW 동료를 버릴 수 없는, 구출 작전을 실시하기로 했다
	$RESCUE_LOOP 
	PRINTFORML 누구를 구출에 향할까?
	CALL SINGLE_DRAWLINE
	FOR LOCAL, 0, CHARANUM
		IF CFLAG:LOCAL:소속 == CFLAG:MASTER:소속 && CFLAG:LOCAL:포로처 != 0 && CFLAG:LOCAL:포로처 != CFLAG:MASTER:소속
			포로번호:(LOCAL:1) = LOCAL
			PRINTFORM [{LOCAL:1}]%ANAME(LOCAL)%
			PRINTL
			LOCAL:1 ++
		ENDIF
	NEXT
	CALL SINGLE_DRAWLINE
	PRINTFORML [1000] 그만둔다
	$INPUT_LOOP
	INPUT
	IF RESULT == 1000
		$CANCEL
		PRINTFORMW 역시 위험하다, 지금은 기를 엿보자
		RETURN 1
	ELSEIF RESULT < 0 || RESULT >= LOCAL:1
		GOTO INPUT_LOOP
	ELSE
		대상 = 포로번호:(RESULT)
		PRINTFORMW %조사처리(ANAME(대상),"를")% 구출하기로 했다
		PRINTFORML 누가 구출에 향할까?
		CALL SELECT_CHARA_LIST_ONLY_LOGIC_SLG("RESCUE", "RESCUE")
		SIF RESULT == -1
			GOTO CANCEL
		실행자 = RESULT
		IF 실행자 == MASTER
			PRINTFORMW %ANAME(실행자)%스스로 향하기로 했다
		ELSE
			PRINTFORMW %ANAME(실행자)%를 향하게 하기로 했다
		ENDIF
		PRINTFORML ·
		PRINTFORML ·
		PRINTFORMW ·
		IF (ABL:실행자:무투 + ABL:실행자:지략 + ABL:실행자:정치) / 10 >= RAND:30 + DAY / 10
			PRINTFORML 했다!
			PRINTFORML 구출 작전은 성공했다!
			PRINTFORMW 잡히고 있었던 %조사처리(ANAME(대상),"를")% 데리고 돌아왔다
			CFLAG:대상:포로처 = 0
			IF 실행자 == MASTER
				PRINTFORMW %ANAME(대상)%의 %ANAME(실행자)%에의 평가가 크게 올랐다
				CFLAG:대상:호감도 += 500
			ENDIF
		ELSE
			PRINTFORML 끝냈다!
			PRINTFORMW 작전에 실패했다……
			IF IS_MALE(실행자)
				PRINTFORML %ANAME(실행자)%는 간신히 도망갔다
				CALL ADD_COOLTIME(실행자, 3)
			ELSE
				PRINTFORML %ANAME(실행자)%는 스파이로서 적병에게 잡혀 버렸다
				PRINTFORMW 다부지게 노려보는 %ANAME(실행자)%에 대해, 그들은 히죽히죽 웃으면서 심문을 위해서(때문에) 데려 갔다……
				PRINTFORML 
				CALL RESCUE_RAPE(실행자)
				PRINTFORML 
				IF RAND:3 == 0
					PRINTFORMW 그 후, %ANAME(실행자)%도 %ANAME(대상)%같이, 포로로 되어 버렸다
					CFLAG:실행자:포로처 = CFLAG:대상:포로처
				ELSE
					PRINTFORML 심하게 능욕 되었지만, %ANAME(실행자)%는 어떻게든 도망갔다
					CALL ADD_COOLTIME(실행자, 3)
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF

RETURN 1

@SELECT_CHARA_LIST_SHOW_LOGIC_RESCUE(대상)
#DIM 대상
RETURN CFLAG:대상:행동불능상태 != 행동불능_아이 && CFLAG:대상:소속 == CFLAG:MASTER:소속 && !IS_ANIMAL(대상) && !TALENT:대상:임신 && GET_COUNTRY_BOSS(CFLAG:대상:소속) != 대상

@SELECT_CHARA_LIST_SELECT_LOGIC_RESCUE(대상)
#DIM 대상
RETURN CFLAG:대상:포로처 == 0


;-----------------------------------------------
;잠입 실패
;-----------------------------------------------
@RESCUE_RAPE(ARG:0)

PRINTFORMW 잡힌 %조사처리(ANAME(ARG:0),"가")% 능욕을 받고 있다……
SELECTCASE RAND:40
	CASE 0
		PRINTFORML 침대에 대자로 구속된 %조사처리(ANAME(ARG:0),"가")%, 쫑긋쫑긋 몸을 진동시키면서 얼굴을 뒤로 젖히게 해 숨을 거칠게 하고 있다
		PRINTFORML %조사처리(ANAME(ARG:0),"는")% 심문이라고 칭해 병사들의 노리겟감으로 되어 쉬는 일도 용서되지 않고 오로지 그 몸을 저 좋은 대로 사용되었다
		PRINTFORML 새빨갛게 부은 그 비밀스런 장소로부터는 대량의 백탁액이 끝 없게 넘치고 있어 야무지지 못하게 열고 있을 뿐이 되어 버리고 있다
		PRINTFORML 끊임없는 능욕에 의한 고통과 쾌락의 물결로 %ANAME(ARG:0)%의 머리는 정신나가고 자르고 있어 때때로 신체를 경련시키면서 목으로부터 달콤한 한숨을 흘린다
		PRINTFORMW 여자 가뭄의 병사들에게 있어 %조사처리(ANAME(ARG:0),"는")% 극상의 육변기이며, 그 뒤도 용서 없는 씨뿌리기 강간이 반복해졌다
	CASE 1
		PRINTFORML 어떤마을의 일각부터 %ANAME(ARG:0)%의 비통한 신음소리와 그것을 둘러싸는 남자들의 야유가 들려 온다
		PRINTFORML 좀처럼 자백하지 않는 %조사처리(ANAME(ARG:0),"는")%, 본보기로서 쬐어 받침대에 구속되어 온 마을에 설치되는 일이 되었다
		PRINTFORML 다수의 병사나 주민들에 의해 끊임없이 그 몸을 저 좋은 대로 사용되고 더럽혀져 %ANAME(ARG:0)%의 전신은 질척질척이 되어 간다
		PRINTFORML 무수한 페니스를 쑤셔 넣어지고 계속한 보지와 엉덩이는 빨강에 개여, 전신은 백탁액으로 질척질척이 되어 버리고 있다
		PRINTFORML 한사람의 남자에게 범해지면서 갑자기 격렬하게 엉덩이를 얻어맞아 %조사처리(ANAME(ARG:0),"는")% 무심코 교성을 주어 버려, 주위로부터 비웃음의 소리가 높아졌다
		PRINTFORMW 그 연회는 밤까지 계속되어, 해방되는 무렵에는 %조사처리(ANAME(ARG:0),"는")% 신체의 안쪽 깊게까지 완전히 쾌락을 새겨져 있었다
	CASE 2
		PRINTFORML 어슴푸레한 감옥중에서 흐려진 %ANAME(ARG:0)%의 신음소리와 추잡한 미츠네, 그리고 고기가 서로 치는 소리가 울리고 있다
		PRINTFORML 극상의 암컷인 %조사처리(ANAME(ARG:0),"를")% 앞에 병사들은 참지 못하고, 심문이라고 칭해 그들의 육변기로서 계속 휴일 없이 범하고 있다
		PRINTFORML 처음은 저항하고 있었던 %ANAME(ARG:0)%도, 몇 번이나 맞고 위협해진 결과, 완전히 온순하게 그들에게 봉사하게 되어 있었다
		PRINTFORML 한사람의 병사에 걸쳐 스스로 허리를 흔들면서, 양손과 입을 사용해 주위의 병사들에게도 열심히 만족시켜, 그때마다 질척질척으로 된다
		PRINTFORMW 그 %ANAME(ARG:0)%의 요염한 봉사에 병사들의 육욕은 들어가지 않고, 그들의 심문은 심야 늦게까지 계속되는 일이 되었다
	CASE 3
		PRINTFORML 감옥안, 심문관들에게 둘러싸인 %조사처리(ANAME(ARG:0),"는")%, 깊숙히 페니스를 쑤셔 넣어지면서 교성을 주고 있다
		PRINTFORML 저 좋은 대로 범해지고 있는 것에도 불구하고 %조사처리(ANAME(ARG:0),"는")% 에헤에헤와 웃어 스스로 허리를 흔들어, 양손으로 열심히 페니스를 훑는다
		PRINTFORML 심문을 위해서(때문에) 약품 과다투여로 된 %ANAME(ARG:0)%의 머릿속은 쾌락으로 다 메워져, 페니스를 물 수밖에 생각할 수 없게 되고 있다
		PRINTFORML 페니스로 일발 찌르기 될 때마다 신체를 치게 해 클리토리스를 튕겨지면 야무지지 못하게 군침을 흩뿌려 기쁨의 소리를 지른다
