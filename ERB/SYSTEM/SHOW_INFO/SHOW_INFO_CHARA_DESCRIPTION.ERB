;-------------------------------------------------
;에로스테이타스 표시
;-------------------------------------------------
@SHOW_INFO_ERO_DESCRIPTION(대상)
#DIM 대상
CALL SINGLE_DRAWLINE
PRINTFORML 육체
CALL COLOR_LINE

CALL SHOW_INFO_ERO_DESCRIPTION_BODY(대상)

CALL SINGLE_DRAWLINE
PRINTFORML 감각
CALL COLOR_LINE

CALL SHOW_INFO_ERO_DESCRIPTION_SENSE(대상)

CALL SINGLE_DRAWLINE
PRINTFORML 성적 기호
CALL COLOR_LINE

CALL SHOW_INFO_ERO_DESCRIPTION_FETISH(대상)

CALL SINGLE_DRAWLINE
PRINTFORML 함락
CALL COLOR_LINE

CALL SHOW_INFO_ERO_DESCRIPTION_FALLEN(대상)

CALL SINGLE_DRAWLINE
PRINTFORML 특수한 함락
CALL COLOR_LINE

CALL SHOW_INFO_ERO_DESCRIPTION_SP_FALLEN(대상)

CALL SINGLE_DRAWLINE
PRINTFORML SLG 설명
CALL COLOR_LINE
TRYCCALLFORM SHOW_CHARA_EXPLANATION_{NO:(대상)}
CALL SINGLE_EMPTY_LINE()
CATCH
	PRINTFORML SLG 설명 없음　
	CALL SINGLE_EMPTY_LINE()
ENDCATCH

;-------------------------------------------------
;에로스테이타스 표시  육체
;-------------------------------------------------
@SHOW_INFO_ERO_DESCRIPTION_BODY(대상)
#DIM 대상
IF TALENT:대상:키스미경험
	PRINTFORML 입맞춤을 주고 받았던 적이 없다
ELSE
	CALL COLOR_PRINT(SEXUAL_EXPERIENCE:대상:첫체험_키스 == "불명" ? "누군가" # SEXUAL_EXPERIENCE:대상:첫체험_키스, 칼라_핑크, "에", GETDEFCOLOR())
	SIF SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_키스 != ""
		CALL COLOR_PRINT(SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_키스, 칼라_핑크, "로", GETDEFCOLOR())
	PRINTFORML 입술의 처음을 바쳤다
	IF SEXUAL_LAST_EXPERIENCE:대상:첫체험_키스 != ""
		CALL COLOR_PRINT("바로 옆에서는", GETDEFCOLOR(), SEXUAL_LAST_EXPERIENCE:대상:첫체험_키스 == "불명" ? "누군가" # SEXUAL_LAST_EXPERIENCE:대상:첫체험_키스, 칼라_핑크, "에", GETDEFCOLOR())
		SIF SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_키스 != ""
			CALL COLOR_PRINT(SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_키스, 칼라_핑크, "로", GETDEFCOLOR())
		PRINTFORML 입맞춤하고 있다
	ENDIF
ENDIF

IF HAS_VAGINA(대상)
	IF TALENT:대상:처녀
		PRINTFORML 질 구멍은 순결을 유지하고 있다
	ELSE
		CALL COLOR_PRINT(SEXUAL_EXPERIENCE:대상:첫체험_처녀 == "불명" ? "누군가" # SEXUAL_EXPERIENCE:대상:첫체험_처녀, 칼라_핑크, "에", GETDEFCOLOR())
		SIF SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_처녀 != ""
			CALL COLOR_PRINT(SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_처녀, 칼라_핑크, "로", GETDEFCOLOR())
		PRINTFORML 순결을 바쳤다
		IF SEXUAL_LAST_EXPERIENCE:대상:첫체험_처녀 != ""
			CALL COLOR_PRINT("바로 옆에서는", GETDEFCOLOR(), SEXUAL_LAST_EXPERIENCE:대상:첫체험_처녀 == "불명" ? "누군가" # SEXUAL_LAST_EXPERIENCE:대상:첫체험_처녀, 칼라_핑크, "와", GETDEFCOLOR())
			SIF SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_처녀 != ""
				CALL COLOR_PRINT(SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_처녀, 칼라_핑크, "로", GETDEFCOLOR())
			PRINTFORML 사귀고 있다
		ENDIF
	ENDIF
ENDIF

IF TALENT:대상:애널처녀
	PRINTFORML 엉덩이 구멍은 순결을 유지하고 있다
ELSE
	CALL COLOR_PRINT(SEXUAL_EXPERIENCE:대상:첫체험_애널처녀 == "불명" ? "누군가" # SEXUAL_EXPERIENCE:대상:첫체험_애널처녀, 칼라_핑크, "에", GETDEFCOLOR())
	SIF SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_애널처녀 != ""
		CALL COLOR_PRINT(SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_애널처녀, 칼라_핑크, "로", GETDEFCOLOR())
	PRINTFORML 엉덩이 구멍에서의 첫체험을 바쳤다
	IF SEXUAL_LAST_EXPERIENCE:대상:첫체험_애널처녀 != ""
		CALL COLOR_PRINT("바로 옆에서는", GETDEFCOLOR(), SEXUAL_LAST_EXPERIENCE:대상:첫체험_애널처녀 == "불명" ? "누군가" # SEXUAL_LAST_EXPERIENCE:대상:첫체험_애널처녀, 칼라_핑크, "와", GETDEFCOLOR())
		SIF SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_애널처녀 != ""
			CALL COLOR_PRINT(SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_애널처녀, 칼라_핑크, "로", GETDEFCOLOR())
		PRINTFORML 사귀고 있다
	ENDIF
ENDIF

IF HAS_PENIS(대상)
	IF TALENT:대상:동정
		PRINTFORML 아직도 동정이다
	ELSE
		CALL COLOR_PRINT(SEXUAL_EXPERIENCE:대상:첫체험_동정 == "불명" ? "누군가" # SEXUAL_EXPERIENCE:대상:첫체험_동정, 칼라_핑크, "에", GETDEFCOLOR())
		SIF SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_동정 != ""
			CALL COLOR_PRINT(SEXUAL_EXPERIENCE_SITUATION:대상:첫체험_동정, 칼라_핑크, "로", GETDEFCOLOR())
		PRINTFORML 동정을 바쳤다
		IF SEXUAL_LAST_EXPERIENCE:대상:첫체험_동정 != ""
			CALL COLOR_PRINT("바로 옆에서는", GETDEFCOLOR(), SEXUAL_LAST_EXPERIENCE:대상:첫체험_동정 == "불명" ? "누군가" # SEXUAL_LAST_EXPERIENCE:대상:첫체험_동정, 칼라_핑크, "와", GETDEFCOLOR())
			SIF SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_동정 != ""
				CALL COLOR_PRINT(SEXUAL_LAST_EXPERIENCE_SITUATION:대상:첫체험_동정, 칼라_핑크, "로", GETDEFCOLOR())
			PRINTFORML 사귀고 있다
		ENDIF
	ENDIF
ENDIF

IF IS_FEMALE(대상)
	SELECTCASE GET_BUSTSIZE(대상)
		CASE -2
			PRINTFORML 가슴에는 부푼 곳이 없고, 완전히 평평함
		CASE -1
			PRINTFORML 유방은 좀 작으면서도 갖추어진 형태를 하고 있다
		CASE 0
			PRINTFORML 유방은 딱 좋은 크기의 매력적인 형태를 하고 있다
		CASE 1
			PRINTFORML 풍부하고 부드러운 것 같은 유방이 눈을 끈다
		CASE 2
			PRINTFORML 터지지 않을 정도 휘어지게 여문 유방이 시선을 못을 박아 고정시킴으로 한다
	ENDSELECT
	SELECTCASE GET_HIPSIZE(대상)
		CASE -2
			PRINTFORML 엉덩이의 살집은 꽤 얇고, 완전히 평평함
		CASE -1
			PRINTFORML 엉덩이는 좀 작으면서도 갖추어진 형태를 하고 있다
		CASE 0
			PRINTFORML 엉덩이는 딱 좋은 크기의 매력적인 형태를 하고 있다
		CASE 1
			PRINTFORML 풍부해 탄력이 있는 엉덩이가 눈을 끈다
		CASE 2
			PRINTFORML 터지지 않을 정도 고기가 막힌 엉덩이가 시선을 못을 박아 고정시킴으로 한다
	ENDSELECT
	SIF TALENT:대상:모유체질
		CALL COLOR_PRINTL("유방으로부터는 모유가 분비되고 있다", 칼라_핑크)
	SIF TALENT:대상:각선미
		PRINTFORML 날씬한 아름다운 다리가 이성을 매료한다
	SIF TALENT:대상:예쁜엉덩이
		PRINTFORML 엉덩이살은 아름답고, 꽉 긴장되고 있다
ENDIF

IF TALENT:대상:임신
	CALL ICPRINT(@"<\@ GET_SPERM_NAME(CFLAG:대상:아이의부친) == "불명" ? 부친 불명 # %GET_SPERM_NAME(CFLAG:(대상):아이의부친)% \@>의 아이를 품고 있다", "L", 칼라_핑크)
	SIF CFLAG:대상:행동불능상태 == 행동불능_임월
		CALL COLOR_PRINTL("임월을 맞이한 배는 크게 부풀고 있다", 칼라_핑크)
ENDIF 

FOR LOCAL, 0, 2
		CONTINUE
	IF LOCAL == 0
		SIF !HAS_VAGINA(대상)
			CONTINUE
		LOCAL:1 = TALENT:대상:Ｖ조임
		LOCALS = 여자구멍
	ELSE
		LOCAL:1 = TALENT:대상:Ａ조임
		LOCALS = 엉덩이 구멍
	ENDIF

	SELECTCASE LOCAL:1
		CASE IS >= 조임_꽉닫힘
			PRINTFORML %조사처리(LOCALS,"는")% 새끼 손가락조차 거절할 만큼 좁다
		CASE IS >= 조임_꽉조임
			PRINTFORML %조사처리(LOCALS,"는")% 새끼 손가락이라도 꽈악 단단히 조이는 좁음이다
		CASE IS >= 조임_빡빡함
			PRINTFORML %조사처리(LOCALS,"는")% 손가락을 큐우큐우 단단히 조여 온다
		CASE IS >= 조임_명기
			PRINTFORML %조사처리(LOCALS,"는")% 문 것에 쾌감을 주는 명기다
		CASE IS >= 조임_보통
			PRINTFORML %LOCALS%의 조임은 보통이라는 곳이다
		CASE IS >= 조임_느슨한
			PRINTFORML %LOCALS%의 조임은 느슨한이다
		CASE IS >= 조임_느슨느슨
			PRINTFORML %조사처리(LOCALS,"는")% 대단히 느슨해져 버리고 있다
		CASE IS >= 조임_뻐끔뻐끔
			PRINTFORML 사용된 %조사처리(LOCALS,"는")% 뻐끔 열어 닫지 않게 되어 있다
		CASE IS >= 조임_헐렁헐렁
			PRINTFORML 너무 사용되어, %조사처리(LOCALS,"는")% 형태가 망가져 헐렁헐렁이 되어 있다
		CASEELSE
			PRINTFORML 너무 사용되어, %조사처리(LOCALS,"는")% 원래의 형태를 모를 정도로 무너져 버리고 있다
	ENDSELECT
NEXT

SELECTCASE TALENT:대상:음모현재치
	CASE 음모_백보지
		PRINTFORML 음모는 제모되어 치부는 반질반질 하고 있다
	CASE 음모_깔끔
		PRINTFORML 음모가 나지 않은 치부는 반질반질 하고 있다
	CASE 음모_손질됨
		PRINTFORML 음모는 제대로 손질되고 있다
	CASE 음모_솜털
		PRINTFORML 치부에는 희미하게, 음모가 솜털과 같이 나 있다
	CASE 음모_살짝
		PRINTFORML 얇은 음모가 치부로 풀숲을 형성하고 있다
	CASE 음모_표준
		PRINTFORML 음모는 평균적으로 나고 갖추어져 있다
	CASE 음모_많은
		PRINTFORML 음모는 보통 보다 약간 좀많다
	CASE 음모_주렁주렁
		PRINTFORML 음모는 주렁주렁무성함
	CASE 음모_무식하게
		PRINTFORML 꽤 진한 음모가 무식하게 무성함
	CASE 음모_강모
		PRINTFORML 굵은 음모가 밀림과 같이 무성해, 음부를 가리고 있다
	CASE 음모_항문에난털
		PRINTFORML 손질되어 있지 않은 음모는, 나고 마음껏으로 엉덩이까지 달하고 있다
	CASE 음모_하트형
		CALL COLOR_PRINTL("음모는 이성을 유혹하는 것 같은 하트형에 정돈되고 있다", 칼라_핑크)
	CASEELSE
		PRINTFORML 음모는 평균적으로 나고 갖추어져 있다
ENDSELECT

SELECTCASE TALENT:대상:겨드랑이털현재치
	CASE 겨드랑이털_제모완료
		PRINTFORML 겨드랑이털은 제모되어 겨드랑이는 반질반질 하고 있다
	CASE 겨드랑이털_깔끔
		PRINTFORML 겨드랑이털이 나지 않은 겨드랑이는 반질반질 하고 있다
	CASE 겨드랑이털_손질됨
		PRINTFORML 겨드랑이는 제대로 손질 되고 있다
	CASE 겨드랑이털_솜털
		PRINTFORML 겨드랑이에는 희미하게, 겨드랑이털이 솜털과 같이 나 있다
	CASE 겨드랑이털_살짝
		PRINTFORML 얇은 겨드랑이털이 겨드랑이로 풀숲을 형성하고 있다
	CASE 겨드랑이털_보통
		PRINTFORML 겨드랑이털은 평균적으로 나고 갖추어져 있다
	CASE 겨드랑이털_많은
		PRINTFORML 겨드랑이털은 보통 보다 약간 좀많다
	CASE 겨드랑이털_주렁주렁
		PRINTFORML 겨드랑이털은 주렁주렁무성함
	CASE 겨드랑이털_무식하게
		PRINTFORML 꽤 진한 겨드랑이털이 무식하게 무성함
	CASE 겨드랑이털_강모
		PRINTFORML 진한 겨드랑이털이 밀림과 같이 무성해, 겨드랑이를 가리고 있다
	CASE 겨드랑이털_밀림
		PRINTFORML 손질되어 있지 않은 겨드랑이털은, 나고 마음껏으로 겨드랑이로부터 넘쳐 나오고 있다
	CASEELSE
		PRINTFORML 겨드랑이털은 평균적으로 나고 갖추어져 있다
ENDSELECT


SIF IS_PIERCED(대상, 피어스_유두) || IS_PIERCED(대상, 피어스_클리토리스) || IS_PIERCED(대상, 피어스_페니스) || IS_PIERCED(대상, 피어스_음순)
	CALL COLOR_PRINTL("추잡한 부위에 피어스가 되고 있다", 칼라_핑크)

SIF IS_TATTOOED(대상, 문신_뺨) || IS_TATTOOED(대상, 문신_이마)
	CALL COLOR_PRINTL("얼굴이 눈에 띄는 근처에 문신이 들어가 있다", 칼라_핑크)

SIF IS_TATTOOED(대상, 문신_가슴) || IS_TATTOOED(대상, 문신_비부) || IS_TATTOOED(대상, 문신_엉덩이)
	CALL COLOR_PRINTL("추잡한 부위에 문신이 들어가 있다", 칼라_핑크)

CALL SINGLE_EMPTY_LINE()

