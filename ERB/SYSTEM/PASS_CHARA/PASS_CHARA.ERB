		EXP인계_TMP:LOCAL = EXP인계:대상:LOCAL
	NEXT


	FOR LOCAL, 0, VARSIZE("CFLAG인계") 
		CFLAG인계_TMP:LOCAL = CFLAG인계:대상:LOCAL
	NEXT


