@EVENT_PRISONER_TEXT0(ARG:0)

IF TALENT:(ARG:0):붕괴 || TALENT:(ARG:0):공허
	PRINTFORML %조사처리(ANAME(ARG:0),"는")% 감옥 안에서 공허하게 시선을 방황하게 하고 있다
ELSE
	IF RAND:4
		IF ABL:(ARG:0):지략 >= 70
			PRINTFORML %조사처리(ANAME(ARG:0),"는")% 감옥 안에서 얌전하게 하고 있다…
		ELSEIF ABL:(ARG:0):무투 >= 70
			PRINTFORML %조사처리(ANAME(ARG:0),"는")% 감옥 안에서 날뛰고 있는 것 같다…
		ELSEIF ABL:(ARG:0):정치 >= 70
			PRINTFORML %조사처리(ANAME(ARG:0),"는")% 감옥의 밖에 있는 간수와 이야기하고 있다…
		ELSE
			PRINTFORML %조사처리(ANAME(ARG:0),"는")% 감옥 안에서 불안한 듯이 하고 있다…
		ENDIF
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML %조사처리(ANAME(ARG:0),"는")% 감옥 안에서 가만히 하고 있다…
			CASE 1
				PRINTFORML %조사처리(ANAME(ARG:0),"는")% 감옥 안에서 불평을 말하고 있다…
			CASE 2
				PRINTFORML %조사처리(ANAME(ARG:0),"는")% 감옥 안에서 불안한 것 같게 하고 있다…
			CASEELSE
				PRINTFORML %조사처리(ANAME(ARG:0),"는")% 감옥 안에서 불안한 듯이 하고 있다…
		ENDSELECT
	ENDIF
ENDIF

