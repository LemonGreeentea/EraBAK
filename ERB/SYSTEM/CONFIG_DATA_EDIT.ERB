PRINTL ◆요구하는 인재는
PRINT    
CALL PRINT_SELECT_BUTTON(" 10{다    능}", 10, LOCAL:10 == 0)
CALL PRINT_SELECT_BUTTON(" 11{무    관}", 11, LOCAL:10 == 1)
CALL PRINT_SELECT_BUTTON(" 12{군    사}", 12, LOCAL:10 == 2)
CALL PRINT_SELECT_BUTTON(" 13{정 무 관}", 13, LOCAL:10 == 3)
CALL PRINT_SELECT_BUTTON(" 14{아 이 돌}", 14, LOCAL:10 == 4)
CALL PRINT_SELECT_BUTTON(" 15{요 리 인}", 15, LOCAL:10 == 5)
PRINTL 
PRINTL 

PRINTL ◆성별은
PRINT    
CALL PRINT_SELECT_BUTTON(" 20{묻지않는다}", 20, LOCAL:11 == 0)
CALL PRINT_SELECT_BUTTON(" 21{남    성}", 21, LOCAL:11 == 1)
CALL PRINT_SELECT_BUTTON(" 22{여    성}", 22, LOCAL:11 == 2)
CALL PRINT_SELECT_BUTTON(" 23{여 후 타}", 23, LOCAL:11 == 3)
CALL PRINT_SELECT_BUTTON(" 23{남 후 타}", 24, LOCAL:11 == 4)
CALL PRINT_SELECT_BUTTON(" 24{오토코노코}", 25, LOCAL:11 == 5)
CALL PRINT_SELECT_BUTTON(" 24{남낭 후타}", 26, LOCAL:11 == 6)

PRINTL 
PRINTL
PRINTL ◆이름은
PRINT    
CALL PRINT_SELECT_BUTTON(" 30{일 본 식}", 30, LOCAL:13 == 0)
CALL PRINT_SELECT_BUTTON(" 31{서 양 식}", 31, LOCAL:13 == 1)
;CALL PRINT_SELECT_BUTTON(" 32{중 화 식}", 32, LOCAL:13 == 2)
CALL PRINT_SELECT_BUTTON(" 32{한 국 식}", 32, LOCAL:13 == 2)
PRINTL
CALL SINGLE_DRAWLINE
PRINTBUTTON "  0[이 조건으로 모집]", 0
PRINTL 
PRINTBUTTON "  1[캔슬]", 1
PRINTL 

REDRAW 0

$INPUT_LOOP
INPUT

IF RESULT == 1
	REDRAW 1
	RETURN 0
ELSEIF RESULT >= 10 && RESULT <= 15 && LOCAL:10 != RESULT - 10
	LOCAL:10 = RESULT - 10
	CLEARLINE LINECOUNT - FIRST_LINE
	GOTO SHOW_LOOP
ELSEIF RESULT >= 20 && RESULT <= 26 && LOCAL:11 != RESULT - 20
	LOCAL:11 = RESULT - 20
	CLEARLINE LINECOUNT - FIRST_LINE
	GOTO SHOW_LOOP
ELSEIF RESULT >= 30 && RESULT <= 32 && LOCAL:13 != RESULT - 30
	LOCAL:13 = RESULT - 30
	CLEARLINE LINECOUNT - FIRST_LINE
	GOTO SHOW_LOOP
ELSEIF RESULT != 0
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF

REDRAW 1

LOCAL:0 = GET_COMMANDER_NUM(CFLAG:MASTER:소속)

;군주가[동물귀]or[꼬리]를 가진다면 케모율 높은
LOCAL:4 = GET_COUNTRY_BOSS(CFLAG:MASTER:소속)
IF LOCAL:4 >= 0 && (TALENT:(LOCAL:4):동물귀 || TALENT:(LOCAL:4):꼬리)
	LOCAL:5 = 75
ELSE
	LOCAL:5 = 1
ENDIF

$INPUT_LOOP_NEWCHARA

;지정의 조건으로 랜덤 캐릭터를 작성
CALL CREATE_RANDOM_CHARA(LOCAL:5, LOCAL:10, LOCAL:11 - 1, LOCAL:13)
LOCAL:2 = RESULT

;사관의 작성에 실패했을 경우
IF LOCAL:2 < 0
	PRINTW 유감스럽지만 눈주위 사관은 발견되지 않았습니다…

;성공
ELSE
	CALL SINGLE_DRAWLINE
	PRINTFORML %조사처리(ANAME(LOCAL:2),"가")% 관직에 오름을 요구해 왔습니다

	$INPUT_LOOP_SELECT

	;SHOW_INFO의 초기 화면을 기본 정보로 한다
	FLAG:능력표시모드 = 0

	;캐릭터의 정보를 표시
	CALL SINGLE_DRAWLINE
	CALL SHOW_INFO(LOCAL:2)
	CALL SINGLE_DRAWLINE

	PRINTBUTTON "0[이 캐릭터를 등용]", 0
	PRINT    
	PRINTBUTTON "1[다른 캐릭터를 찾는다]", 1
	PRINT    
	IF hold == -1
	ELSEIF showingHold
		PRINTBUTTON "2[진행된다]", 2
	ELSE
		PRINTBUTTON "2[돌아온다]", 2
	ENDIF
	
	PRINT    
	PRINTBUTTON "9[중지]", 9
	PRINTL 

	$INPUT_LOOP_TREAT
	INPUT

	IF RESULT == 0
		CALL CHANGE_COUNTRY(LOCAL:2, CFLAG:MASTER:소속)
		PRINTFORMW %조사처리(ANAME(LOCAL:2),"가")% 우리 나라의 사관이 되었습니다
		IF hold != -1
			CALL DELETE_CHARA(hold)
		ENDIF
		RESTART
	ELSEIF RESULT == 1
		IF hold != -1
			;필요없는 캐릭터를 삭제
			CALL DELETE_CHARA(hold)
		ENDIF
		hold = CHARANUM - 1
		showingHold = 0
		GOTO INPUT_LOOP_NEWCHARA
	ELSEIF RESULT == 2
		IF showingHold
			showingHold = 0
		ELSE
			showingHold = 1
		ENDIF
			;스왑
			LOCAL:21 = hold
			hold    = LOCAL:2
			LOCAL:2 = LOCAL:21
		GOTO INPUT_LOOP_SELECT
	ELSEIF RESULT == 9
		;PRINTW 이번은 등용을 보류하기로 했습니다
		;작성한 캐릭터를 삭제
		CALL DELETE_CHARA(CHARANUM - 1)
		IF hold != -1
			CALL DELETE_CHARA(CHARANUM - 1)
		ENDIF
	ELSE
		GOTO INPUT_LOOP_TREAT
	ENDIF
ENDIF

RETURN 0

@CONFIG_DATA_EDIT_RANDOM_DEAD
#DIM LOOP_C
#DIM 종별
#DIM 비율
#DIM 삭제하지않는다

PRINTFORML 어느 종류별로 속하는 모든 캐릭터를, 난수의 결과 나름으로 사망시킵니다
PRINTFORML （「MASTER」 「군주」 「포로」 「임신중」 「동물」은 대상외）
PRINTFORML 고정 캐릭터 이외의 경우, 캐릭터 데이터 자체를 삭제하는 일도 가능합니다
PRINTFORML

PRINTFORML 캐릭터의 종별은?
CALL ASK_MULTI("（캔슬）", "모두", "고정 캐릭터", "랜덤 캐릭터（아이 이외）", "아이", "특수 캐릭터（동물 이외）")
IF RESULT == 0
	PRINTFORML 캔슬했습니다
	RETURN
ENDIF
종별 = RESULT

SIF 종별 == 1
	PRINT 고정 캐릭터 이외의 경우,
IF 종별 != 2
	PRINTFORML 캐릭터 데이터 자체를 삭제합니까?
	CALL ASK_YN("삭제한다（두 번 다시 부활할 수 없습니다）", "「특수상태」등의 개서 마셔（부활 가능）")
	삭제하지않는다 = RESULT
ENDIF

PRINTFORML 사망 상태로 하는 확률（퍼센트）는? （101이상 또는 0 이하 입력으로 캔슬할 수 있습니다）
PRINTFORML [0] [50] [100]
INPUT
IF RESULT <= 0 || RESULT > 100
	PRINTFORML 캔슬했습니다
	RETURN
ENDIF
비율 = RESULT

FOR LOOP_C, CHARANUM - 1, -1, -1
	SIF (LOOP_C == MASTER) || (GET_COUNTRY_FROM_BOSS_ID(GET_ID(LOOP_C)) > 0) || CFLAG:LOOP_C:포로처 || TALENT:LOOP_C:임신
		CONTINUE
	SIF (종별 == 2) && !IS_FIXED_CHARA(LOOP_C)
		CONTINUE
	SIF (종별 == 3) && !IS_RANDOM_CHARA(LOOP_C)
		CONTINUE
	SIF (종별 == 4) && !IS_CHILD(LOOP_C)
		CONTINUE
	SIF (종별 == 5) && !IS_SP_CHARA(LOOP_C)
		CONTINUE
	SIF IS_ANIMAL(LOOP_C) ; IS_ANIMAL 가 진정한 경우는 IS_SP_CHARA 도 진일 것이지만 일단 단독으로 체크해 둔다
		CONTINUE
	SIF RAND:100 >= 비율
		CONTINUE
	CALL FORCE_FREE(LOOP_C)
	IF IS_FIXED_CHARA(LOOP_C) || 삭제하지않는다
		IF  (CFLAG:LOOP_C:특수상태 != 특수상태_사망)
			PRINTFORML %ANAME(LOOP_C)%를 사망 상태로 합니다.
			CALL CHANGE_COUNTRY(LOOP_C, 0, 1, 1)
		ENDIF
	ELSE
		PRINTFORML %ANAME(LOOP_C)%를 삭제합니다.
		CALL DELETE_CHARA(LOOP_C)
	ENDIF
NEXT

@CONFIG_DATA_EDIT_ADD_RANDOM_CHARA_MULTI
