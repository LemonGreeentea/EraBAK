;-------------------------------------------------
;데이터 조작
;-------------------------------------------------
@CONFIG_DATA_EDIT
CALL SINGLE_DRAWLINE
PRINTL 실시하는 조작을 선택해 주세요
SETCOLOR 0xD0A080
PRINTL   ◎조작에 따라서는 위독한 불안정이 발생하는 경우가 있습니다
PRINTL   ◎남용하면 게임의 즐거움을 해칠 우려가 있습니다
PRINTL   이상을 이해한 다음 이용하십시오
RESETCOLOR
CALL SINGLE_DRAWLINE

PRINTBUTTON " 0[캐릭터 데이터의 개별 개서]", 0
PRINTL 
PRINTBUTTON " 1[캐릭터 데이터의 일괄 개서]", 1
PRINTL 
PRINTBUTTON " 2[소지금의 추가]", 2
PRINTL 
PRINTBUTTON " 3[세력도의 변경]", 3
PRINTL 
PRINTBUTTON " 4[전 아이템 추가]", 4
PRINTL
PRINTBUTTON " 5[시기 설정]", 5
PRINTL
IF IS_COUNTRY(CFLAG:MASTER:소속)
	PRINTBUTTON " 6[각국과의 관계 설정]", 6
	PRINTL
ENDIF
PRINTBUTTON " 7[이어받아 데이터 작성]", 7
PRINTL
PRINTBUTTON " 8[부대 덤프]", 8
PRINTL
PRINTBUTTON " 9[난수 조정]", 9
PRINTL
PRINTBUTTON "10[랜덤 캐릭터 추가]", 10
PRINTL
PRINTBUTTON "11[병을 늘린다]", 11
PRINTL
PRINTBUTTON "12[복수 캐릭터를 랜덤에서 사망시킨다]", 12
PRINTL
PRINTBUTTON "13[랜덤 캐릭터 복수 추가]", 13
PRINTL
PRINTBUTTON "15[MASTER와 캐릭터를 바꿔 넣는다]", 15
PRINTL
PRINTBUTTON "16[기존의 캐릭터의 클론 생성]", 16
PRINTL
PRINTBUTTON "17[전캐릭터의 행동완료를 리셋트]", 17
PRINTL
CALL SINGLE_DRAWLINE
PRINTBUTTON " 99[돌아온다]", 99
PRINTL 

REDRAW 0

$INPUT_LOOP
INPUT

IF RESULT == 99
	REDRAW 1
	RETURN
ELSEIF RESULT == 0
	REDRAW 1
	WHILE 1
	CALL SELECT_CHARA_LIST_ONLY_LOGIC_SLG("NONE", "NONE")
	SIF RESULT == -1
		BREAK
	CALL CHARACTER_EDIT(RESULT, 1)
	WEND
ELSEIF RESULT == 1
	REDRAW 1
	CALL CONFIG_DATA_EDIT_CHARA_ALL
ELSEIF RESULT == 2
	REDRAW 1
	PRINTFORML 추가하는 소지금의 양을 입력해 주세요(현재:{MONEY})
	INPUT
	MONEY = LIMIT(MONEY + RESULT, 0, 9999999999999)
ELSEIF RESULT == 3
	CALL CONFIG_DATA_EDIT_SLG
ELSEIF RESULT == 4
	CALL ASK_YN("번호 지정", "전추가")
	IF RESULT == 0
		PRINTFORML 번호 지정
		INPUT
		LOCAL:1 = RESULT
		PRINTFORML 개수 지정
		INPUT
		LOCAL:2 = RESULT
		ITEM:(LOCAL:1) = LOCAL:2
	ELSE
		REDRAW 1
		PRINTFORML 구입 가능한 전 아이템을 판매 개시해, 한편 추가해 두었습니다
		FOR LOCAL, 0, 499
			IF ITEMNAME:LOCAL != ""
				SELECTCASE LOCAL
					CASE 140
						ITEMSALES:LOCAL = 1
						ITEMTERM:LOCAL = 0
						ITEM:LOCAL = 1
					CASE 141, 142
						ITEMSALES:LOCAL = 1
						ITEMTERM:LOCAL = 0
					CASE 100 TO 149
						ITEMSALES:LOCAL = 0
						ITEM:LOCAL = 1
						ITEMTERM:LOCAL = 0
					CASE 150 TO 199
						ITEMSALES:LOCAL = 1
						ITEM:LOCAL = 99
						ITEMTERM:LOCAL = 0
					CASE 200 TO 249
						ITEMSALES:LOCAL = 0
						ITEM:LOCAL = 1
						ITEMTERM:LOCAL = 0
					CASE 250 TO 279
						ITEMSALES:LOCAL = 1
						ITEM:LOCAL = 99
						ITEMTERM:LOCAL = 0
					CASE IS >= 280
						ITEMSALES:LOCAL = 1
						ITEMTERM:LOCAL = 0
				ENDSELECT
			ENDIF
		NEXT
	ENDIF
ELSEIF RESULT == 5
	REDRAW 1
		PRINTFORML 제몇기로 합니까? (현재:{DAY})
		PRINTFORML 0 이하 입력으로 캔슬할 수 있습니다
		INPUT
		IF RESULT <= 0
			PRINTFORML 캔슬했습니다
		ELSE
			PRINTFORML {RESULT}기로 했습니다
			DAY = RESULT
		ENDIF
ELSEIF RESULT == 6 && IS_COUNTRY(CFLAG:MASTER:소속)
	VARSET LOCAL
	VARSET LOCALS
	$DEPLOMACY_LOOP
	CALL SINGLE_DRAWLINE
	PRINTL 관계 설정을 실시하는 세력을 선택해 주세요
	CALL SINGLE_DRAWLINE
	PRINTL                     【관계】【우호도／적대도】  【인상】  【붙잡고 있는 포로】        【붙잡혀 있는 포로】

	;외 세력 일람의 표시
	FOR LOCAL:0, 1, MAX_COUNTRY
		;도둑은 원래 표시시키지 않다
		IF IS_COUNTRY(LOCAL:0) && LOCAL:0 != CFLAG:MASTER:소속
			LOCALS:0 = {LOCAL:0, 2}[%ANAME(GET_COUNTRY_BOSS(LOCAL:0))%]
			;외교 금지 플래그가 서 있다면 회색 표시
			IF COUNTRY_IS_CLOSED:(LOCAL:0)
				SETCOLOR 칼라_선택불가
				PRINTPLAINFORM %LOCALS:0, 22, LEFT%
				RESETCOLOR
			ELSE
				PRINTBUTTON @"%LOCALS:0, 22, LEFT%", LOCAL:0
			ENDIF

			CALL TMP_SETCOLOR_COUNTRY_RELATION(CFLAG:MASTER:소속, LOCAL:0)
			PRINTFORM %TMP_TOSTR_COUNTRY_RELATION(CFLAG:MASTER:소속, LOCAL:0, 0), 9, LEFT%
			RESETCOLOR
			LOCAL:4 = GET_COUNTRY_BOSS(CFLAG:MASTER:소속)
			LOCAL:5 = GET_COUNTRY_BOSS(LOCAL:0)
	;		PRINTFORM 
			PRINTFORM {REL_LIKE:(LOCAL:5):(LOCAL:4), 5, RIGHT}
			PRINT 　
			PRINTFORM {REL_HATE:(LOCAL:5):(LOCAL:4), 5, RIGHT}
			PRINT        

			CALL TMP_PRINT_COUNTRY_RELATION2(LOCAL:5, LOCAL:4)
			PRINT      

			;붙잡고 있는 포로의 표시
			LOCALS:1 = 
			FOR LOCAL:1, 0, CHARANUM
				IF CFLAG:(LOCAL:1):소속 == LOCAL:0 && CFLAG:(LOCAL:1):포로처 == CFLAG:MASTER:소속
					IF LOCALS:1 == ""
						LOCALS:1 = %ANAME(LOCAL:1)%
					ELSE
						IF STRLENS(@"%LOCALS:1%、%ANAME(LOCAL:1)%") >= 24
							LOCALS:1 = %LOCALS:1%, 외
							BREAK
						ELSE
							LOCALS:1 = %LOCALS:1%、%ANAME(LOCAL:1)%
						ENDIF
					ENDIF
				ENDIF
			NEXT
			SETCOLOR 0xFF8020
			PRINTFORM %LOCALS:1, 26, LEFT%
			RESETCOLOR

			;붙잡혀 있는 포로의 표시
			LOCALS:1 = 
			FOR LOCAL:1, 0, CHARANUM
				IF CFLAG:(LOCAL:1):소속 == CFLAG:MASTER:소속 && CFLAG:(LOCAL:1):포로처 == LOCAL:0
					IF LOCALS:1 == ""
						LOCALS:1 = %ANAME(LOCAL:1)%
					ELSE
						IF STRLENS(@"%LOCALS:1%、%ANAME(LOCAL:1)%") >= 24
							LOCALS:1 = %LOCALS:1%, 외
							BREAK
						ELSE
							LOCALS:1 = %LOCALS:1%、%ANAME(LOCAL:1)%
						ENDIF
					ENDIF
				ENDIF
			NEXT
			SETCOLOR 0x40FFFF
			PRINTFORM %LOCALS:1, 26, LEFT%
			RESETCOLOR
			PRINTL 
		ENDIF
	NEXT
	CALL SINGLE_DRAWLINE
	PRINTBUTTON "98[일괄]", 98
	PRINTL
	PRINTBUTTON "99[돌아온다]", 99
	PRINTL 
	REDRAW 0

	$INPUT_LOOP2
	INPUT

	IF RESULT >= 1 && RESULT < MAX_COUNTRY && RESULT != CFLAG:MASTER:소속 && IS_COUNTRY(RESULT) && !COUNTRY_IS_CLOSED:RESULT
		LOCAL:1 = RESULT
		REDRAW 1
		PRINTFORML 우호도를 얼마로 합니까? (부의 수로 현상 유지, 최대치 1500)
		INPUT
		IF RESULT < 0
		ELSE
			REL_LIKE:(GET_COUNTRY_BOSS(LOCAL:1)):(GET_COUNTRY_BOSS(CFLAG:MASTER:소속)) = MIN(RESULT, 1500)
		ENDIF
		PRINTFORML 적대도를 얼마로 합니까? (부의 수로 현상 유지, 최대치 1500)
		INPUT
		IF RESULT < 0
		ELSE
			REL_HATE:(GET_COUNTRY_BOSS(LOCAL:1)):(GET_COUNTRY_BOSS(CFLAG:MASTER:소속)) = MIN(RESULT, 1500)
		ENDIF
		PRINTFORMW 설정했습니다
		GOTO DEPLOMACY_LOOP
	ELSEIF RESULT == 98
		REDRAW 1
		PRINTFORML 우호도를 얼마로 합니까? (부의 수로 현상 유지, 최대치 1500)
		INPUT
		IF RESULT < 0
		ELSE
			FOR LOCAL, 0, MAX_COUNTRY
				IF IS_COUNTRY(LOCAL) && !COUNTRY_IS_CLOSED:LOCAL && LOCAL != CFLAG:MASTER:소속
					REL_LIKE:(GET_COUNTRY_BOSS(LOCAL)):(GET_COUNTRY_BOSS(CFLAG:MASTER:소속)) = MIN(RESULT, 1500)
				ENDIF
			NEXT
		ENDIF
		PRINTFORML 적대도를 얼마로 합니까? (부의 수로 현상 유지, 최대치 1500)
		INPUT
		IF RESULT < 0
		ELSE
			FOR LOCAL, 0, MAX_COUNTRY
				IF IS_COUNTRY(LOCAL) && !COUNTRY_IS_CLOSED:LOCAL && LOCAL != CFLAG:MASTER:소속
					REL_HATE:(GET_COUNTRY_BOSS(LOCAL)):(GET_COUNTRY_BOSS(CFLAG:MASTER:소속)) = MIN(RESULT, 1500)
				ENDIF
			NEXT
		ENDIF
		PRINTFORMW 설정했습니다
		GOTO DEPLOMACY_LOOP
	ELSEIF RESULT != 99
		CLEARLINE 1
		GOTO INPUT_LOOP2
	ENDIF
	REDRAW 1
	RESTART
ELSEIF RESULT == 7
	PRINTFORMW 이어받아 데이터를 작성합니다
	FLAG:클리어직후플래그 = 1
	CALL SAVE_GAME
	FLAG:클리어직후플래그 = 0
ELSEIF RESULT == 8
	FOR LOCAL, 0, MAX_COUNTRY
		SIF !IS_COUNTRY(LOCAL)
			CONTINUE
		LOCAL:6 = GET_COUNTRY_BOSS(LOCAL:0)
		FOR LOCAL:1, 0, MAX_UNIT
			SIF UNIT_SOLDIER:(LOCAL:0):(LOCAL:1) <= 0
				CONTINUE
			CALL PRINT_UNIT_INFO(LOCAL:0, LOCAL:1)
			PRINTFORM 거리:%GET_CITYNAME(UNIT_POSITION:(LOCAL:0):(LOCAL:1))%
			PRINTL
		NEXT
	NEXT
	RESETCOLOR
	WAIT
ELSEIF RESULT == 9
	PRINTFORML random number generation 회수를 입력(0 이하를 입력으로 캔슬)
	INPUT
	IF RESULT > 0
		LOCAL:1 = RESULT
		FOR LOCAL, 0, LOCAL:1
			LOCAL:2 = RAND:2
		NEXT
	ENDIF
ELSEIF RESULT == 10
	CALL CONFIG_DATA_EDIT_RECRUIT_RANDOM_CHARA
ELSEIF RESULT == 11
	FOR LOCAL:0, 1, MAX_COUNTRY
		SIF IS_COUNTRY(LOCAL:0)
			PRINTFORML [{LOCAL:0}] - %ANAME(GET_COUNTRY_BOSS(LOCAL:0))%
	NEXT
	PRINTFORML [99] 돌아온다
	$INPUT_LOOP_SOLDIER
	INPUT
	IF RESULT == 99
	ELSEIF IS_COUNTRY(RESULT)
		LOCAL = RESULT
		PRINTFORML 늘리는 수를 입력
		INPUT
		COUNTRY_SOLDIER:(LOCAL:0) += RESULT
	ELSE
		GOTO INPUT_LOOP_SOLDIER
	ENDIF
ELSEIF RESULT == 12
	CALL CONFIG_DATA_EDIT_RANDOM_DEAD
ELSEIF RESULT == 13
	CALL CONFIG_DATA_EDIT_ADD_RANDOM_CHARA_MULTI
ELSEIF RESULT == 15
	CALL CONFIG_DATA_EDIT_SWAP_MASTER
ELSEIF RESULT == 16
	CALL CONFIG_DATA_EDIT_CREATE_CLONE
ELSEIF RESULT == 17
	FOR LOCAL, 0, CHARANUM
		CFLAG:LOCAL:행동완료 = 0
	NEXT
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF
RESTART

