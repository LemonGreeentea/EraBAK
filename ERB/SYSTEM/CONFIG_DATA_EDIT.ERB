	CALL CREATE_RANDOM_CHARA(,,성별)
	IF RESULT < 0
		PRINTFORML {LOOP1}인생 이루었습니다
		RETURN
	ELSE
		IF 특수
			; @VIGILANTE_INIT 를 참고로 한다.
			FLAG:범용무장카운트 --
			NO:RESULT = GET_EMPTY_NO()
			;CALL INIT_NEWCHARA(RESULT)
		ENDIF
		CALL CHANGE_COUNTRY(RESULT, 소속)
	ENDIF
