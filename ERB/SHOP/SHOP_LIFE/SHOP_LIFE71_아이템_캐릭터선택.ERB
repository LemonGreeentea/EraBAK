;RETURN 0

;●후타나리약
@IS_ITEM_USE280(ARG:0)
SIF IS_ANIMAL(ARG:0)
	RETURN 0
IF ((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속
	RETURN 1
ENDIF
RETURN 0

;●성전환약
@IS_ITEM_USE281(ARG:0)
IF !IS_ANIMAL(ARG:0) && (((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속)
	RETURN 1
ENDIF
RETURN 0

;●이와가미노 비약
@IS_ITEM_USE282(ARG:0)
IF ((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속
	IF GROUPMATCH(TALENT:(ARG:0):성별, 0, 4) && !TALENT:(ARG:0):잉태가능
		RETURN 1
	ENDIF
ENDIF
RETURN 0

;●짐승 변화의 비약
@IS_ITEM_USE283(ARG:0)
SIF ((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속
	RETURN 1
RETURN 0

;●재생의 비약
@IS_ITEM_USE284(ARG:0)
IF (((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속) && ARG:0 != NAME_TO_CHARA("당신")
	RETURN 1
ENDIF
RETURN 0

;●태평신비서
@IS_ITEM_USE141(ARG:0)
IF ((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속
	IF !TALENT:(ARG:0):신비지식
		RETURN 1
	ENDIF
ENDIF
RETURN 0

;●음기술대전
@IS_ITEM_USE142(ARG:0)
IF ((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속
	IF ABL:(ARG:0):성지식 == 4 && ABL:(ARG:0):성기술 >= 6
		RETURN 1
	ENDIF
ENDIF
RETURN 0

;●약속의목걸이
@IS_ITEM_USE143(ARG:0)
IF CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 && (IS_CHILD(ARG:0) || IS_RANDOM_CHARA(ARG:0)) && CFLAG:(ARG:0):포로처 == 0 && (TALENT:(ARG:0):친애 || TALENT:(ARG:0):예속)
	IF CFLAG:(ARG:0):파견불가 == 0
		RETURN 1
	ENDIF
ENDIF
RETURN 0

;●영구 제모제
@IS_ITEM_USE287(ARG:0)
SIF TALENT:(ARG:0):음모현재치 != 음모_백보지
	RETURN ((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속
RETURN 0

;●문신 제거 수술
@IS_ITEM_USE288(ARG:0)
FOR LOCAL, 0, VARSIZE("TATTOO")
	SIF TATTOO:(ARG:0):LOCAL != ""
		RETURN ((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속
NEXT
RETURN 0

;●구멍 트레이닝
@IS_ITEM_USE289(ARG:0)
RETURN ((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속

;●미용
@IS_ITEM_USE290(ARG:0)
RETURN (((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속)

;●위험일않고들 해
@IS_ITEM_USE291(ARG:0)
RETURN (((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속) && HAS_VAGINA(ARG:0)


;피임약
@IS_ITEM_USE292(ARG:0)
RETURN !CFLAG:(ARG:0):임신기간 && IS_FEMALE(ARG:0) && CFLAG:(ARG:0):피임약남은턴 == 0 && (((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속)

;낙태약
@IS_ITEM_USE293(ARG:0)
RETURN TALENT:(ARG:0):임신  && IS_FEMALE(ARG:0) && (((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속)

;●능동 수동
@IS_ITEM_USE294(ARG:0)
RETURN ((CFLAG:(ARG:0):소속 == CFLAG:MASTER:소속 || GROUPMATCH(TMP_COUNTRY_RELATION:(CFLAG:(ARG:0):소속):(CFLAG:(MASTER):소속), 3, 4)) && CFLAG:(ARG:0):포로처 == 0) || CFLAG:(ARG:0):포로처 == CFLAG:MASTER:소속

;-------------------------------------------------
; 아이템의 대상 캐릭터를 선택해 그 자리에서 사용
;ARG:0= 아이템 번호
;-------------------------------------------------
@SELECT_ITEM_USER(ARG:0)
#DIM FIRST_LINE

CLEARLINE 3

;전원의 조교참가플래그를 클리어
CVARSET CFLAG, 6, 0

; 아이템을 사용 가능한 캐릭터의 수를 센다
LOCAL:5 = 0
FOR LOCAL:0, 0, CHARANUM
	RESULT = 0
	TRYCALLFORM IS_ITEM_USE{ARG:0}(LOCAL:0)
	IF RESULT
		LOCAL:5 ++
	ENDIF
NEXT

;페이지수의 계산
LOCAL:10 = (LOCAL:5 - 1) / 44 + 1
LOCAL:11 = 1
SETCOLOR 칼라_회색
CALL SINGLE_DRAWLINE
RESETCOLOR
PRINTFORM %TOSTR_SPACE(6)%
PRINTFORML %조사처리(ITEMNAME:(ARG:0),"를")% 누구에게 사용합니까?
SETCOLOR 칼라_회색
CALL SINGLE_DRAWLINE
RESETCOLOR
FIRST_LINE = LINECOUNT

$SHOW_LOOP

;캐릭터 리스트의 표시
LOCAL:12 = (LOCAL:11 - 1) * 44
LOCAL:13 = LOCAL:11* 44
LOCAL:14 = 0
LOCAL:15 = 0
LOCAL:16 = 4
PRINTFORM %TOSTR_SPACE(6)%
FOR LOCAL:0, 0, CHARANUM
	RESULT = 0
	TRYCALLFORM IS_ITEM_USE{ARG:0}(LOCAL:0)
	IF RESULT
		IF LOCAL:14 >= LOCAL:12 && LOCAL:14 < LOCAL:13
			IF LOCAL:15 % 2 != 0
				PRINTPLAINFORM %TOSTR_SPACE(2)%
			ELSEIF LOCAL:15 >= 1
				PRINTL 
				PRINTFORM %TOSTR_SPACE(6)%
				LOCAL:16 ++
			ENDIF
			;CALL PRINT_PARTNER_DATA(LOCAL:0)
			CALL COLUMN_RIGHT_CHARALIST_BUTTON(LOCAL:0, 0, , , 1)
			LOCAL:15 ++
		ENDIF
		LOCAL:14 ++
	ENDIF
NEXT
PRINTL 
SETCOLOR 칼라_회색
CALL SINGLE_DRAWLINE
RESETCOLOR

IF LOCAL:10 >= 2
	LOCAL:16 += 2
	IF LOCAL:11 > 1
		PRINTBUTTON @"[%"이전페이지", MAX_CHARANAME_LENGTH, LEFT%]", SHOP_LIFE_LIST1_PAGE_BACK
	ELSE
		SETCOLOR 칼라_선택불가
		PRINTPLAINFORM [%"이전페이지", MAX_CHARANAME_LENGTH, LEFT%]
		RESETCOLOR
	ENDIF
	LOCALS:0 = page{LOCAL:11}/{LOCAL:10}
	PRINTPLAINFORM %TOSTR_SPACE(10)%
	PRINTPLAINFORM %LOCALS:0, 16, LEFT%
	IF LOCAL:11 < LOCAL:10
		PRINTBUTTON @"[%"다음페이지", MAX_CHARANAME_LENGTH, LEFT%]", SHOP_LIFE_LIST1_PAGE_NEXT
	ELSE
		SETCOLOR 칼라_선택불가
		PRINTPLAINFORM [%"다음페이지", MAX_CHARANAME_LENGTH, LEFT%]
		RESETCOLOR
	ENDIF
	PRINTL 
	SETCOLOR 칼라_회색
	CALL SINGLE_DRAWLINE
	RESETCOLOR
ENDIF

PRINTPLAIN  
PRINTBUTTON @"[%"돌아온다", MAX_CHARANAME_LENGTH, LEFT%]", 0
PRINTFORML 

REDRAW 0

$INPUT_LOOP
INPUT 0

CLEARLINE 2

IF RESULT == 0
	REDRAW 1
	RETURN 0
ELSEIF RESULT == SHOP_LIFE_LIST1_PAGE_BACK
	LOCAL:11 --
	IF LOCAL:11 < 0
		LOCAL:11 = LOCAL:10
	ENDIF
	CLEARLINE LINECOUNT - FIRST_LINE
	GOTO SHOW_LOOP
ELSEIF RESULT == SHOP_LIFE_LIST1_PAGE_NEXT
	LOCAL:11 ++
	IF LOCAL:11 > LOCAL:10
		LOCAL:11 = 0
	ENDIF
	CLEARLINE LINECOUNT - FIRST_LINE
	GOTO SHOW_LOOP
ELSE
	LOCAL:20 = RESULT - 100
	LOCAL:21 = NO_TO_CHARA(LOCAL:20)
	IF LOCAL:21 >= 0
		RESULT = 0
		TRYCALLFORM IS_ITEM_USE{ARG:0}(LOCAL:21)
		IF RESULT
			CALLFORM ITEM_EFFECT{ARG:0}(LOCAL:21)
			REDRAW 1
			RETURN 1
		ENDIF
	ENDIF
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF

